{
  this.project=(ProjectInternal)project;
  ClassGenerator classGenerator=this.project.getServices().get(ClassGenerator.class);
  this.sourceSets=classGenerator.newInstance(FactoryNamedDomainObjectContainer.class,NativeSourceSet.class,classGenerator,new ReflectiveNamedDomainObjectFactory<NativeSourceSet>(DefaultNativeSourceSet.class,classGenerator,classGenerator,this.project));
  sourceSets.all(new Action<NativeSourceSet>(){
    public void execute(    final NativeSourceSet ss){
      ss.addRule(new Rule(){
        public String getDescription(){
          return "create all";
        }
        public void apply(        String domainObjectName){
          ss.create(domainObjectName);
        }
      }
);
    }
  }
);
}
