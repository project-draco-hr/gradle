{
  if (ast == null) {
    type.setName("java.lang.Object");
    return;
  }
switch (ast.getType()) {
case LITERAL_boolean:
    type.setName("boolean");
  return;
case LITERAL_byte:
type.setName("byte");
return;
case LITERAL_char:
type.setName("char");
return;
case LITERAL_double:
type.setName("double");
return;
case LITERAL_float:
type.setName("float");
return;
case LITERAL_int:
type.setName("int");
return;
case LITERAL_long:
type.setName("long");
return;
case LITERAL_void:
type.setName("void");
return;
case ARRAY_DECLARATOR:
extractTypeName((GroovySourceAST)ast.getFirstChild(),type);
type.addArrayDimension();
return;
}
type.setName(extractName(ast));
}
