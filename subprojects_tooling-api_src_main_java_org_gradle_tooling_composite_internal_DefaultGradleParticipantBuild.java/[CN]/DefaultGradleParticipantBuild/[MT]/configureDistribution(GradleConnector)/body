{
  if (gradleDistribution == null) {
    if (gradleHome == null) {
      if (gradleVersion == null) {
        connector.useBuildDistribution();
      }
 else {
        connector.useGradleVersion(gradleVersion);
      }
    }
 else {
      connector.useInstallation(gradleHome);
    }
  }
 else {
    connector.useDistribution(gradleDistribution);
  }
}
