{
  if (file.isDirectory()) {
    new FileSet(file,null).visit(visitor);
  }
 else   if (file.isFile()) {
    visitor.visitFile(new FileVisitDetails(){
      public File getFile(){
        return file;
      }
      public RelativePath getRelativePath(){
        return new RelativePath(true,file.getName());
      }
      public void stopVisiting(){
      }
    }
);
  }
  return this;
}
