{
  StringWriter stringWriter=new StringWriter();
  final PrintWriter testWriter=new PrintWriter(stringWriter);
  testMavenExcludes.clear();
  mavenDependency=new DefaultMavenDependency(null,null,null,null,null,testMavenExcludes,false,null);
  mavenDependency.write(testWriter);
  assertEquals(String.format("    <dependency>%n    </dependency>%n"),stringWriter.toString());
}
