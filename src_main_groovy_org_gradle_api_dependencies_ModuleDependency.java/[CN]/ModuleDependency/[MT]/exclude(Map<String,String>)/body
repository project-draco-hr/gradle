{
  String org=GUtil.elvis(args.get("org"),PatternMatcher.ANY_EXPRESSION);
  String module=GUtil.elvis(args.get("module"),PatternMatcher.ANY_EXPRESSION);
  excludeRules.add(new DefaultExcludeRule(new ArtifactId(new ModuleId(org,module),PatternMatcher.ANY_EXPRESSION,PatternMatcher.ANY_EXPRESSION,PatternMatcher.ANY_EXPRESSION),ExactPatternMatcher.INSTANCE,null));
  return this;
}
