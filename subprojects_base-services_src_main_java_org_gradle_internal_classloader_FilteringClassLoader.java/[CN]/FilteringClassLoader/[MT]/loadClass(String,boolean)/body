{
  try {
    return EXT_CLASS_LOADER.loadClass(name);
  }
 catch (  ClassNotFoundException ignore) {
  }
  if (!classAllowed(name)) {
    throw new ClassNotFoundException(String.format("%s not found.",name));
  }
  Class<?> cl=super.loadClass(name,false);
  if (resolve) {
    resolveClass(cl);
  }
  return cl;
}
