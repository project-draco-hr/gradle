{
  boolean isTaskMethod=call.getMethod() instanceof ConstantExpression && call.getMethod().getText().equals(name);
  if (!isTaskMethod) {
    return false;
  }
  if (!(call.getArguments() instanceof ArgumentListExpression)) {
    return false;
  }
  return call.getObjectExpression() instanceof VariableExpression && call.getObjectExpression().getText().equals("this");
}
