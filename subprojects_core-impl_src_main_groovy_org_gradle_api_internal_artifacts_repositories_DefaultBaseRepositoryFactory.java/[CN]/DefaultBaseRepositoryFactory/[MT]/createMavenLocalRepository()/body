{
  MavenArtifactRepository mavenRepository=instantiator.newInstance(DefaultMavenLocalArtifactRepository.class,fileResolver,createPasswordCredentials(),transportFactory,locallyAvailableResourceFinder,metadataProcessor);
  final File localMavenRepository=localMavenRepositoryLocator.getLocalMavenRepository();
  mavenRepository.setUrl(localMavenRepository);
  return mavenRepository;
}
