{
  lifecycle.use(new Runnable(){
    public void run(){
      executor.execute(new Runnable(){
        public void run(){
          T result;
          try {
            result=actionExecutor.run(new RethrowingErrorsConsumerAction<T>(action));
          }
 catch (          Throwable t) {
            handler.onFailure(t);
            return;
          }
          handler.onComplete(result);
        }
      }
);
    }
  }
);
}
