{
  ModelProjection<C> identityProjection=new IdentityModelProjection<C>(modelReference.getType(),true,true);
  ModelProjection<C> containerProjection=new PolymorphicDomainObjectContainerModelProjection<I,C>(container,itemClass);
  List<ModelProjection<C>> projections=ImmutableList.of(identityProjection,containerProjection);
  return new DefaultModelCreator<C>(modelReference.getPath(),descriptor,Collections.<ModelReference<?>>emptyList(),projections,Transformers.<C,Inputs>constant(container));
}
