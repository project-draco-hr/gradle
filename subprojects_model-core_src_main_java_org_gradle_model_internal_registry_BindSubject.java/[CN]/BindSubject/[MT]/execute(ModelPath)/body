{
  ModelNodeInternal subject=modelGraph.get(modelPath);
  if (!subject.canApply(type)) {
    throw new IllegalStateException(String.format("Cannot add %s rule '%s' for model element '%s' when element is in state %s.",type,mutator.getDescriptor(),modelPath,subject.getState()));
  }
  mutationBinders.put(modelPath,binder);
  binder.bindSubject(modelPath);
}
