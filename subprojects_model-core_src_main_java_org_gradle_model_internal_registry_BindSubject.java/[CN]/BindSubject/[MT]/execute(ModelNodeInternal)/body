{
  if (!subject.canApply(type)) {
    throw new IllegalStateException(String.format("Cannot add %s rule '%s' for model element '%s' when element is in state %s.",type,mutator.getDescriptor(),subject.getPath(),subject.getState()));
  }
  mutationBinders.put(subject.getPath(),binder);
  binder.bindSubject(subject);
}
