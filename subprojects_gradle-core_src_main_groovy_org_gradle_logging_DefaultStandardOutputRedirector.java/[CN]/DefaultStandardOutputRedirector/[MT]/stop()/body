{
  try {
    if (originalStdOut != null) {
      System.setOut(originalStdOut);
    }
    if (originalStdErr != null) {
      System.setErr(originalStdErr);
    }
    redirectedStdOut.flush();
    redirectedStdErr.flush();
  }
  finally {
    originalStdOut=null;
    originalStdErr=null;
    stdOut.setDestination(null);
    stdErr.setDestination(null);
  }
  return this;
}
