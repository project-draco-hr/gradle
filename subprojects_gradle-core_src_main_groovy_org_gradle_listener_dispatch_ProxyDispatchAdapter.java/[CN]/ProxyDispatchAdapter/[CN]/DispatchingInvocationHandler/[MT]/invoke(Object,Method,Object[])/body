{
  if (method.getName().equals("equals")) {
    return parameters[0] != null && Proxy.isProxyClass(parameters[0].getClass()) && Proxy.getInvocationHandler(parameters[0]) == this;
  }
  if (method.getName().equals("hashCode")) {
    return hashCode();
  }
  if (method.getName().equals("toString")) {
    return String.format("%s broadcast",type.getSimpleName());
  }
  dispatch.dispatch(new MethodInvocation(method,parameters));
  return null;
}
