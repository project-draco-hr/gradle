{
  if (testDescriptor instanceof JvmTestDescriptorVersion1) {
    return String.format("TestDescriptor[id(%s), name(%s), className(%s), parent(%s)]",testDescriptor.getId(),testDescriptor.getName(),((JvmTestDescriptorVersion1)testDescriptor).getClassName(),testDescriptor.getParentId());
  }
 else {
    return String.format("TestDescriptor[id(%s), name(%s), parent(%s)]",testDescriptor.getId(),testDescriptor.getName(),testDescriptor.getParentId());
  }
}
