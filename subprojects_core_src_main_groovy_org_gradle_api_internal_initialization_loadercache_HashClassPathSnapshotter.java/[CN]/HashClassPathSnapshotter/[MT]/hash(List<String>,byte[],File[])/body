{
  for (  File file : toHash) {
    if (file.isDirectory()) {
      combinedHash=hash(visitedFilePaths,combinedHash,file.listFiles());
    }
 else {
      visitedFilePaths.add(file.getAbsolutePath());
      combinedHash=Bytes.concat(combinedHash,hasher.hash(file));
    }
  }
  return combinedHash;
}
