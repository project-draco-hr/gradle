{
  final String name=task.getName();
  final ModelPath modelPath=TaskContainerInternal.MODEL_PATH.child(name);
  ModelRegistry modelRegistry=projectInternal.getModelRegistry();
  modelRegistry.create(InstanceBackedModelCreator.of(ModelReference.of(modelPath,ModelType.typeOf(task)),new SimpleModelRuleDescriptor("TaskModelPopulatingConfigurationAction." + name),task));
}
