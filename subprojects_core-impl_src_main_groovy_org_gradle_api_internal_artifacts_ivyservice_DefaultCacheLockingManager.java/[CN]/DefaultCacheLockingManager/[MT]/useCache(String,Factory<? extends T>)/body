{
  boolean wasStarted=lockCache(operationDisplayName);
  try {
    if (!wasStarted) {
      cacheManager.onStartWork(operationDisplayName);
    }
    try {
      return action.create();
    }
  finally {
      if (!wasStarted) {
        cacheManager.onEndWork();
      }
    }
  }
  finally {
    unlockCache(wasStarted);
  }
}
