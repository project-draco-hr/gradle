{
  cache=cacheRepository.store(String.format("artifacts-%d",CACHE_LAYOUT_VERSION)).withDisplayName("artifact cache").withVersionStrategy(CacheBuilder.VersionStrategy.SharedCache).withLockMode(FileLockManager.LockMode.None).open();
  this.cacheAccess=new DefaultCacheAccess(String.format("artifact cache '%s'",getCacheDir()),getCacheDir(),fileLockManager);
}
