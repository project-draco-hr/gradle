{
  File dynamicRevisionsFile=new File(cacheMetadata.getCacheDir(),"dynamic-revisions.bin");
  FileLock dynamicRevisionsLock=cacheLockingManager.getCacheMetadataFileLock(dynamicRevisionsFile);
  return new BTreePersistentIndexedCache<RevisionKey,CachedRevisionEntry>(dynamicRevisionsFile,dynamicRevisionsLock,new DefaultSerializer<CachedRevisionEntry>(CachedRevisionEntry.class.getClassLoader()));
}
