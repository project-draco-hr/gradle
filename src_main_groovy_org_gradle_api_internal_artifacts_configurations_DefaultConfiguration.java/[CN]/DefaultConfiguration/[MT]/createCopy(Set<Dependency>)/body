{
  DetachedConfigurationsProvider configurationsProvider=new DetachedConfigurationsProvider();
  DefaultConfiguration copiedConfiguration=new DefaultConfiguration("copyOf" + getName(),configurationsProvider,ivyService,resolverProvider,dependencyMetaDataProvider);
  configurationsProvider.setTheOnlyConfiguration(copiedConfiguration);
  for (  Dependency dependency : dependencies) {
    copiedConfiguration.addDependency(dependency.copy());
  }
  return copiedConfiguration;
}
