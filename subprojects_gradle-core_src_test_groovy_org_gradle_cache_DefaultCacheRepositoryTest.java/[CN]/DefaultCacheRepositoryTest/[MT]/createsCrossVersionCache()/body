{
  context.checking(new Expectations(){
{
      one(cacheFactory).open(sharedCacheDir.file("noVersion","a/b/c"),CacheUsage.ON,Collections.singletonMap("gradle.version",version));
      will(returnValue(cache));
    }
  }
);
  assertThat(repository.cache("a/b/c").invalidateOnVersionChange().open(),sameInstance(cache));
}
