{
  try {
    try {
      return (Boolean)method.invoke(original);
    }
 catch (    InvocationTargetException e) {
      if (!(e.getCause() instanceof SocketException)) {
        throw e.getCause();
      }
      return null;
    }
  }
 catch (  Throwable throwable) {
    throw UncheckedException.throwAsUncheckedException(throwable);
  }
}
