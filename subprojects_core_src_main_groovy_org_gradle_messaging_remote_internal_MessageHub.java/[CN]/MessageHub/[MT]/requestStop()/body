{
  lock.lock();
  try {
    if (unicastOutgoing != null) {
      unicastOutgoing.requestStop();
    }
    if (broadcastOutgoing != null) {
      broadcastOutgoing.requestStop();
    }
    for (    ProtocolStack<ChannelMessage> handler : handlers) {
      handler.requestStop();
    }
  }
  finally {
    lock.unlock();
  }
}
