{
  addDependsOnTaskInOtherProjects(project.getTasks().getByName(JavaBasePlugin.BUILD_NEEDED_TASK_NAME),true,JavaBasePlugin.BUILD_NEEDED_TASK_NAME,TEST_RUNTIME_CONFIGURATION_NAME);
