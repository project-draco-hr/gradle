{
  List<File> taskClasspath=new ArrayList<File>(getGroovyClasspath().getFiles());
  throwExceptionIfTaskClasspathIsEmpty(taskClasspath);
  ProjectInternal project=(ProjectInternal)getProject();
  antGroovydoc.execute(getSource(),getDestinationDir(),isUse(),getWindowTitle(),getDocTitle(),getHeader(),getFooter(),getOverview(),isIncludePrivate(),getLinks(),project.getGradle().getIsolatedAntBuilder(),taskClasspath,project);
}
