{
  FileCopyAction visitor=new FileCopyAction(new BaseDirFileResolver(TestFiles.fileSystem(),destDir));
  visit(visitor,file(new RelativePath(true,"rootfile.txt"),new File(destDir,"rootfile.txt")),file(new RelativePath(true,"subdir","anotherfile.txt"),new File(destDir,"subdir/anotherfile.txt")));
}
