{
  ArtifactDependencyResolver resolver=new DefaultDependencyResolver(resolveIvyFactory,publishModuleDescriptorConverter,new ResolvedArtifactFactory(cacheLockingManager,ivyContextManager),new DefaultProjectModuleRegistry(publishModuleDescriptorConverter),cacheLockingManager,ivyContextManager,resolutionResultsStoreFactory);
  return new ErrorHandlingArtifactDependencyResolver(new ShortcircuitEmptyConfigsArtifactDependencyResolver(new SelfResolvingDependencyResolver(new CacheLockingArtifactDependencyResolver(cacheLockingManager,resolver))));
}
