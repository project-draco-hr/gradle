{
  ArtifactDependencyResolver resolver=new DefaultDependencyResolver(resolveIvyFactory,publishModuleDescriptorConverter,new ResolvedArtifactFactory(cacheLockingManager,ivyContextManager),new DefaultProjectModuleRegistry(publishModuleDescriptorConverter,projectRegistry),cacheLockingManager,ivyContextManager,resolutionResultsStoreFactory,versionMatcher,latestStrategy);
  return new ErrorHandlingArtifactDependencyResolver(new ShortcircuitEmptyConfigsArtifactDependencyResolver(new SelfResolvingDependencyResolver(new CacheLockingArtifactDependencyResolver(cacheLockingManager,resolver))));
}
