{
  DefaultResolutionStrategy resolutionStrategy=instantiator.newInstance(DefaultResolutionStrategy.class,globalDependencySubstitutionRules);
  return instantiator.newInstance(DefaultConfiguration.class,context.absoluteProjectPath(name),name,this,resolver,listenerManager,dependencyMetaDataProvider,resolutionStrategy,projectAccessListener,projectFinder,configurationComponentMetaDataBuilder,fileCollectionFactory);
}
