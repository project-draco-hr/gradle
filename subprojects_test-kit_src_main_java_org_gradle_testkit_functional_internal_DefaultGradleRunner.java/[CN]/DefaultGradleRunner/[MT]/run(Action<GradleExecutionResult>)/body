{
  GradleExecutionResult execResult=gradleExecutor.run(gradleHome,gradleUserHomeDir,workingDirectory,arguments,jvmArguments);
  resultVerification.execute(execResult);
  return new DefaultBuildResult(execResult.getStandardOutput(),execResult.getStandardError(),execResult.getExecutedTasks(),execResult.getSkippedTasks());
}
