{
  URL resource=classLoader.getResource(resourceName);
  if (resource == null) {
    throw new RuntimeException("Unable to find the released versions information.\n" + "The resource '" + resourceName + "' was not found.\n"+ "Most likely, you haven't ran the 'prepareVersionsInfo' task.\n"+ "If you have trouble running tests from your IDE, please run gradlew idea|eclipse first.");
  }
  try {
    Properties properties=new Properties();
    InputStream stream=resource.openStream();
    try {
      properties.load(stream);
    }
  finally {
      stream.close();
    }
    return properties;
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
