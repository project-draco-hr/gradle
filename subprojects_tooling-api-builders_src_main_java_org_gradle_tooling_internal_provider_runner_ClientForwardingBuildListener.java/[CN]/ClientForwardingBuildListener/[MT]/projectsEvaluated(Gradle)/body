{
  DefaultBuildStartedProgressEvent startEvent=new DefaultBuildStartedProgressEvent(eventTracker.eventTime(),new DefaultBuildDescriptor(EventIdGenerator.generateId(gradle,"projectsEvaluated"),"projects evaluation","Gradle " + gradle.getGradleVersion(),EventIdGenerator.generateId(gradle)));
  DefaultBuildFinishedProgressEvent endEvent=new DefaultBuildFinishedProgressEvent(eventTracker.eventTime(),new DefaultBuildDescriptor(EventIdGenerator.generateId(gradle,"projectsEvaluated"),"projects evaluation","Gradle " + gradle.getGradleVersion(),EventIdGenerator.generateId(gradle)),new DefaultBuildSuccessResult(eventTracker.buildStartTime(),eventTracker.eventTime()));
  eventConsumer.dispatch(startEvent);
  eventConsumer.dispatch(endEvent);
}
