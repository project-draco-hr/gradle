{
  try {
    StringWriter stringWriter=new StringWriter();
    PrintWriter writer=new PrintWriter(stringWriter);
    state.failure.printStackTrace(writer);
    writer.close();
    return stringWriter.toString();
  }
 catch (  Throwable t) {
    StringWriter stringWriter=new StringWriter();
    PrintWriter writer=new PrintWriter(stringWriter);
    t.printStackTrace(writer);
    writer.close();
    return stringWriter.toString();
  }
}
