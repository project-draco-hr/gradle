{
  Iterator it=globalClassSetIterator();
  while (it.hasNext()) {
    Object classInfo=it.next();
    if (classInfo != null) {
      Class clazz=(Class)clazzField.get(classInfo);
      if (inHierarchy(clazz,affectedLoaders)) {
        removeFromGlobalClassValue.invoke(globalClassValue,clazz);
      }
    }
  }
}
