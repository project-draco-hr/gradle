{
  return (prefix + invocationInfo.getBuildExperimentSpec().getDisplayName() + "_"+ invocationInfo.getBuildExperimentSpec().getProjectName()+ "_"+ invocationInfo.getPhase().name().toLowerCase()+ "_"+ invocationInfo.getIterationNumber()+ "_"+ invocationInfo.getIterationMax()+ postfix).replaceAll("[^a-zA-Z0-9.-]","_").replaceAll("[_]+","_");
}
