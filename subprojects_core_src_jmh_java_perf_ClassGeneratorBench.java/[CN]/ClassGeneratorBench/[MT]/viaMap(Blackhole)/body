{
  for (  Class aClass : classes) {
    for (int i=0; i < iter; i++) {
      lock.lock();
      try {
        Object o=map.get(aClass);
        if (o == null) {
          map.put(aClass,aClass);
          o=map.get(aClass);
        }
        blackhole.consume(o);
      }
  finally {
        lock.unlock();
      }
    }
  }
}
