{
  if (element == null) {
    Document document;
    try {
      document=DocumentBuilderFactory.newInstance().newDocumentBuilder().parse(new InputSource(new StringReader(toString())));
    }
 catch (    Exception e) {
      throw UncheckedException.asUncheckedException(e);
    }
    element=document.getDocumentElement();
    builder=null;
    node=null;
  }
  return element;
}
