{
  if (StringUtils.isEmpty(cacheId)) {
    throw new IllegalArgumentException("cacheId is empty!");
  }
  MessageDigest digester=cache.get(cacheId);
  if (digester == null) {
    digester=digesterFactory.createDigester();
    cache.put(cacheId,digester);
  }
 else {
    digester.reset();
  }
  return digester;
}
