{
  IncrementalFileSnapshot beforeSnapshot=beforeSnapshots.get(path);
  if (beforeSnapshot == null) {
    return true;
  }
  if (!fileSnapshot.isContentAndMetadataUpToDate(beforeSnapshot)) {
    return true;
  }
  if (afterPreviousSnapshots.containsKey(path)) {
    return true;
  }
  return false;
}
