{
  return new ExecuteAtMostOnceTaskExecuter(new SkipOnlyIfTaskExecuter(new SkipTaskWithNoActionsExecuter(new SkipUpToDateTaskExecuter(new PostExecutionAnalysisTaskExecuter(new ExecuteActionsTaskExecuter(get(ListenerManager.class).getBroadcaster(TaskActionListener.class))),get(TaskArtifactStateRepository.class)))));
