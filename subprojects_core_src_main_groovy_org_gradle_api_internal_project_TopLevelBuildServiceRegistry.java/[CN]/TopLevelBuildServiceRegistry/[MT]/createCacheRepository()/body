{
  CacheFactory factory=get(CacheFactory.class);
  CacheUsage cacheUsage=DeprecationLogger.whileDisabled(new Factory<CacheUsage>(){
    public CacheUsage create(){
      return startParameter.getCacheUsage();
    }
  }
);
  return new DefaultCacheRepository(startParameter.getGradleUserHomeDir(),startParameter.getProjectCacheDir(),cacheUsage,factory);
}
