{
  ConfigureActionsProjectEvaluator withActionsEvaluator=new ConfigureActionsProjectEvaluator(new BuildScriptProcessor(get(ScriptPluginFactory.class)),new PluginsProjectConfigureActions(get(ClassLoaderRegistry.class).getPluginsClassLoader()),new ResolveDeferredConfigurableAction());
  return new LifecycleProjectEvaluator(withActionsEvaluator);
}
