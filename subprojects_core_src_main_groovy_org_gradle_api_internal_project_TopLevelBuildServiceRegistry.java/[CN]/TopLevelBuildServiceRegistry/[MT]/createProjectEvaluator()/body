{
  ConfigureActionsProjectEvaluator withActionsEvaluator=new ConfigureActionsProjectEvaluator(new PluginsProjectConfigureActions(get(ClassLoaderRegistry.class).getPluginsClassLoader()),new BuildScriptProcessor(get(ScriptPluginFactory.class)),new ResolveDeferredConfigurableAction());
  return new LifecycleProjectEvaluator(withActionsEvaluator);
}
