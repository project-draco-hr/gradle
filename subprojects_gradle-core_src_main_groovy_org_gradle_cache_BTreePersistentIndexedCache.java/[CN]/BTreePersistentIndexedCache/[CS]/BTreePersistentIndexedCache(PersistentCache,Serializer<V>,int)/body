{
  this.backingCache=backingCache;
  this.serializer=serializer;
  this.maxIndexChildNodes=maxIndexChildNodes;
  this.minIndexChildNodes=maxIndexChildNodes / 2;
  cacheFile=new File(backingCache.getBaseDir(),"cache.bin");
  try {
