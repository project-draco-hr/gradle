{
  return new Daemon(new DaemonTcpServerConnector(get(ExecutorFactory.class),get(MessagingServices.class).get(InetAddressFactory.class)),get(DaemonRegistry.class),get(DaemonContext.class),"password",new StopHandlingCommandExecuter(new DefaultDaemonCommandExecuter(new InProcessBuildActionExecuter(get(GradleLauncherFactory.class)),get(ProcessEnvironment.class),loggingManager,getDaemonLogFile(),get(DaemonHealthServices.class).getHygieneAction())),get(ExecutorFactory.class));
