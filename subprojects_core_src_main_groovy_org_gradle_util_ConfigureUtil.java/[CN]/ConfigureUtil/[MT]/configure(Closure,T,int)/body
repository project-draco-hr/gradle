{
  if (configureClosure == null) {
    return delegate;
  }
  Closure copy=(Closure)configureClosure.clone();
  copy.setResolveStrategy(resolveStrategy);
  copy.setDelegate(delegate);
  if (copy.getMaximumNumberOfParameters() == 0) {
    copy.call();
  }
 else {
    copy.call(delegate);
  }
  return delegate;
}
