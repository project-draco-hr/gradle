{
  this.target=target;
  if (isEnabled(new ClassBackedTestDetails(target))) {
    try {
      assertCanExecute();
      runner=createExecutionRunner();
    }
 catch (    Throwable t) {
      runner=new CannotExecuteRunner(getDisplayName(),target,t);
    }
  }
 else {
    runner=new IgnoredExecutionRunner(getDisplayName(),target);
  }
}
