{
  String[] candidateLocations=new String[]{"C:/Program Files (x86)/Microsoft SDKs/Windows/v7.1","C:/Program Files/Microsoft SDKs/Windows/v7.1","C:/Program Files (x86)/Microsoft SDKs/Windows/v7.0A","C:/Program Files/Microsoft SDKs/Windows/v7.0A"};
  for (  String candidateLocation : candidateLocations) {
    File candidate=new File(candidateLocation);
    if (isWindowsSdk(candidate)) {
      return candidate;
    }
  }
  return null;
}
