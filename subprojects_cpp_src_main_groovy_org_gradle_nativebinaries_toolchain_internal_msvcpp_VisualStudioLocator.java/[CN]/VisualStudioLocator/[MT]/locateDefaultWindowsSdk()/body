{
  File resourceCompilerInPath=os.findInPath("rc.exe");
  if (resourceCompilerInPath != null) {
    return locateInHierarchy(resourceCompilerInPath,isWindowsSdk());
  }
  return locateInCandidates(isWindowsSdk(),"C:/Program Files (x86)/Microsoft SDKs/Windows/v7.1","C:/Program Files/Microsoft SDKs/Windows/v7.1","C:/Program Files (x86)/Microsoft SDKs/Windows/v7.0A","C:/Program Files/Microsoft SDKs/Windows/v7.0A");
