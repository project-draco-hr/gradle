{
  setUpMocksAndAttributes(testObj,TEST_GROOVY_CLASSPATH);
  context.checking(new Expectations(){
{
      one(antGroovycCompileMock).execute(testObj.getProject().getAnt(),testObj.getGroovySourceDirs(),testObj.getGroovyIncludes(),testObj.getGroovyExcludes(),testObj.getGroovyJavaIncludes(),testObj.getGroovyJavaExcludes(),testObj.getDestinationDir(),GUtil.addLists(AbstractCompileTest.TEST_DEPENDENCY_MANAGER_CLASSPATH),testObj.getSourceCompatibility(),testObj.getTargetCompatibility(),testObj.getGroovyOptions(),testObj.getOptions(),TEST_GROOVY_CLASSPATH);
    }
  }
);
  testObj.execute();
