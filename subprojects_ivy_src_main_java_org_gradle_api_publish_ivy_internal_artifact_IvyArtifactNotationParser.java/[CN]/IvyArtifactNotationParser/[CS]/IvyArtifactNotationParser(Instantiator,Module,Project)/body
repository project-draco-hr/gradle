{
  this.instantiator=instantiator;
  this.module=module;
  FileNotationParser fileNotationParser=new FileNotationParser(project);
  NotationParserBuilder<IvyArtifact> parserBuilder=new NotationParserBuilder<IvyArtifact>().resultingType(IvyArtifact.class).parser(new ArchiveTaskNotationParser()).parser(new PublishArtifactNotationParser()).parser(new FileMapNotationParser(fileNotationParser)).parser(fileNotationParser);
  delegate=parserBuilder.toComposite();
}
