{
  if (targets.isEmpty()) {
    to(defaultTargets);
  }
  for (  Object script : scripts) {
    File scriptFile=resolver.resolve(script);
    ScriptObjectConfigurer configurer=configurerFactory.create(new StrictScriptSource(new FileScriptSource("script",scriptFile)));
    for (    Object target : targets) {
      configurer.apply(target);
    }
  }
}
