{
  BuildLayout layout=layoutFactory.getLayoutFor(startParameter.getCurrentDir(),startParameter.isSearchUpwards());
  if (layout.getSettingsFile() == null) {
    return new SettingsLocation(layout.getSettingsDir(),new StringScriptSource("empty settings file",""));
  }
 else {
    return new SettingsLocation(layout.getSettingsDir(),new UriScriptSource("settings file",layout.getSettingsFile()));
  }
}
