{
  hasCustomActions=true;
  if (action == null) {
    throw new InvalidUserDataException("Action must not be null!");
  }
  taskMutator.mutate("Task.leftShift(Closure)",new Runnable(){
    public void run(){
      actions.add(taskMutator.leftShift(convertClosureToAction(action)));
    }
  }
);
  return this;
}
