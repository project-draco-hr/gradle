{
  Ivy ivy=ivy(new ArrayList<DependencyResolver>(),publishResolvers,gradleUserHome,new HashMap());
  Set<String> confs=Configurations.getNames(configurationContainer.get(configuration).getChain());
  dependencyPublisher.publish(confs,publishInstruction,publishResolvers,moduleDescriptorConverter.convert(new HashMap<String,Boolean>(),configurationContainer,publishInstruction.getModuleDescriptor().getConfigurationSpec(),dependencyContainer,publishInstruction.getModuleDescriptor().getDependencySpec(),artifactContainer,publishInstruction.getArtifactSpec()),ivy.getPublishEngine());
}
