{
  PomFilterContainer pomFilterContainer=new BasePomFilterContainer(new DefaultMavenPomFactory(configurationContainer,scopeMapping,new DefaultPomDependenciesConverter(new DefaultExcludeRuleConverter()),fileResolver));
  return new DefaultGroovyMavenDeployer(name,pomFilterContainer,new DefaultArtifactPomContainer(pomMetaInfoProvider,pomFilterContainer,new DefaultArtifactPomFactory()),loggingManagerFactory.create());
}
