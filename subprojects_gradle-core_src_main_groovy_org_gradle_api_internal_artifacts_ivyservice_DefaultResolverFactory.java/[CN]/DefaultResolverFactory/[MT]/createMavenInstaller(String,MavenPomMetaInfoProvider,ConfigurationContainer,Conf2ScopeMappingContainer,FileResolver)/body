{
  DefaultGroovyPomFilterContainer pomFilterContainer=new DefaultGroovyPomFilterContainer(new DefaultMavenPomFactory(configurationContainer,scopeMapping,new DefaultPomDependenciesConverter(new DefaultExcludeRuleConverter()),fileResolver));
  return new BaseMavenInstaller(name,pomFilterContainer,new DefaultArtifactPomContainer(pomMetaInfoProvider,pomFilterContainer,new DefaultArtifactPomFactory()),loggingManagerFactory.create());
}
