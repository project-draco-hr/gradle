{
  if (parentSpec == null) {
    return getRootDir();
  }
  File rootDir=parentSpec.getRootDir();
  if (rootDir == null) {
    return null;
  }
  String destPath=getDestPath();
  if (destPath.equals("/") || destPath.equals(File.separator)) {
    return rootDir;
  }
  return GFileUtils.canonicalise(new File(rootDir,destPath));
}
