{
  ClassLoader parent=getClass().getClassLoader();
  File toolsJar=ClasspathUtil.getToolsJar();
  if (toolsJar != null) {
    ClasspathUtil.addUrl((URLClassLoader)parent,Collections.singleton(toolsJar));
  }
  URL[] classPath=classPathRegistry.getClassPathUrls("GRADLE_PLUGINS");
  rootClassLoader=new URLClassLoader(classPath,parent);
}
