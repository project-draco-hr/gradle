{
  try {
    ProcessBuilder processBuilder=processBuilderFactory.createProcessBuilder(execHandle);
    Process process=processLauncher.start(processBuilder);
    streamsHandler.connectStreams(process,execHandle.getDisplayName());
    setProcess(process);
    execHandle.started();
    LOGGER.debug("waiting until streams are handled...");
    streamsHandler.start();
    if (execHandle.isDaemon()) {
      streamsHandler.stop();
      detached();
    }
 else {
      int exitValue=process.waitFor();
      streamsHandler.stop();
      completed(exitValue);
    }
  }
 catch (  Throwable t) {
    execHandle.failed(t);
  }
}
