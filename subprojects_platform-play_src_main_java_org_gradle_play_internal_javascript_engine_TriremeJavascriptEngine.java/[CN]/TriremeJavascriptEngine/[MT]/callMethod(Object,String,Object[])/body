{
  Class[] classes;
  if (args != null) {
    classes=new Class[args.length];
    for (int i=0; i < args.length; i++) {
      classes[i]=args[i].getClass();
    }
  }
 else {
    classes=null;
  }
  Method method=object.getClass().getMethod(methodName,classes);
  return method.invoke(object,args);
}
