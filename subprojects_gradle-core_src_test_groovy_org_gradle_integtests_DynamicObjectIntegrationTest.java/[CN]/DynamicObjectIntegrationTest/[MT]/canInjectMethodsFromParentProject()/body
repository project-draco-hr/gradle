{
  TestFile testDir=dist.getTestDir();
  testDir.file("settings.gradle").writelns("include 'child'");
  testDir.file("build.gradle").writelns("subprojects {","  injectedMethod = { project.name }","}");
  testDir.file("child/build.gradle").writelns("task testTask << {","   assert injectedMethod() == 'child'","}");
  executer.inDirectory(testDir).withTasks("testTask").run();
}
