{
  for (  Configuration configuration : moduleDescriptor.getConfigurations()) {
    uploadModuleDescriptor.addConfiguration(configuration);
    for (    Artifact artifact : moduleDescriptor.getArtifacts(configuration.getName())) {
      Artifact artifactWithoutFilepath=new DefaultArtifact(artifact.getModuleRevisionId(),artifact.getPublicationDate(),artifact.getName(),artifact.getType(),artifact.getExt(),createMapWithoutFilePathAttribute(artifact.getExtraAttributes()));
      uploadModuleDescriptor.addArtifact(configuration.getName(),artifactWithoutFilepath);
    }
  }
}
