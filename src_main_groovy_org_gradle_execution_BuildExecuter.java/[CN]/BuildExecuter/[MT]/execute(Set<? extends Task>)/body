{
  boolean dagNeutral=true;
  for (  Task task : tasks) {
    dagNeutral=execute(new TreeSet<Task>(dag.getChildren(task)));
    if (!task.getExecuted()) {
      logger.info("Executing: " + task);
      ((TaskInternal)task).execute();
      if (dagNeutral) {
        dagNeutral=task.isDagNeutral();
      }
    }
  }
  return dagNeutral;
}
