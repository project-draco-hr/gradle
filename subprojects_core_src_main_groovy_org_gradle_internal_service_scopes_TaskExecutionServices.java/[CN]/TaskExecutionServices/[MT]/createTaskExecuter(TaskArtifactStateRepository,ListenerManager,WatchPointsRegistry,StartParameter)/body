{
  return new ExecuteAtMostOnceTaskExecuter(new SkipOnlyIfTaskExecuter(new SkipTaskWithNoActionsExecuter(new SkipEmptySourceFilesTaskExecuter(new WatchPointsRegistrarTaskExecuter(watchPointsRegistry,startParameter.isContinuousModeEnabled(),new ValidatingTaskExecuter(new SkipUpToDateTaskExecuter(repository,new PostExecutionAnalysisTaskExecuter(new ExecuteActionsTaskExecuter(listenerManager.getBroadcaster(TaskActionListener.class))))))))));
}
