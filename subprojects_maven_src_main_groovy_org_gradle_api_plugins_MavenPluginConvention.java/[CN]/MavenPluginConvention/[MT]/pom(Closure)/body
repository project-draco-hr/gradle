{
  Factory<MavenPom> pomFactory=mavenFactory.newMavenPomFactory(project.getConfigurations(),mavenFactory.newConf2ScopeMappingContainer(conf2ScopeMappings.getMappings()),mavenFactory.newPomDependenciesConverter(mavenFactory.newExcludeRuleConverter()),project.getFileResolver());
  MavenPom pom=pomFactory.create();
  pom.setGroupId(project.getGroup().toString());
  pom.setArtifactId(project.getName());
  pom.setVersion(project.getVersion().toString());
  return ConfigureUtil.configure(configureClosure,pom);
}
