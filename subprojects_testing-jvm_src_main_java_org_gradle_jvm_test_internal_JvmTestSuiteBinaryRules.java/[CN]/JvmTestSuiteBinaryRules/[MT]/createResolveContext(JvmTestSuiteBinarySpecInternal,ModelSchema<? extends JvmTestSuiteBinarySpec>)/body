{
  LibraryBinaryIdentifier id=testBinary.getId();
  LibraryBinaryIdentifier thisId=new DefaultLibraryBinaryIdentifier(id.getProjectPath(),id.getLibraryName() + "Test",id.getVariant());
  return new LocalComponentResolveContext(thisId,DefaultVariantsMetaData.extractFrom(testBinary,schema),runtimeDependencies(testBinary),UsageKind.RUNTIME,testBinary.getDisplayName());
}
