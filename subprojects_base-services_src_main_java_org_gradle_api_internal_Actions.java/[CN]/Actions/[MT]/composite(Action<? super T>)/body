{
  final List<Action<? super T>> actionsCopy=new ArrayList<Action<? super T>>(actions.length);
  Collections.addAll(actionsCopy,actions);
  return new Action<T>(){
    public void execute(    final T item){
      for (      Action<? super T> action : actionsCopy) {
        action.execute(item);
      }
    }
  }
;
}
