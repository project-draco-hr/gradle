{
  ChildNodeInitializerStrategyAccessor<E> strategyAccessor=ChildNodeInitializerStrategyAccessors.fromPrivateData();
  Class<? extends T> implementationType=schema.getImplementationType().asSubclass(schema.getType().getConcreteClass());
  return AssignProjectionsNoInputsAction.of(ModelReference.of(path),descriptor,new SpecializedModelMapProjection<T,E>(schema.getType(),schema.getElementType(),implementationType,strategyAccessor),ModelMapModelProjection.unmanaged(schema.getElementType(),strategyAccessor));
}
