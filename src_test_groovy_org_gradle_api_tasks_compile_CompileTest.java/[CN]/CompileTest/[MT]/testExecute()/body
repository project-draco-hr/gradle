{
  setUpMocksAndAttributes(compile);
  context.checking(new Expectations(){
{
      one(antCompileMock).execute(compile.getSrcDirs(),compile.getIncludes(),compile.getExcludes(),compile.getDestinationDir(),compile.getClasspath(),compile.getSourceCompatibility(),compile.getTargetCompatibility(),compile.getOptions(),compile.getProject().getAnt());
    }
  }
);
  compile.execute();
}
