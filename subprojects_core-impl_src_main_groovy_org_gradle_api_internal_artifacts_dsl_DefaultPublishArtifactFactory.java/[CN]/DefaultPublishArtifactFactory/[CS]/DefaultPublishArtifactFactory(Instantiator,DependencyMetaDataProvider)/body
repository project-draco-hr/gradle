{
  this.instantiator=instantiator;
  this.metaDataProvider=metaDataProvider;
  FileNotationParser fileParser=new FileNotationParser();
  delegate=new NotationParserBuilder<PublishArtifact>().resultingType(PublishArtifact.class).parser(new ArchiveTaskNotationParser()).parser(new FileMapNotationParser(fileParser)).parser(fileParser).toComposite();
}
