{
  this.instantiator=instantiator;
  this.metaDataProvider=metaDataProvider;
  delegate=new NotationParserBuilder<PublishArtifact>().resultingType(PublishArtifact.class).parser(new ArchiveTaskNotationParser()).parser(new FileNotationParser()).invalidNotationMessage("The artifact notation cannot be used to form the artifact.\n" + "The supported artifact notation types/formats:\n" + "  - instances of AbstractArchiveTask, e.g. jar\n"+ "  - instances of File").toComposite();
}
