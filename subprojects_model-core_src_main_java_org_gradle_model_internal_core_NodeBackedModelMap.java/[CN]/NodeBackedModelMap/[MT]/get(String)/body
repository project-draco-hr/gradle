{
  MutableModelNode link=modelNode.getLink(name);
  if (link == null) {
    return null;
  }
  link.ensureUsable();
  if (mutable) {
    return link.asWritable(elementType,sourceDescriptor,null).getInstance();
  }
 else {
    return link.asReadOnly(elementType,sourceDescriptor).getInstance();
  }
}
