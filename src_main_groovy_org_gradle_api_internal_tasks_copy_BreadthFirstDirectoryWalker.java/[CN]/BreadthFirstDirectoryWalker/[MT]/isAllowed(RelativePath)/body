{
  if (!includes.isEmpty()) {
    boolean accepted=false;
    for (    PatternMatcher include : includes) {
      accepted=include.isSatisfiedBy(path);
      if (accepted) {
        break;
      }
    }
    if (!accepted) {
      return false;
    }
  }
  if (!excludes.isEmpty()) {
    for (    PatternMatcher exclude : excludes) {
      if (exclude.isSatisfiedBy(path)) {
        return false;
      }
    }
  }
  return true;
}
