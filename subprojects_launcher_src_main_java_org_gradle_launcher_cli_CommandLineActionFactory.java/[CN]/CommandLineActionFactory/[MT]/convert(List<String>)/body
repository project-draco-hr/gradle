{
  ServiceRegistry loggingServices=createLoggingServices();
  LoggingConfiguration loggingConfiguration=new LoggingConfiguration();
  return new ExceptionReportingAction(new WithLogging(loggingServices,args,loggingConfiguration,new JavaRuntimeValidationAction(new ParseAndBuildAction(loggingServices,args))),new BuildExceptionReporter(loggingServices.get(StyledTextOutputFactory.class),loggingConfiguration,clientMetaData()));
}
