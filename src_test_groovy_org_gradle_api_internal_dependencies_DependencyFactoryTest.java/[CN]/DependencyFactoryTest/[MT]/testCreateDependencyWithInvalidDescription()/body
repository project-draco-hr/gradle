{
  context.checking(new Expectations(){
{
      allowing(testImplIntegerFactoryMock).createDependency(with(equal(TEST_CONFIGURATION_SET)),with(not(Integer.class)),with(same(project)));
      will(throwException(new UnknownDependencyNotation()));
      allowing(testImplStringFactoryMock).createDependency(with(equal(TEST_CONFIGURATION_SET)),with(not(String.class)),with(same(project)));
      will(throwException(new UnknownDependencyNotation()));
    }
  }
);
  dependencyFactory.createDependency(TEST_CONFIGURATION_SET,new Point(3,4),project);
}
