{
  GradleVersion gradleVersion=new GradleVersion();
  return PomHeaderWriter.HEADER_XML + NL + PomHeaderWriter.GENERATE_TEXT_PRE+ GradleVersion.URL+ NL+ PomHeaderWriter.GENERATE_TEXT_VERSION+ gradleVersion.getVersion()+ " "+ gradleVersion.getBuildTime()+ NL+ PomHeaderWriter.GENERATE_TEXT_POST+ PomHeaderWriter.HEADER_XMLNS+ NL+ PomHeaderWriter.HEADER_MAVEN+ NL;
}
