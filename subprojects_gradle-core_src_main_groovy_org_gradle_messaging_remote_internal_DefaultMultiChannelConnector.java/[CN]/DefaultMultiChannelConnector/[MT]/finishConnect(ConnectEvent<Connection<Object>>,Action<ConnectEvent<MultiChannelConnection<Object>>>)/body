{
  URI localAddress=event.getLocalAddress();
  URI remoteAddress=event.getRemoteAddress();
  DefaultMultiChannelConnection channelConnection=new DefaultMultiChannelConnection(executorFactory,String.format("Incoming Connection %s",localAddress),event.getConnection(),localAddress,remoteAddress);
  action.execute(new ConnectEvent<MultiChannelConnection<Object>>(channelConnection,localAddress,remoteAddress));
}
