{
  if (outputEvent.getDestination() == TestOutputEvent.Destination.StdOut && shouldLogStandardStreamEvent(TestLogEvent.STANDARD_OUT)) {
    logEvent(descriptor,TestLogEvent.STANDARD_OUT,outputEvent.getMessage());
  }
 else   if (outputEvent.getDestination() == TestOutputEvent.Destination.StdErr && shouldLogStandardStreamEvent(TestLogEvent.STANDARD_ERR)) {
    logEvent(descriptor,TestLogEvent.STANDARD_ERR,outputEvent.getMessage());
  }
}
