{
  if (outputEvent.getDestination() == TestOutputEvent.Destination.StdOut && shouldLogStandardStreamEvent(TestLogEvent.STANDARD_OUT)) {
    logEvent(descriptor,TestLogEvent.STANDARD_OUT,"\n" + TextUtil.indent(outputEvent.getMessage(),INDENT));
  }
 else   if (outputEvent.getDestination() == TestOutputEvent.Destination.StdErr && shouldLogStandardStreamEvent(TestLogEvent.STANDARD_ERROR)) {
    logEvent(descriptor,TestLogEvent.STANDARD_ERROR,"\n" + TextUtil.indent(outputEvent.getMessage(),INDENT));
  }
}
