{
  String moduleOrg=readString();
  String moduleName=readString();
  String name=readString();
  String type=readString();
  String ext=readString();
  String[] confs=readStringArray();
  String matcher=readString();
  DefaultExcludeRule rule=new DefaultExcludeRule(IvyUtil.createArtifactId(moduleOrg,moduleName,name,type,ext),resolverStrategy.getPatternMatcher(matcher),null);
  for (  String conf : confs) {
    rule.addConfiguration(conf);
  }
  return rule;
}
