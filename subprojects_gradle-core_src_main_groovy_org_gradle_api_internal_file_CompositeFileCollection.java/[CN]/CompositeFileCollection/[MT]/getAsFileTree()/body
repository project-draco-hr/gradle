{
  return new CompositeFileTree(){
    @Override protected void addSourceCollections(    Collection<FileCollection> sources){
      for (      FileCollection collection : CompositeFileCollection.this.getSourceCollections()) {
        sources.add(collection.getAsFileTree());
      }
    }
    @Override public String getDisplayName(){
      return CompositeFileCollection.this.getDisplayName();
    }
    @Override protected void addDependencies(    DefaultTaskDependency dependency){
      CompositeFileCollection.this.addDependencies(dependency);
    }
  }
;
}
