{
  try {
    for (    DependencyResolver resolver : publishResolvers) {
      logger.info("Publishing to Resolver {}",resolver);
      publishEngine.publish(moduleDescriptor,ARTIFACT_PATTERN,resolver,publishOptionsFactory.createPublishOptions(configurations,descriptorDestination));
    }
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
