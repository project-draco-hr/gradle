{
  final TestClosure closure=context.mock(TestClosure.class);
  container.add("c");
  container.add(new StringBuffer("b"));
  context.checking(new Expectations(){
{
      one(closure).call("c");
      one(closure).call("a");
    }
  }
);
  container.withType(String.class,TestUtil.toClosure(closure));
  container.add("a");
}
