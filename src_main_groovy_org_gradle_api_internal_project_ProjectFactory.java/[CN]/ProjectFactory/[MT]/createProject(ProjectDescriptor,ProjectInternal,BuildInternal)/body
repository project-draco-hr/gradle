{
  File buildFile=projectDescriptor.getBuildFile();
  ScriptSource source;
  if (embeddedScript != null) {
    source=embeddedScript;
  }
 else   if (!buildFile.exists()) {
    source=new StringScriptSource("empty build file","");
  }
 else {
    source=new FileScriptSource("build file",buildFile);
  }
  DefaultProject project=new DefaultProject(projectDescriptor.getName(),parent,projectDescriptor.getDir(),projectDescriptor.getBuildFileName(),source,build.getBuildScriptClassLoader(),taskFactory,dependencyManagerFactory,antBuilderFactory,buildScriptProcessor,pluginRegistry,build.getProjectRegistry(),build);
  if (parent != null) {
    parent.addChildProject(project);
  }
  return project;
}
