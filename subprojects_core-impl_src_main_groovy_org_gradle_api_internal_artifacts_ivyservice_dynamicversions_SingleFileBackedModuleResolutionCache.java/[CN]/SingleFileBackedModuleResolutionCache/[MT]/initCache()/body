{
  File dynamicRevisionsFile=new File(cacheMetadata.getCacheDir(),"dynamic-revisions.bin");
  FileLock dynamicRevisionsLock=cacheLockingManager.getCacheMetadataFileLock(dynamicRevisionsFile);
  return new BTreePersistentIndexedCache<RevisionKey,ModuleResolutionCacheEntry>(dynamicRevisionsFile,dynamicRevisionsLock,new DefaultSerializer<ModuleResolutionCacheEntry>(ModuleResolutionCacheEntry.class.getClassLoader()));
}
