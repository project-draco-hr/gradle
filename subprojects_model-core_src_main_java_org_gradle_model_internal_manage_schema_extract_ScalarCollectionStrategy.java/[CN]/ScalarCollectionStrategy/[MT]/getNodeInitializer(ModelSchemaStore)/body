{
  return new Function<ModelCollectionSchema<T,E>,NodeInitializer>(){
    @Override public NodeInitializer apply(    ModelCollectionSchema<T,E> schema){
      return new ProjectionOnlyNodeInitializer(TypedModelProjection.of(ModelTypes.list(schema.getElementType()),new ListViewFactory<E>(schema.getElementType())));
    }
  }
;
}
