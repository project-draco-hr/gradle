{
  if (stateEquals(ExecHandleState.STARTED))   throw new IllegalStateException("already started!");
  execHandleRunLock.lock();
  try {
    start();
    boolean proceed=false;
    while (!proceed) {
switch (getState()) {
case INIT:
        Thread.yield();
      break;
default :
    proceed=true;
}
}
if (getState() == ExecHandleState.STARTED) {
waitForFinish();
}
 else {
shutdownThreadPool();
}
}
  finally {
execHandleRunLock.unlock();
}
return getState();
}
