{
  incomingHubConnector=new TcpIncomingConnector<InterHubMessage>(get(ExecutorFactory.class),new InterHubMessageSerializer(new TypeSafeSerializer<MethodInvocation>(MethodInvocation.class,new MethodInvocationSerializer(messageClassLoader,new JavaSerializer<Object[]>(messageClassLoader)))),get(InetAddressFactory.class),idGenerator);
  return incomingHubConnector;
}
