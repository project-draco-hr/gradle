{
  Object dirs=args.get(argName);
  if (dirs == null) {
    return null;
  }
  Iterable<Object> iterable;
  if (dirs instanceof Iterable) {
    iterable=(Iterable<Object>)dirs;
  }
 else {
    iterable=WrapUtil.toSet(dirs);
  }
  List<String> list=new ArrayList<String>();
  for (  Object o : iterable) {
    list.add(o.toString());
  }
  return list;
}
