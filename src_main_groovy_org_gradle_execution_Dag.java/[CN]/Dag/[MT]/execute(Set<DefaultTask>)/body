{
  boolean dagNeutral=true;
  for (  DefaultTask task : tasks) {
    dagNeutral=execute(new TreeSet(getChildren(task)));
    if (!task.getExecuted()) {
      logger.info(Logging.HIGH_LEVEL,"Executing: " + task);
      task.execute();
      if (dagNeutral) {
        dagNeutral=task.isDagNeutral();
      }
    }
  }
  return dagNeutral;
}
