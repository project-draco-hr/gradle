{
  BufferedWriter writer=null;
  try {
    GFileUtils.parentMkdirs(outputFile);
    writer=new BufferedWriter(new OutputStreamWriter(new FileOutputStream(outputFile),"UTF-8"));
    generate(writer);
    copyCss();
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
 finally {
    if (writer != null) {
      try {
        writer.close();
      }
 catch (      IOException e) {
        throw new UncheckedIOException(e);
      }
    }
  }
}
