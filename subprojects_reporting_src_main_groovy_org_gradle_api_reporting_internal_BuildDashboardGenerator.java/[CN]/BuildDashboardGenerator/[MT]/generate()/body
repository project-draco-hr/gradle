{
  try {
    GFileUtils.parentMkdirs(outputFile);
    Writer writer=new BufferedWriter(new OutputStreamWriter(new FileOutputStream(outputFile),"UTF-8"));
    try {
      generate(writer);
    }
  finally {
      writer.close();
    }
    copyCss();
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
