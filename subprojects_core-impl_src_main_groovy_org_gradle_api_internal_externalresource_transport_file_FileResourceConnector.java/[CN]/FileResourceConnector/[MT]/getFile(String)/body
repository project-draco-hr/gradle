{
  URI uri;
  try {
    uri=new URI(absolutePath);
  }
 catch (  URISyntaxException e) {
    throw UncheckedException.throwAsUncheckedException(e);
  }
  File f=new File(uri);
  if (!f.isAbsolute()) {
    throw new IllegalArgumentException("Filename must be absolute: " + absolutePath);
  }
  return f;
}
