{
  File dir=getFile(parent);
  if (dir.exists() && dir.isDirectory()) {
    String[] names=dir.list();
    if (names != null) {
      List<URI> ret=new ArrayList<URI>(names.length);
      for (      String name : names) {
        ret.add(parent.resolve(name));
      }
      return ret;
    }
  }
  return null;
}
