{
  validateTimeStamps();
  try {
    if (isDirectory()) {
      GFileUtils.mkdirs(target);
    }
 else {
      GFileUtils.mkdirs(target.getParentFile());
      copyFile(target);
    }
    getChmod().chmod(target,getMode());
    return true;
  }
 catch (  Exception e) {
    throw new GradleException(String.format("Could not copy %s to '%s'.",getDisplayName(),target),e);
  }
}
