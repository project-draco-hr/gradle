{
  for (  Task task : project.getTasks()) {
    final String name=task.getName();
    final ModelPath modelPath=TaskContainerInternal.MODEL_PATH.child(name);
    if (project.getModelRegistry().state(modelPath) == null) {
      project.getModelRegistry().create(InstanceBackedModelCreator.of(ModelReference.of(modelPath,ModelType.of(Task.class)),new SimpleModelRuleDescriptor("Project.<init>.tasks." + name + "()"),task));
    }
  }
}
