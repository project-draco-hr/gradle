{
  String cmd;
  if (getExecutable() != null) {
    cmd=getExecutable().replace('/',File.separatorChar);
  }
 else {
    cmd=String.format("%s\\bin\\gradle",gradleHomeDir.getAbsolutePath());
  }
  builder.executable("cmd");
  builder.args("/c",cmd);
  builder.environment("GRADLE_EXIT_CONSOLE","true");
}
