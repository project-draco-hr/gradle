{
  context.checking(new Expectations(){
{
      one(testMock).getIncludes();
      will(returnValue(null));
      one(testMock).include("**/*Tests.class","**/*Test.class");
      one(testMock).getExcludes();
      will(returnValue(null));
      one(testMock).exclude("**/Abstract*.class");
    }
  }
);
}
