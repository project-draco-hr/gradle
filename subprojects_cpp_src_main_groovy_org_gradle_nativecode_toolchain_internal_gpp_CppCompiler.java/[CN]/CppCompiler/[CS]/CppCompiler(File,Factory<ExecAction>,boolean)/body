{
  GccCompileSpecToArguments<CppCompileSpec> specToArguments=new GccCompileSpecToArguments<CppCompileSpec>(new CppCompileOptionsToArguments(),new GccCompileSourcesToArguments<CppCompileSpec>(),useCommandFile);
  this.commandLineTool=new CommandLineTool<CppCompileSpec>("C++ compile",executable,execActionFactory).withArguments(specToArguments);
}
