{
  Formatter formatter=new Formatter();
  if (messages.isEmpty()) {
    formatter.format("Could not resolve all dependencies for %s.",configuration);
  }
 else {
    formatter.format("Could not resolve all dependencies for %s:",configuration);
  }
  for (  String msg : messages) {
    formatter.format("%n    - %s",msg);
  }
  return formatter.toString();
}
