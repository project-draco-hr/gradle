{
  String repositoryId=repository.getId();
  if (repositoryId != null) {
    for (    Mirror mirror : mirrors) {
      if (repositoryId.equals(mirror.getMirrorOf())) {
        return mirror;
      }
    }
    for (    Mirror mirror : mirrors) {
      if (matchPattern(repository,mirror.getMirrorOf())) {
        return mirror;
      }
    }
  }
  return null;
}
