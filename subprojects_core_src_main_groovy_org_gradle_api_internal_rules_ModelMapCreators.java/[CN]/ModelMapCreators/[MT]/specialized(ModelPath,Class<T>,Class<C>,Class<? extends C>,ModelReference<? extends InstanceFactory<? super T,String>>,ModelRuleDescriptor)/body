{
  ChildNodeCreatorStrategy<T> childFactory=NodeBackedModelMap.createUsingFactory(factoryReference);
  ModelType<C> containerType=ModelType.of(containerClass);
  ModelType<T> modelType=ModelType.of(typeClass);
  return ModelCreators.of(ModelReference.of(path,containerType),Factories.<C>constantNull()).descriptor(descriptor).withProjection(new SpecializedModelMapProjection<C,T>(containerType,modelType,viewClass,childFactory)).withProjection(PolymorphicModelMapProjection.of(modelType,childFactory)).build();
}
