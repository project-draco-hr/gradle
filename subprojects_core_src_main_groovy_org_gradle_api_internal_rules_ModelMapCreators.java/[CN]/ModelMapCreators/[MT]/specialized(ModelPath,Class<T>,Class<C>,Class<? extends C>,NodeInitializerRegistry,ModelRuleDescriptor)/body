{
  ModelType<C> containerType=ModelType.of(containerClass);
  ModelType<T> modelType=ModelType.of(typeClass);
  ChildNodeInitializerStrategy<T> childFactory=NodeBackedModelMap.createUsingRegistry(modelType,nodeInitializerRegistry);
  return ModelCreators.of(ModelReference.of(path,containerType),Factories.<C>constantNull()).descriptor(descriptor).withProjection(new SpecializedModelMapProjection<C,T>(containerType,modelType,viewClass,childFactory)).withProjection(PolymorphicModelMapProjection.of(modelType,childFactory)).inputs(ModelReference.of(NodeInitializerRegistry.class)).build();
}
