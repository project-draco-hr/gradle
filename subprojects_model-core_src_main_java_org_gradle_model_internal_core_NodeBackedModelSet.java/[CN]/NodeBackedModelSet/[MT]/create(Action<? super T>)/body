{
  state.assertCanMutate();
  String name=String.valueOf(modelNode.getLinkCount(elementType));
  modelNode.addLink(creatorStrategy.creator(modelNode,descriptor,elementType,name));
  modelNode.applyToLink(ModelActionRole.Initialize,NoInputsModelAction.of(ModelReference.of(modelNode.getPath().child(name),elementType),NestedModelRuleDescriptor.append(descriptor,"create()"),action));
}
