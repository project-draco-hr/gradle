{
  Compile compile=javaPlugin.configureCompile((Compile)project.createTask(GUtil.map("type",GroovyCompile.class,"dependsOn",RESOURCES,"overwrite",true),COMPILE),DefaultConventionsToPropertiesMapping.COMPILE);
  compile.conventionMapping(GUtil.map("groovySourceDirs",new ConventionValue(){
    public Object getValue(    Convention convention,    Task task){
      return groovy(convention).getGroovySrcDirs();
    }
  }
,"groovyClasspath",new ConventionValue(){
    public Object getValue(    Convention convention,    Task task){
      return task.getProject().getDependencies().resolve("groovy");
    }
  }
));
}
