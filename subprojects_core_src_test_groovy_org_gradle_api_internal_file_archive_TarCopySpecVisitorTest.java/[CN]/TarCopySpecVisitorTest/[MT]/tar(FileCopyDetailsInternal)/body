{
  visitor.visit(new Action<Action<? super FileCopyDetailsInternal>>(){
    public void execute(    Action<? super FileCopyDetailsInternal> action){
      for (      FileCopyDetailsInternal f : files) {
        if (f.isDirectory()) {
          action.execute(f);
        }
 else {
          action.execute(f);
        }
      }
    }
  }
);
}
