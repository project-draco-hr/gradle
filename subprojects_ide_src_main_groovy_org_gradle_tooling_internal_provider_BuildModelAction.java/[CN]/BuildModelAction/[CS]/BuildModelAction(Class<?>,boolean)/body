{
  this.runTasks=runTasks;
  List<? extends BuildsModel> modelBuilders=asList(new NullResultBuilder(),new EclipseModelBuilder(),new IdeaModelBuilder(),new GradleProjectBuilder(),new BasicIdeaModelBuilder(),new ProjectOutcomesModelBuilder());
  for (  BuildsModel builder : modelBuilders) {
    if (builder.canBuild(type)) {
      this.builder=builder;
      return;
    }
  }
  throw new UnsupportedOperationException(String.format("I don't know how to build a model of type '%s'.",type.getSimpleName()));
}
