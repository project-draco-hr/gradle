{
  Class<?> targetType=target.getClass();
  Method getterMethod=findGetterMethod(target.getClass(),propertyName);
  if (getterMethod == null) {
    try {
      targetType.getField(propertyName);
      return true;
    }
 catch (    NoSuchFieldException ignore) {
    }
  }
 else {
    return true;
  }
  return false;
}
