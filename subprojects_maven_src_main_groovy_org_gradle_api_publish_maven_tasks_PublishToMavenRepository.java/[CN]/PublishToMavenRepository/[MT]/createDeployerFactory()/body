{
  DefaultMavenFactory mavenFactory=new DefaultMavenFactory();
  final DefaultDeployerFactory deployerFactory=new DefaultDeployerFactory(mavenFactory,loggingManagerFactory,fileResolver,new MavenPomMetaInfoProvider(){
    public File getMavenPomDir(){
      return publication.getPomDir();
    }
  }
,getProject().getConfigurations(),new DefaultConf2ScopeMappingContainer());
  return deployerFactory;
}
