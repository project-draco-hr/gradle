{
  return new CustomTaskFactoryDeployerFactory(new DefaultMavenFactory(),loggingManagerFactory,fileResolver,new MavenPomMetaInfoProvider(){
    public File getMavenPomDir(){
      return publication.getPomDir();
    }
  }
,getProject().getConfigurations(),new DefaultConf2ScopeMappingContainer(),new NoInstallDeployTaskFactory(getTemporaryDirFactory()));
