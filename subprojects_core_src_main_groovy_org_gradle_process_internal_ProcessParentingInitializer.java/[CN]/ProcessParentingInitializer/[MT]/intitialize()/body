{
  if (initialized) {
    return;
  }
  synchronizer.synchronize(new Operation(){
    public void execute(){
      if (initialized) {
        return;
      }
      initialized=true;
      if (OperatingSystem.current().isWindows()) {
        new WindowsHandlesManipulator().uninheritStandardStreams();
      }
    }
  }
);
}
