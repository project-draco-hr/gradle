{
  if (defaultTasks == null) {
    throw new InvalidUserDataException("Default tasks must not be null!");
  }
  this.defaultTasks=new ArrayList<String>();
  for (  String defaultTask : defaultTasks) {
    if (defaultTask == null) {
      throw new InvalidUserDataException("Default tasks must not be null!");
    }
    this.defaultTasks.add(defaultTask);
  }
}
