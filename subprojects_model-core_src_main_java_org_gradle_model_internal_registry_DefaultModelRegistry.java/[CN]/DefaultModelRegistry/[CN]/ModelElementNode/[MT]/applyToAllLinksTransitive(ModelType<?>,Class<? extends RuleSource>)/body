{
  registerListener(new ModelCreationListener(){
    @Override public ModelPath getAncestor(){
      return ModelElementNode.this.getPath();
    }
    @Nullable @Override public ModelType<?> getType(){
      return type;
    }
    @Override public boolean onCreate(    ModelNodeInternal node){
      node.applyToSelf(rules);
      return false;
    }
  }
);
}
