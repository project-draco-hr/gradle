{
  GoalGraphNode node=graph.get(goal);
  if (node == null) {
switch (goal.state) {
case Known:
      node=new MakeKnown(goal);
    break;
case Created:
  node=new Create(goal);
break;
case GraphClosed:
node=new CloseGraph(goal);
break;
default :
node=new ApplyActions(goal);
}
graph.put(goal,node);
}
return node;
}
