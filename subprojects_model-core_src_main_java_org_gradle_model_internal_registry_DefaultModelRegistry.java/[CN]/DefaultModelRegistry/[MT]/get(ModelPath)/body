{
  ModelSearchResult searchResult=modelGraph.search(path);
  ModelNode targetNode=searchResult.getTargetNode();
  if (targetNode != null) {
    close(targetNode);
    return targetNode;
  }
  BoundModelCreator inCreation=creators.remove(path);
  if (inCreation == null) {
    return null;
  }
 else {
    return createAndClose(inCreation);
  }
}
