{
  ModelElement<?> closed=store.get(path);
  if (closed != null) {
    return toState(closed.getReference(),ModelState.Status.FINALIZED);
  }
  ModelCreator<?> creator=creations.get(path);
  if (creator != null) {
    return toState(creator.getReference(),ModelState.Status.PENDING);
  }
  return null;
}
