{
  if (binders.isEmpty()) {
    return;
  }
  boolean newInputsBound=true;
  while (!binders.isEmpty() && newInputsBound) {
    newInputsBound=false;
    for (int i=0; i < binders.size(); i++) {
      newInputsBound=newInputsBound || tryForceBind(binders.get(i));
    }
  }
  if (!binders.isEmpty()) {
    throw unbound(binders);
  }
}
