{
  return Iterables.filter(Arrays.asList(clazz.getMethods()),new Predicate<Method>(){
    @Override public boolean apply(    Method method){
      boolean include=true;
      if (!clazz.isInterface()) {
        include=!method.isSynthetic() && !IGNORED_METHODS.contains(METHOD_EQUIVALENCE.wrap(method));
      }
      if (include) {
        include=!ignoreMethod(method);
      }
      return include;
    }
  }
);
}
