{
  int endIndex=zipEntry.getName().lastIndexOf("/");
  if (endIndex > 0) {
    String packageName=zipEntry.getName().substring(0,endIndex);
    for (    String exclude : getExcludes()) {
      if (zipEntry.getName().startsWith(exclude + "/")) {
        return;
      }
    }
    builder.addWord(packageName);
  }
}
