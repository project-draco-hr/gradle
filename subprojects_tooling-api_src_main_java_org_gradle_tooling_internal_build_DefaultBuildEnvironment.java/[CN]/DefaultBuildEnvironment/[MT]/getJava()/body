{
  return new JavaEnvironment(){
    @Override public File getJavaHome(){
      return javaHome;
    }
    @Override public List<String> getJvmArguments(){
      return effectiveJvmArguments;
    }
    @Override public List<String> getRequestedJvmArguments(){
      return jvmArguments;
    }
    @Override public List<String> getAllJvmArguments(){
      return allJvmArguments;
    }
    @Override public Map<String,String> getSystemProperties(){
      return effectiveSystemProperties;
    }
    @Override public Map<String,String> getRequestedSystemProperties(){
      return systemProperties;
    }
  }
;
}
