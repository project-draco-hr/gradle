{
  super(Compiler.class,instantiator);
  whenObjectAdded(new Action<Compiler>(){
    public void execute(    Compiler compiler){
      searchOrder.add(compiler);
    }
  }
);
  whenObjectRemoved(new Action<Compiler>(){
    public void execute(    Compiler compiler){
      searchOrder.remove(compiler);
    }
  }
);
}
