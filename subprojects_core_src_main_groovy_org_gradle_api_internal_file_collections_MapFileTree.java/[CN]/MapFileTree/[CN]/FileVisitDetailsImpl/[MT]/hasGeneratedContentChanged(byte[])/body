{
  boolean hasChanged=false;
  if (generatedContent.length == file.length()) {
    try {
      byte[] existingContent=Files.toByteArray(file);
      if (!Arrays.equals(generatedContent,existingContent)) {
        hasChanged=true;
      }
    }
 catch (    IOException e) {
      hasChanged=true;
    }
  }
 else {
    hasChanged=true;
  }
  return hasChanged;
}
