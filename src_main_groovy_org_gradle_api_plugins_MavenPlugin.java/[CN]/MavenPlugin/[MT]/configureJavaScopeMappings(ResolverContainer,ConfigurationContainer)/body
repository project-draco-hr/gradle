{
  resolverFactory.getMavenScopeMappings().addMapping(COMPILE_PRIORITY,configurations.get(JavaPlugin.COMPILE),Conf2ScopeMappingContainer.COMPILE);
  resolverFactory.getMavenScopeMappings().addMapping(RUNTIME_PRIORITY,configurations.get(JavaPlugin.RUNTIME),Conf2ScopeMappingContainer.RUNTIME);
  resolverFactory.getMavenScopeMappings().addMapping(TEST_COMPILE_PRIORITY,configurations.get(JavaPlugin.TEST_COMPILE),Conf2ScopeMappingContainer.TEST);
  resolverFactory.getMavenScopeMappings().addMapping(TEST_RUNTIME_PRIORITY,configurations.get(JavaPlugin.TEST_RUNTIME),Conf2ScopeMappingContainer.TEST);
}
