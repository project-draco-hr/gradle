{
  resolverContainer.getMavenScopeMappings().addMapping(PROVIDED_COMPILE_PRIORITY,configurations.get(WarPlugin.PROVIDED_COMPILE_CONFIGURATION_NAME),Conf2ScopeMappingContainer.PROVIDED);
  resolverContainer.getMavenScopeMappings().addMapping(PROVIDED_RUNTIME_PRIORITY,configurations.get(WarPlugin.PROVIDED_RUNTIME_CONFIGURATION_NAME),Conf2ScopeMappingContainer.PROVIDED);
}
