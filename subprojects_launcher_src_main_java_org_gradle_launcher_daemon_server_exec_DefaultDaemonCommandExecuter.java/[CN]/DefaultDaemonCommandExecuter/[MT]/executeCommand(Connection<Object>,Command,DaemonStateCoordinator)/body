{
  new DaemonCommandExecution(connection,command,daemonStateCoordinator,new StopConnectionAfterExecution(),new HandleClientDisconnectBeforeSendingCommand(),new ReturnResult(),new HandleStop(),new UpdateDaemonStatus(),new ForwardOutput(loggingOutput),new ReportExceptions(loggingServices),new HandleSleep(),new EstablishBuildEnvironment(),new ExecuteBuild(loggingServices,launcherFactory)).proceed();
}
