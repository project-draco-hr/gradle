{
  new DaemonCommandExecution(connection,command,daemonStateCoordinator,new StopConnectionAfterExecution(),new HandleClientDisconnectBeforeSendingCommand(),new CatchAndForwardDaemonFailure(),new HandleStop(),new UpdateDaemonStateAndHandleBusyDaemon(),new ReturnResult(),new ForwardOutput(loggingOutput),new ReportExceptions(loggingServices),new HandleSleep(),new EstablishBuildEnvironment(),new ExecuteBuild(loggingServices,launcherFactory)).proceed();
}
