{
  new DaemonCommandExecution(new DisconnectAwareConnectionDecorator<Object>(connection,executorFactory.create("DefaultDaemonCommandExecuter > DisconnectAwareConnectionDecorator")),command,daemonContext,daemonStateCoordinator,new StopConnectionAfterExecution(),new HandleClientDisconnectBeforeSendingCommand(),new CatchAndForwardDaemonFailure(),new HandleStop(),new StartBuildOrRespondWithBusy(),new EstablishBuildEnvironment(),new LogToClient(loggingManager),new ForwardClientInput(executorFactory),new ReturnResult(),new ResetDeprecationLogger(),new CatchAndForwardDaemonFailureAsResult(),new WatchForDisconnection(),new ExecuteBuild(launcherFactory)).proceed();
}
