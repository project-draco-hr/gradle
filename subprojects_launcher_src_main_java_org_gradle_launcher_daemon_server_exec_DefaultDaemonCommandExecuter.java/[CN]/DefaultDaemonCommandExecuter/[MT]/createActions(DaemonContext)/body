{
  return new LinkedList<DaemonCommandAction>(Arrays.asList(new StopConnectionAfterExecution(),new HandleClientDisconnectBeforeSendingCommand(),new CatchAndForwardDaemonFailure(),new HandleStop(),new StartBuildOrRespondWithBusy(),new EstablishBuildEnvironment(processEnvironment),new LogToClient(loggingManager,daemonContext.getPid()),new ForwardClientInput(executorFactory),new ReturnResult(),new ResetDeprecationLogger(),new WatchForDisconnection(),new ExecuteBuild(launcherFactory)));
