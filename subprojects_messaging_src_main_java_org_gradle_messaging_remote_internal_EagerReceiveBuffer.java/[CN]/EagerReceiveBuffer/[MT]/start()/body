{
  lock.lock();
  try {
    if (state != State.Init) {
      throw new IllegalStateException("this eager receive buffer has already been started");
    }
    state=State.Started;
    for (    Receive<T> receiver : receivers) {
      asyncReceive.receiveFrom(receiver);
    }
  }
  finally {
    lock.unlock();
  }
}
