{
  return new NotationParserBuilder<Spec<DependencyResult>>().resultingType(new TypeInfo<Spec<DependencyResult>>(Spec.class)).invalidNotationMessage("Please check the input for the DependencyInsight.dependency element.").parser(new ClosureToSpecNotationParser<DependencyResult>()).parser(new DependencyResultSpecNotationParser()).toComposite();
}
