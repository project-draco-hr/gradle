{
  String[] dependencyParts=descriptor.split(":");
  Map allExtraAttributes=(dependencyParts.length == 4 ? WrapUtil.toMap(DependencyManager.CLASSIFIER,dependencyParts[3]) : new HashMap());
  allExtraAttributes.putAll(extraAttributes);
  return createDescriptor(parent,ModuleRevisionId.newInstance(dependencyParts[0],dependencyParts[1],dependencyParts[2],allExtraAttributes),force,transitive,changing,confs,excludeRules,Dependency.DEFAULT_CONFIGURATION);
}
