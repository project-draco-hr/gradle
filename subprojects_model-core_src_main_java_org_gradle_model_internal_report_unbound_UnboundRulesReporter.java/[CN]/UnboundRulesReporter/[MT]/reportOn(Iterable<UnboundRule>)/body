{
  boolean first=true;
  for (  UnboundRule rule : rules) {
    if (!first) {
      writer.println();
    }
    first=false;
    writer.print(prefix);
    rule.getDescriptor().describeTo(writer);
    if (rule.getMutableInputs().size() > 0) {
      heading("Mutable:");
      reportInputs(rule.getMutableInputs());
    }
    if (rule.getImmutableInputs().size() > 0) {
      heading("Immutable:");
      reportInputs(rule.getImmutableInputs());
    }
  }
}
