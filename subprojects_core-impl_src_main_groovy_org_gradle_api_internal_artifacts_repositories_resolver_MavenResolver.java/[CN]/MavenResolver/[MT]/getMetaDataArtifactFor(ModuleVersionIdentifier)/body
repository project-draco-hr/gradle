{
  if (isUsepoms()) {
    DefaultModuleVersionArtifactIdentifier artifactId=new DefaultModuleVersionArtifactIdentifier(moduleVersionIdentifier,moduleVersionIdentifier.getName(),"pom","pom",Collections.<String,String>emptyMap());
    return new DefaultModuleVersionArtifactMetaData(artifactId);
  }
  return null;
}
