{
  context.checking(new Expectations(){
{
      one(scriptHandlerMock).createScript(TEST_IN_MEMORY_SCRIPT_TEXT + System.getProperty("line.separator") + TEST_SCRIPT_ATACHEMENT,testClassLoader,TEST_SCRIPT_NAME,expectedScriptBaseClass);
      will(returnValue(expectedScript));
    }
  }
);
  assertSame(expectedScript,scriptProcessor.createScriptFromText(TEST_IN_MEMORY_SCRIPT_TEXT,TEST_SCRIPT_ATACHEMENT,TEST_SCRIPT_NAME,testClassLoader,expectedScriptBaseClass));
}
