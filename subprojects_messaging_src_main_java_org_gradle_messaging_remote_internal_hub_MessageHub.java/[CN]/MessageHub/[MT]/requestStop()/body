{
  lock.lock();
  try {
    if (state != State.Running) {
      return;
    }
    try {
      for (      ChannelQueue channelQueue : outgoingChannels.values()) {
        channelQueue.stop();
      }
    }
  finally {
      outgoingChannels.clear();
      state=State.Stopping;
    }
  }
  finally {
    lock.unlock();
  }
}
