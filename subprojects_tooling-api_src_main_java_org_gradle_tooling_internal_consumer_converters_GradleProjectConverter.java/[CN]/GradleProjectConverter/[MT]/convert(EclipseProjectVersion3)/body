{
  List<DefaultGradleProject> children=new LinkedList<DefaultGradleProject>();
  for (  EclipseProjectVersion3 p : project.getChildren()) {
    children.add(convert(p));
  }
  DefaultGradleProject parent=new DefaultGradleProject().setPath(project.getPath()).setName(project.getName()).setChildren(children).setDescription(project.getDescription());
  List<GradleTask> tasks=new LinkedList<GradleTask>();
  for (  TaskVersion1 t : project.getTasks()) {
    tasks.add(new DefaultGradleTask().setName(t.getName()).setPath(t.getPath()).setDescription(t.getDescription()).setProject(parent));
  }
  parent.setTasks(tasks);
  for (  DefaultGradleProject child : children) {
    child.setParent(parent);
  }
  return parent;
}
