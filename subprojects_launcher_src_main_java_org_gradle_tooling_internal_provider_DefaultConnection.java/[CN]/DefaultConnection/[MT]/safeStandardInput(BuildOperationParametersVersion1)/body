{
  InputStream is;
  try {
    new CompatibilityChecker(operationParameters).assertSupports("getStandardInput");
    is=operationParameters.getStandardInput();
  }
 catch (  IncompatibleVersionException e) {
    return null;
  }
  if (is == null) {
    return new ByteArrayInputStream(new byte[0]);
  }
  return is;
}
