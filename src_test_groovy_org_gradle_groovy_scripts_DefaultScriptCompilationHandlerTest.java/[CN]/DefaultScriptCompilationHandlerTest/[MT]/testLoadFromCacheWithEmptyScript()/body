{
  context.checking(new Expectations(){
{
      allowing(cachePropertiesHandlerMock).getCacheState(testScript,scriptCacheDir);
      will(returnValue(CachePropertiesHandler.CacheState.EMPTY_SCRIPT));
    }
  }
);
  assertThat(scriptCompilationHandler.loadFromCache(testScript,classLoader,TEST_SCRIPT_NAME,scriptCacheDir,expectedScriptClass),Matchers.instanceOf(EmptyScript.class));
}
