{
  lock.lock();
  try {
    while (!stateIn(ExecHandleState.SUCCEEDED,ExecHandleState.ABORTED,ExecHandleState.FAILED,ExecHandleState.DETACHED)) {
      try {
        condition.await();
      }
 catch (      InterruptedException e) {
      }
    }
  }
  finally {
    lock.unlock();
  }
  executor.stop();
  return result();
}
