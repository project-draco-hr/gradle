{
  try {
    ZipFile zipFile=new ZipFile(jarFile);
    try {
      ZipEntry entry=zipFile.getEntry(resource);
      return GUtil.loadProperties(zipFile.getInputStream(entry));
    }
  finally {
      zipFile.close();
    }
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
