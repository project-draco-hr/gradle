{
  ModuleRevisionId moduleRevisionId=ivyNode.getId();
  Set<String> configurations=getConfigurationHierarchy(ivyNode,configuration);
  DefaultResolvedDependency resolvedDependency=new DefaultResolvedDependency(moduleRevisionId.getOrganisation() + ":" + moduleRevisionId.getName()+ ":"+ moduleRevisionId.getRevision(),configuration,configurations,getFilesForReport(ivyNode,resolveReport.getArtifactsReports(ivyNode.getId()),configurations));
  return resolvedDependency;
}
