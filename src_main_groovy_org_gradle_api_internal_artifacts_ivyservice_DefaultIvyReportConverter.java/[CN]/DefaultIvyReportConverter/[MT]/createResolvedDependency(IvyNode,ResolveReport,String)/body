{
  ModuleRevisionId moduleRevisionId=ivyNode.getId();
  DefaultResolvedDependency resolvedDependency=new DefaultResolvedDependency(moduleRevisionId.getOrganisation() + ":" + moduleRevisionId.getName()+ ":"+ moduleRevisionId.getRevision(),configuration,getFilesForReport(ivyNode,resolveReport.getArtifactsReports(ivyNode.getId()),configuration));
  return resolvedDependency;
}
