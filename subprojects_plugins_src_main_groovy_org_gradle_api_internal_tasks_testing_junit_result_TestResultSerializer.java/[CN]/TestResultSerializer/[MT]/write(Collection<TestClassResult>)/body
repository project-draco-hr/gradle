{
  try {
    OutputStream outputStream=new FileOutputStream(resultsFile);
    try {
      if (!results.isEmpty()) {
        Output output=new Output(outputStream);
        output.writeInt(RESULT_VERSION,true);
        write(results,output);
        output.flush();
      }
    }
  finally {
      outputStream.close();
    }
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
