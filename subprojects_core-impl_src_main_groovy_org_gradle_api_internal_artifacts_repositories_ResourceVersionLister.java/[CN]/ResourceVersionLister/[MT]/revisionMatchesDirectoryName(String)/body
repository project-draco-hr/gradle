{
  int index=pattern.indexOf(REVISION_TOKEN);
  boolean patternStartsWithRevisionToken=index == 0;
  return (pattern.length() <= index + REV_TOKEN_LENGTH) || fileSeparator.equals(pattern.substring(index + REV_TOKEN_LENGTH,index + REV_TOKEN_LENGTH + 1)) && (patternStartsWithRevisionToken || fileSeparator.equals(pattern.substring(index - 1,index)));
}
