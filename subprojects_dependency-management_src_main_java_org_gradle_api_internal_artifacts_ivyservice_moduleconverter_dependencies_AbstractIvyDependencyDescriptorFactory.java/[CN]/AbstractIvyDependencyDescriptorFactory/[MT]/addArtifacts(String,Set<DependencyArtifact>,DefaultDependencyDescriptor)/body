{
  for (  DependencyArtifact artifact : artifacts) {
    DefaultDependencyArtifactDescriptor artifactDescriptor;
    try {
      artifactDescriptor=new DefaultDependencyArtifactDescriptor(dependencyDescriptor,artifact.getName(),artifact.getType(),getExtension(artifact),artifact.getUrl() != null ? new URL(artifact.getUrl()) : null,artifact.getClassifier() != null ? WrapUtil.toMap(Dependency.CLASSIFIER,artifact.getClassifier()) : null);
    }
 catch (    MalformedURLException e) {
      throw new InvalidUserDataException("URL for artifact can't be parsed: " + artifact.getUrl(),e);
    }
    dependencyDescriptor.addDependencyArtifact(configuration,artifactDescriptor);
  }
}
