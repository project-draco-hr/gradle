{
  AbstractProject dependencyProject=HelperUtil.createRootProject(new File("someName"));
  dependencyProject.setGroup("someGroup");
  dependencyProject.setVersion("someVersion");
  final ProjectDependency projectDependency=new DefaultProjectDependency(dependencyProject,dependencyConfiguration,new ProjectDependenciesBuildInstruction(Collections.<String>emptyList()));
  return projectDependency;
}
