{
  UnboundRuleInput.Builder builder=UnboundRuleInput.type(reference.getType().toString());
  if (binding != null) {
    builder.bound();
  }
  ModelPath path=reference.getPath();
  if (path != null) {
    builder.path(path.toString()).suggestions(CollectionUtils.stringize(suggestionsProvider.transform(path)));
  }
  return builder;
}
