{
  UnboundRuleInput.Builder builder=UnboundRuleInput.builder();
  ModelPath path;
  if (binding == null) {
    path=reference.getPath();
  }
 else {
    path=binding.getPath();
    builder.bound();
  }
  if (path != null) {
    builder.path(path.toString()).suggestions(CollectionUtils.collect(suggestionsProvider.transform(path),new Transformer<String,ModelPath>(){
      public String transform(      ModelPath original){
        return original.toString();
      }
    }
));
  }
  return builder.type(reference.getType().toString());
}
