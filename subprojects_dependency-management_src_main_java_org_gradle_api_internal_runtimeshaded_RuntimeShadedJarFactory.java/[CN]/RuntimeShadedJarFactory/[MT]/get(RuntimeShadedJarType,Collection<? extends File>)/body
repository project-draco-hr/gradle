{
  final File jarFile=jarFile(cache,type);
  if (!jarFile.exists()) {
    cache.useCache("Generating " + jarFile.getName(),new Runnable(){
      public void run(){
        if (!jarFile.exists()) {
          creator.create(jarFile,classpath);
        }
      }
    }
);
  }
  LOGGER.debug("Using Gradle runtime shaded JAR file: {}",jarFile);
  return jarFile;
}
