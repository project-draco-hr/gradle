{
  for (  DependencyDescriptor dependencyDescriptor : moduleDescriptor.getDependencies()) {
    if (dependencyDescriptor.getDependencyRevisionId().equals(targetDescriptor.getDependencyRevisionId()) && Arrays.equals(dependencyDescriptor.getModuleConfigurations(),targetDescriptor.getModuleConfigurations())) {
      return (DefaultDependencyDescriptor)dependencyDescriptor;
    }
  }
  return null;
}
