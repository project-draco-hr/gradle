{
  final GradleException exception=new GradleException();
  final Matcher<String> errorMessage=allOf(containsString("Build failed with an exception."),containsString(GradleException.class.getName() + " (no error message)"));
  context.checking(new Expectations(){
{
      one(logger).error(with(errorMessage));
    }
  }
);
  reporter.buildFinished(HelperUtil.createBuildResult(exception));
}
