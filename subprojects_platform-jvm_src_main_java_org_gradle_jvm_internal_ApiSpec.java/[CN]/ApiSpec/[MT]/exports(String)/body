{
  final PackageName packageName;
  try {
    packageName=PackageName.of(value);
  }
 catch (  IllegalArgumentException cause) {
    throw new InvalidUserDataException(format("Invalid public API specification: %s",cause.getMessage()),cause);
  }
  if (!exports.add(packageName)) {
    throw new InvalidUserDataException(format("Invalid public API specification: package '%s' has already been exported",packageName));
  }
}
