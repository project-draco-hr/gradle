{
  if (externalProcess != null) {
    requestShutdown();
    protocol.aboutToKillProcess();
    try {
      externalProcess.stop();
    }
 catch (    InterruptedException e) {
      logger.error("Stopping external process",e);
    }
    setExternalProcess(null);
    notifyClientExited(-1,"Process Canceled");
  }
}
