{
  if (testFramework == null)   throw new IllegalArgumentException("testFramework is null!");
  this.testFrameworkInstance=testFramework.getInstance(this);
  testFrameworkInstance.initialize(getProject(),this);
  if (testFrameworkConfigure != null)   ConfigureUtil.configure(testFrameworkConfigure,testFrameworkInstance.getOptions());
  return testFrameworkInstance;
}
