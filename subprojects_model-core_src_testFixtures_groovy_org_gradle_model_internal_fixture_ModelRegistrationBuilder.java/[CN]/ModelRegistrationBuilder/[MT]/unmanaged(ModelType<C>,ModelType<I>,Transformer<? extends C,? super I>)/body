{
  return ModelRegistrations.of(path).action(ModelActionRole.Create,ModelReference.of(inputModelType),new BiAction<MutableModelNode,I>(){
    @Override public void execute(    MutableModelNode mutableModelNode,    I input){
      mutableModelNode.setPrivateData(modelType,action.transform(input));
    }
  }
).withProjection(new UnmanagedModelProjection<C>(modelType,true,true)).descriptor(descriptor).build();
}
