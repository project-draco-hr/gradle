{
  return ModelRegistrations.of(path).action(ModelActionRole.Create,ModelReference.of(inputPath,ModelType.UNTYPED,inputDescriptor),new BiAction<MutableModelNode,Object>(){
    @Override public void execute(    MutableModelNode mutableModelNode,    Object input){
      mutableModelNode.setPrivateData(modelType,action.transform(input));
    }
  }
).withProjection(new UnmanagedModelProjection<C>(modelType,true,true)).descriptor(descriptor).build();
}
