{
  if (!working) {
    throw new IllegalStateException("Unit of work has not been started.");
  }
  try {
    for (    MultiProcessSafePersistentIndexedCache<?,?> cache : caches) {
      cache.onEndWork();
    }
    if (lock != null) {
      lock.close();
    }
  }
  finally {
    lock=null;
    working=false;
  }
}
