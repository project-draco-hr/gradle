{
  return ModelCreators.of(ModelReference.of(path),new BiAction<MutableModelNode,Inputs>(){
    @Override public void execute(    MutableModelNode mutableModelNode,    Inputs inputs){
      action.execute(mutableModelNode);
    }
  }
).withProjection(new UnmanagedModelProjection<C>(modelType,true,true)).descriptor(descriptor).build();
}
