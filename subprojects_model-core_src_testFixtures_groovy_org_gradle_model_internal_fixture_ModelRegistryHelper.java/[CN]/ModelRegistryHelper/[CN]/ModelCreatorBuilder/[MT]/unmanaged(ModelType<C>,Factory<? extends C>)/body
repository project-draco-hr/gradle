{
  return ModelCreators.of(ModelReference.of(path),new BiAction<MutableModelNode,List<ModelView<?>>>(){
    @Override public void execute(    MutableModelNode mutableModelNode,    List<ModelView<?>> inputs){
      mutableModelNode.setPrivateData(modelType,initializer.create());
    }
  }
).withProjection(new UnmanagedModelProjection<C>(modelType,true,true)).descriptor(descriptor).build();
}
