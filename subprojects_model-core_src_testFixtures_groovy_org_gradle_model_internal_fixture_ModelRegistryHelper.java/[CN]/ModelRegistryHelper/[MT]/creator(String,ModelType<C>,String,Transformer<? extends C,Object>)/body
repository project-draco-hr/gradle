{
  return ModelCreators.of(ModelPath.path(path),new BiAction<MutableModelNode,List<ModelView<?>>>(){
    @Override public void execute(    MutableModelNode mutableModelNode,    List<ModelView<?>> inputs){
      mutableModelNode.setPrivateData(modelType,action.transform(inputs.get(0).getInstance()));
    }
  }
).withProjection(new UnmanagedModelProjection<C>(modelType,true,true)).inputs(refs(ModelReference.of(inputPath))).descriptor("create " + path).build();
}
