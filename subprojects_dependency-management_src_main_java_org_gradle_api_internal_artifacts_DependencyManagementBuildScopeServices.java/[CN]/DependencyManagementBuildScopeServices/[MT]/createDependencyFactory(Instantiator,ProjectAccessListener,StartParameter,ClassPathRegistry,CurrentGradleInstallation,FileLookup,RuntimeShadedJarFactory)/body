{
  DefaultProjectDependencyFactory factory=new DefaultProjectDependencyFactory(projectAccessListener,instantiator,startParameter.isBuildProjectDependencies());
  ProjectDependencyFactory projectDependencyFactory=new ProjectDependencyFactory(factory);
  return new DefaultDependencyFactory(DependencyNotationParser.parser(instantiator,factory,classPathRegistry,fileLookup,runtimeShadedJarFactory,currentGradleInstallation),new ClientModuleNotationParserFactory(instantiator).create(),projectDependencyFactory);
}
