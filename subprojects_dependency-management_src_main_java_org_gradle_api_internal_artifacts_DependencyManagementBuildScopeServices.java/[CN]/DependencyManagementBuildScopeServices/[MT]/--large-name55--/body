{
  ArtifactDependencyResolver resolver=new DefaultDependencyResolver(resolveIvyFactory,publishModuleDescriptorConverter,dependencyDescriptorFactory,new DefaultProjectComponentRegistry(publishModuleDescriptorConverter,projectRegistry),cacheLockingManager,ivyContextManager,resolutionResultsStoreFactory,versionComparator,startParameter.isBuildProjectDependencies());
  return new ErrorHandlingArtifactDependencyResolver(new ShortcircuitEmptyConfigsArtifactDependencyResolver(new SelfResolvingDependencyResolver(new CacheLockingArtifactDependencyResolver(cacheLockingManager,resolver)),componentIdentifierFactory));
}
