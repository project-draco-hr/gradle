{
  InputStream xmlStream=URLHandlerRegistry.getDefault().openStream(xmlURL);
  try {
    InputSource inSrc=new InputSource(xmlStream);
    inSrc.setSystemId(xmlURL.toExternalForm());
    parse(inSrc,schema,handler,lHandler);
  }
  finally {
    try {
      xmlStream.close();
    }
 catch (    IOException e) {
    }
  }
}
