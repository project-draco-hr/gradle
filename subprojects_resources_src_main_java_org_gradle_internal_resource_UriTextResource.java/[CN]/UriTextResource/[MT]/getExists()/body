{
  try {
    Reader reader=getInputStream(sourceUri);
    try {
      return true;
    }
  finally {
      reader.close();
    }
  }
 catch (  FileNotFoundException e) {
    return false;
  }
catch (  Exception e) {
    throw ResourceException.failure(sourceUri,String.format("Could not determine if %s exists.",getDisplayName()),e);
  }
}
