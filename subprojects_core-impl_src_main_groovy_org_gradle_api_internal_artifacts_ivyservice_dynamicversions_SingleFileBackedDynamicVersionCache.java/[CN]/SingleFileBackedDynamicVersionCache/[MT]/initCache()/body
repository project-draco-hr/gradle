{
  File dynamicRevisionsFile=new File(cacheMetadata.getCacheDir(),"dynamic-revisions.bin");
  FileLock dynamicRevisionsLock=cacheLockingManager.getCacheMetadataFileLock(dynamicRevisionsFile);
  return new BTreePersistentIndexedCache<RevisionKey,DynamicVersionCacheEntry>(dynamicRevisionsFile,dynamicRevisionsLock,new DefaultSerializer<DynamicVersionCacheEntry>(DynamicVersionCacheEntry.class.getClassLoader()));
}
