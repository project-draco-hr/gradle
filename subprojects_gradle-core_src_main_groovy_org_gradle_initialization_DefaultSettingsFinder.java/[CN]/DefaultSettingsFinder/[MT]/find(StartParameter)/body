{
  File settingsFile=null;
  for (  ISettingsFileSearchStrategy settingsFileSearchStrategy : settingsFileSearchStrategies) {
    settingsFile=settingsFileSearchStrategy.find(startParameter);
    if (settingsFile != null) {
      break;
    }
  }
  if (settingsFile == null) {
    return new SettingsLocation(startParameter.getCurrentDir(),new StringScriptSource("empty settings file",""));
  }
 else {
    return new SettingsLocation(settingsFile.getParentFile(),new FileScriptSource("settings file",settingsFile));
  }
}
