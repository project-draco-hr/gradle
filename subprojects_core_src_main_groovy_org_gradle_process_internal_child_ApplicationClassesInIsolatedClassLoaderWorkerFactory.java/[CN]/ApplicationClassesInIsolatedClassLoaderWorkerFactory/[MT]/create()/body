{
  List<URI> applicationClassPath=GFileUtils.toURIs(processBuilder.getApplicationClasspath());
  ActionExecutionWorker injectedWorker=new ActionExecutionWorker(processBuilder.getWorker(),workerId,displayName,serverAddress);
  ImplementationClassLoaderWorker worker=new ImplementationClassLoaderWorker(processBuilder.getLogLevel(),processBuilder.getSharedPackages(),implementationClassPath,injectedWorker);
  return new IsolatedApplicationClassLoaderWorker(applicationClassPath,worker);
}
