{
  Object current=path;
  while (current != null) {
    if (current instanceof Closure) {
      current=((Closure)current).call();
    }
 else     if (current instanceof Callable) {
      current=uncheckedCall((Callable)current);
    }
 else     if (current instanceof Factory) {
      return ((Factory)current).create();
    }
 else {
      return current;
    }
  }
  return null;
}
