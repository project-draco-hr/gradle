{
  String[] split=notation.toString().split(":");
  if (split.length != 3) {
    throw new InvalidDependencyFormat("Invalid format: '" + notation + "'. Correct notation is a 3-part group:name:version notation,"+ "e.g: org.gradle:gradle-core:1.0-milestone-3");
  }
  final String group=split[0];
  final String name=split[1];
  final String version=split[2];
  return identifier(group,name,version);
}
