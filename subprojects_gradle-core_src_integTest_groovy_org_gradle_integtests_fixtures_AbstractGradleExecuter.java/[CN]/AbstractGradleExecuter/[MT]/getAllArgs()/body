{
  List<String> allArgs=new ArrayList<String>();
  if (quiet) {
    allArgs.add("--quiet");
  }
  if (taskList) {
    allArgs.add("--tasks");
  }
  if (!searchUpwards) {
    allArgs.add("--no-search-upward");
  }
  if (userHomeDir != null) {
    args.add("--gradle-user-home");
    args.add(userHomeDir.getAbsolutePath());
  }
  allArgs.addAll(args);
  allArgs.addAll(tasks);
  return allArgs;
}
