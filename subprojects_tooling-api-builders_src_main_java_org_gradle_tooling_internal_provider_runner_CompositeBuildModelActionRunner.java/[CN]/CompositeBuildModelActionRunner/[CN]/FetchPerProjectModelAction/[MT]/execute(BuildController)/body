{
  Class<?> modelType;
  try {
    modelType=Class.forName(modelTypeName);
  }
 catch (  ClassNotFoundException e) {
    throw UncheckedException.throwAsUncheckedException(e);
  }
  final Map<Object,Object> results=new HashMap<Object,Object>();
