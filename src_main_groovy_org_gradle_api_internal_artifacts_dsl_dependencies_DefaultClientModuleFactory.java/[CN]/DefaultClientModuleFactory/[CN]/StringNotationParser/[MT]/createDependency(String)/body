{
  ParsedModuleStringNotation parsedNotation=new ParsedModuleStringNotation(notation,null);
  DefaultClientModule clientModule=new DefaultClientModule(parsedNotation.getGroup(),parsedNotation.getName(),parsedNotation.getVersion());
  ModuleFactoryHelper.addClassifierArtifactIfSet(parsedNotation.getClassifier(),clientModule);
  return clientModule;
}
