{
  ParsedModuleStringNotation parsedNotation=new ParsedModuleStringNotation(notation,null);
  DefaultClientModule clientModule=new DefaultClientModule(parsedNotation.getGroup(),parsedNotation.getName(),parsedNotation.getVersion());
  if (parsedNotation.getClassifier() != null) {
    clientModule.addArtifact(new DefaultDependencyArtifact(clientModule.getName(),DependencyArtifact.DEFAULT_TYPE,DependencyArtifact.DEFAULT_TYPE,parsedNotation.getClassifier(),null));
  }
  return clientModule;
}
