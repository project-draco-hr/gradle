{
  if (unfilteredFileSystemSubset.contains(file)) {
    return true;
  }
 else {
    String absolutePathWithSeparator=file.getAbsolutePath() + File.separator;
    for (    File root : roots) {
      if (root.equals(file) || root.getPath().startsWith(absolutePathWithSeparator)) {
        return true;
      }
    }
  }
  return false;
}
