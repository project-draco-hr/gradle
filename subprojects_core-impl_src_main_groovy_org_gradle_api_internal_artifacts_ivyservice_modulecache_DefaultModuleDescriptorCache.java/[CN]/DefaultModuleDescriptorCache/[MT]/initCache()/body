{
  File artifactResolutionCacheFile=new File(cacheMetadata.getCacheDir(),"module-metadata.bin");
  FileLock artifactResolutionCacheLock=cacheLockingManager.getCacheMetadataFileLock(artifactResolutionCacheFile);
  return new BTreePersistentIndexedCache<RevisionKey,ModuleDescriptorCacheEntry>(artifactResolutionCacheFile,artifactResolutionCacheLock,new DefaultSerializer<ModuleDescriptorCacheEntry>(ModuleDescriptorCacheEntry.class.getClassLoader()));
}
