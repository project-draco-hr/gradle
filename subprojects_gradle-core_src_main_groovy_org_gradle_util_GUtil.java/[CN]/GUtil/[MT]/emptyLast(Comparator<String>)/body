{
  return new Comparator<String>(){
    @Override public int compare(    String o1,    String o2){
      boolean o1Empty=o1 == null || o1.isEmpty();
      boolean o2Empty=o2 == null || o2.isEmpty();
      if (o1Empty && o2Empty) {
        return 0;
      }
      if (o1Empty && !o2Empty) {
        return 1;
      }
      if (!o1Empty && o2Empty) {
        return -1;
      }
      return comparator.compare(o1,o2);
    }
  }
;
}
