{
  List<String> split=Lists.newArrayList(Splitter.onPattern("\\s(?=([^\"]*\"[^\"]*\")*[^\"]*$)").omitEmptyStrings().split(input));
  List<String> out=new ArrayList<String>();
  for (  String s : split) {
    if ((s.startsWith("-D") || s.startsWith("\"-D")) && s.endsWith("\"")) {
      out.add(s.replaceFirst("\"","").substring(0,s.length() - 2));
    }
 else {
      out.add(s);
    }
  }
  return out;
}
