{
  if (includePatterns.isEmpty()) {
    return true;
  }
  String fullName=className + "." + methodName;
  for (  Pattern pattern : includePatterns) {
    if (methodName != null && pattern.matcher(fullName).matches()) {
      return true;
    }
    if (pattern.matcher(className).matches()) {
      return true;
    }
  }
  return false;
}
