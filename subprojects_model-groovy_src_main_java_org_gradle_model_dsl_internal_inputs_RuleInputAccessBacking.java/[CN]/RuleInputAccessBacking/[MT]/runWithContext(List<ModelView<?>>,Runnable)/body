{
  ImmutableMap.Builder<String,Object> builder=ImmutableMap.builder();
  int i=0;
  for (  ModelView<?> view : views) {
    builder.put(view.getPath().toString(),views.get(i++).getInstance());
  }
  ImmutableMap<String,Object> inputsMap=builder.build();
  INPUT.set(inputsMap);
  try {
    runnable.run();
  }
  finally {
    INPUT.remove();
  }
}
