{
  final TestFile tarFile=initializeTarFile(tmpDir.getTestDirectory().file("test.tar"),new SimpleCompressor());
  tar(dir("dir"),file("file"));
  Map<String,Integer> expected=new HashMap<String,Integer>();
  expected.put("dir",2);
  expected.put("file",1);
  assertVisitsPermissions(new TarFileTree(tarFile,new FileResource(tarFile),null,fileSystem()),expected);
}
