{
  int pos=startIndex;
  for (int i=0; pos < segments.length && i < steps.size(); i++, pos++) {
    PatternStep step=steps.get(i);
    if (!step.matches(segments[pos],isFile)) {
      return false;
    }
  }
  if (pos == segments.length) {
    return true;
  }
  return next.isPrefix(segments,pos,isFile);
}
