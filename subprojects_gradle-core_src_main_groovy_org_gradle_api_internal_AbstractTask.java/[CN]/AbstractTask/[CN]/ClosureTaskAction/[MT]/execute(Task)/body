{
  try {
    if (closure.getMaximumNumberOfParameters() == 0) {
      closure.call();
    }
 else {
      closure.call(task);
    }
  }
 catch (  InvokerInvocationException e) {
    Throwable cause=e.getCause();
    if (cause instanceof RuntimeException) {
      throw (RuntimeException)cause;
    }
    throw e;
  }
}
