{
  List<File> existingSourceDirs=existentDirsFilter.checkDestDirAndFindExistingDirsAndThrowStopActionIfNone(getDestinationDir(),getSrcDirs());
  options.sourcepath(existingSourceDirs).directory(getDestinationDir()).classpath(getClasspath()).classpath((File)getProject().getConvention().getProperty("classesDir")).windowTitle("\"" + getTitle() + "\"").exclude("**/package.html");
  if (maxMemory != null)   options.jFlags("-Xmx" + maxMemory);
  executeExternalJavadoc();
}
