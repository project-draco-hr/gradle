{
  lock.lock();
  try {
    ClassLoaderDetails details=classLoaderDetails.get(classLoader);
    if (details != null) {
      return details;
    }
    details=factory.transform(classLoader);
    classLoaderDetails.put(classLoader,details);
    classLoaderIds.put(details.uuid,classLoader);
    return details;
  }
  finally {
    lock.unlock();
  }
}
