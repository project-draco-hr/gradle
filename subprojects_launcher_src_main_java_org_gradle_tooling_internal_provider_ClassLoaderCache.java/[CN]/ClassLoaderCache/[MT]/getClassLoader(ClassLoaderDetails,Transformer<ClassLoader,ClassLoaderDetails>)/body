{
  lock.lock();
  try {
    ClassLoader classLoader=classLoaderIds.get(details.uuid);
    if (classLoader != null) {
      return classLoader;
    }
    classLoader=factory.transform(details);
    classLoaderIds.put(details.uuid,classLoader);
    classLoaderDetails.put(classLoader,details);
    return classLoader;
  }
  finally {
    lock.unlock();
  }
}
