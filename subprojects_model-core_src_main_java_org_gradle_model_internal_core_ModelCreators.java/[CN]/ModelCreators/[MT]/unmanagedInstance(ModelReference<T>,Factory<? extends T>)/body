{
  Transformer<? extends Action<ModelNode>,Object> initializer=Transformers.toTransformer(Factories.constant(new Action<ModelNode>(){
    public void execute(    ModelNode modelNode){
      modelNode.setPrivateData(modelReference.getType(),factory.create());
    }
  }
));
  return of(modelReference,initializer).withProjection(new UnmanagedModelProjection<T>(modelReference.getType(),true,true));
}
