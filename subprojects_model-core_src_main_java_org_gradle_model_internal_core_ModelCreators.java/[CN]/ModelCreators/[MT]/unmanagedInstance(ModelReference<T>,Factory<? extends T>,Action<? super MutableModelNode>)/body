{
  @SuppressWarnings("unchecked") Action<? super MutableModelNode> initializers=Actions.composite(new Action<MutableModelNode>(){
    public void execute(    MutableModelNode modelNode){
      modelNode.setPrivateData(modelReference.getType(),factory.create());
    }
  }
,initializer);
  return of(modelReference,initializers).withProjection(new UnmanagedModelProjection<T>(modelReference.getType(),true,true));
}
