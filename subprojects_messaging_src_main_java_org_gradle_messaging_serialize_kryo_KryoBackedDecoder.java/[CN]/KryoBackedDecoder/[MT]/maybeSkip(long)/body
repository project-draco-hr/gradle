{
  int remaining=input.limit() - input.position();
  if (remaining == 0) {
    return inputStream.skip(count);
  }
 else   if (count <= remaining) {
    input.setPosition(input.position() + (int)count);
    return count;
  }
 else {
    input.setPosition(input.limit());
    return remaining;
  }
}
