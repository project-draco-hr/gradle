{
  getPom().addDependencies(configurations);
  try {
    pomFile.getParentFile().mkdirs();
    FileWriter writer=new FileWriter(pomFile);
    try {
      getPom().write(writer);
    }
  finally {
      writer.close();
    }
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
