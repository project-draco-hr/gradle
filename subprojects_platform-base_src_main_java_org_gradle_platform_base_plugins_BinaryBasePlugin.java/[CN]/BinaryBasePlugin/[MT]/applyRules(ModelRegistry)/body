{
  SimpleModelRuleDescriptor ruleDescriptor=new SimpleModelRuleDescriptor(BinaryBasePlugin.class.getSimpleName() + ".apply()");
  modelRegistry.configure(ModelActionRole.Defaults,DirectNodeNoInputsModelAction.of(ModelReference.of("binaries"),ruleDescriptor,new Action<MutableModelNode>(){
    @Override public void execute(    MutableModelNode binariesNode){
      binariesNode.applyTo(allLinks(),BaseBinaryRules.class);
    }
  }
));
  modelRegistry.getRoot().applyTo(allDescendants(),ModelActionRole.Defaults,DirectNodeNoInputsModelAction.of(ModelReference.of(BinarySpec.class),ruleDescriptor,ComponentSpecInitializer.binaryAction()));
}
