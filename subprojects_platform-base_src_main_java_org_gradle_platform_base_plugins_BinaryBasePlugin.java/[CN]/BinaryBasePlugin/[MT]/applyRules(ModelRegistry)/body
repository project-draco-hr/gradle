{
  SimpleModelRuleDescriptor ruleDescriptor=new SimpleModelRuleDescriptor(BinaryBasePlugin.class.getSimpleName() + ".apply()");
  modelRegistry.getRoot().applyTo(allDescendants(ModelNodes.withType(BinarySpec.class)),ModelActionRole.Defaults,DirectNodeNoInputsModelAction.of(ModelReference.of(BinarySpec.class),ruleDescriptor,new Action<MutableModelNode>(){
    @Override public void execute(    MutableModelNode binaryNode){
      if (binaryNode instanceof ModelReferenceNode) {
        return;
      }
      binaryNode.applyToSelf(BaseBinaryRules.class);
    }
  }
));
}
