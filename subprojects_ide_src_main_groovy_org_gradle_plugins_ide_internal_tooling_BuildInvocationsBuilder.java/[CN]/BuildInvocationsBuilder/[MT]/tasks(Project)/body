{
  List<LaunchableGradleTask> tasks=Lists.newArrayList();
  for (  Task task : taskLister.listProjectTasks(project,true)) {
    tasks.add(new LaunchableGradleTask().setPath(task.getPath()).setName(task.getName()).setDisplayName(task.toString()).setDescription(task.getDescription()));
  }
  return tasks;
}
