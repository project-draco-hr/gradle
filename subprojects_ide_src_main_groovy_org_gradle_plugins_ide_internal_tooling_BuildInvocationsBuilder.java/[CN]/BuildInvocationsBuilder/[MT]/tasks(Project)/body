{
  List<LaunchableGradleTask> tasks=Lists.newArrayList();
  for (  Task task : taskLister.listProjectTasks(project)) {
    tasks.add(new LaunchableGradleTask().setPath(task.getPath()).setName(task.getName()).setDisplayName(task.toString()).setDescription(task.getDescription()).setPublic(task.getGroup() != null));
  }
  return tasks;
}
