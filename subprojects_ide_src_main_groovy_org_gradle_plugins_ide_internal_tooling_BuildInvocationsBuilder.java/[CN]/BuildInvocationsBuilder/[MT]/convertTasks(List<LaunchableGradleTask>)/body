{
  List<LaunchableGradleTask> convertedTasks=Lists.newArrayList();
  for (  LaunchableGradleTask task : tasks) {
    convertedTasks.add(new LaunchableGradleTask().setPath(task.getPath()).setName(task.getName()).setDisplayName(task.toString()).setDescription(task.getDescription()));
  }
  return convertedTasks;
}
