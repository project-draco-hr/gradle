{
  if (index >= executionActions.size()) {
    return;
  }
  executionActions.get(index).execute(new BuildExecutionContext(){
    public GradleInternal getGradle(){
      return gradle;
    }
    public void proceed(){
      execute(index + 1);
    }
    public BuildCancellationToken getCancellationToken(){
      return cancellationToken;
    }
  }
);
}
