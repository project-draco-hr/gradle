{
  Writer writer;
  File cssFile=new File(reportDir,resourceName);
  writer=new BufferedWriter(new FileWriter(cssFile));
  try {
    InputStream cssResource=getClass().getResourceAsStream(resourceName);
    try {
      IOUtils.copy(cssResource,writer);
    }
  finally {
      cssResource.close();
    }
  }
  finally {
    writer.close();
  }
}
