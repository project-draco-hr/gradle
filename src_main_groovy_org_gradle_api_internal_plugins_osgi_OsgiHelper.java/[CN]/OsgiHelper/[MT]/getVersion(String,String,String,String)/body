{
  StringBuffer sb=new StringBuffer();
  sb.append(major != null ? major : "0");
  sb.append('.');
  sb.append(minor != null ? minor : "0");
  sb.append('.');
  sb.append(service != null ? service : "0");
  if (qualifier != null) {
    sb.append('.');
    sb.append(qualifier);
  }
  return sb.toString();
}
