{
  collector.args("/c");
  collector.args(spec.getArgs());
  collector.args("/nologo");
  collector.args("/EHsc");
  if (spec.isForDynamicLinking()) {
    collector.args("/LD");
  }
  for (  File file : spec.getIncludeRoots()) {
    collector.args("/I",file.getAbsolutePath());
  }
  for (  File file : spec.getSource()) {
    collector.args(file);
  }
}
