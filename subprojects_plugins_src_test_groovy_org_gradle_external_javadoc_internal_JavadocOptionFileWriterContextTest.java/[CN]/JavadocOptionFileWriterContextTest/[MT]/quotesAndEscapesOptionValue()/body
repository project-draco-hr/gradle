{
  context.checking(new Expectations(){
{
      one(bufferedWriterMock).write("-");
      one(bufferedWriterMock).write("key");
      one(bufferedWriterMock).write(" ");
      one(bufferedWriterMock).write("'");
      one(bufferedWriterMock).write("1\\\\2\\\\");
      one(bufferedWriterMock).write("'");
      one(bufferedWriterMock).newLine();
    }
  }
);
  writerContext.writeValueOption("key","1\\2\\");
}
