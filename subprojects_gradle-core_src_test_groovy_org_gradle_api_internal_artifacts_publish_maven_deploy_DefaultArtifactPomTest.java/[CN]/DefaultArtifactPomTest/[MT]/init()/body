{
  assertSame(testPom,artifactPom.getPom());
  assertEquals(expectedArtifact,artifactPom.getArtifact());
  assertEquals(expectedFile,artifactPom.getArtifactFile());
  checkPom(expectedArtifact.getModuleRevisionId().getOrganisation(),expectedArtifact.getName(),expectedArtifact.getType(),expectedArtifact.getModuleRevisionId().getRevision(),expectedArtifact.getExtraAttribute(Dependency.CLASSIFIER));
}
