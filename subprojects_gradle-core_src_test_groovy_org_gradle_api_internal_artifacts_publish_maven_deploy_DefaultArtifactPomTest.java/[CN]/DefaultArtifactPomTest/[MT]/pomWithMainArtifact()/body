{
  artifactPom.addArtifact(expectedArtifact,expectedFile);
  assertEquals(expectedArtifact,artifactPom.getArtifact());
  assertEquals(expectedFile,artifactPom.getArtifactFile());
  checkPom(expectedArtifact.getModuleRevisionId().getOrganisation(),expectedArtifact.getName(),expectedArtifact.getType(),expectedArtifact.getModuleRevisionId().getRevision(),null);
}
