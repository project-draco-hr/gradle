{
  ArchitectureInternal architecture=(ArchitectureInternal)targetPlatform.getArchitecture();
  if (architecture == ArchitectureInternal.TOOL_CHAIN_DEFAULT) {
    return args();
  }
  if (architecture.isI386()) {
    return args("-m32");
  }
  if (architecture.isAmd64()) {
    return args("-m64");
  }
  return args();
}
