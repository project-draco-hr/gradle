{
  statusLock.lock();
  try {
    TestClassRunInfo nextTest=null;
switch (status) {
case FAILED:
      nextTest=currentTest;
    break;
case STARTED:
  try {
    nextTest=pipelineDispatcher.nextTest();
  }
 catch (  InterruptedException e) {
  }
break;
}
if (pipeline.isPipelineSplittingEnded() && pipelineDispatcher.isAllTestsExecuted()) pipelineDispatcher.stop();
return nextTest;
}
  finally {
statusLock.unlock();
}
}
