{
  statusLock.lock();
  try {
    if (status != ForkStatus.STARTED)     throw new IllegalArgumentException("can't change status to RESTARTING, current status is " + status);
    status=ForkStatus.RESTARTING;
    forkControl.setRestarting(pipeline.getId(),forkId,true);
  }
  finally {
    statusLock.unlock();
  }
}
