{
  if (clients.isEmpty()) {
    registerStopOnBuildFinished(project);
  }
  for (  CompilerDaemonClient client : clients) {
    if (client.isCompatibleWith(forkOptions)) {
      return client;
    }
  }
  CompilerDaemonClient client=compilerDaemonStarter.startDaemon(project,forkOptions);
  clients.add(client);
  return client;
}
