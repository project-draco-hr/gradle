{
  if (firstRequest) {
    registerStopOnBuildFinished(project);
    firstRequest=false;
  }
  for (  CompilerDaemonClient client : clients) {
    if (client.isCompatibleWith(forkOptions)) {
      return client;
    }
  }
  CompilerDaemonClient client=startDaemon(project,forkOptions);
  clients.add(client);
  return client;
}
