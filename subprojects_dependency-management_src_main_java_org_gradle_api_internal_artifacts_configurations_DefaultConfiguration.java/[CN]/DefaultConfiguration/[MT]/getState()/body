{
synchronized (lock) {
    if (state == InternalState.RESULTS_RESOLVED || state == InternalState.TASK_DEPENDENCIES_RESOLVED) {
      if (resolvedWithFailures) {
        return State.RESOLVED_WITH_FAILURES;
      }
 else {
        return State.RESOLVED;
      }
    }
 else {
      return State.UNRESOLVED;
    }
  }
}
