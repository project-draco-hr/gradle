{
  context.checking(new Expectations(){
{
      allowing(ivyServiceStub).resolveAsReport(configuration,dependencyMetaDataProvider.getModule(),dependencyMetaDataProvider.getGradleUserHomeDir(),dependencyMetaDataProvider.getClientModuleRegistry());
      will(returnValue(resolveReport));
      allowing(resolveReport).hasError();
      will(returnValue(withErrors));
    }
  }
);
}
