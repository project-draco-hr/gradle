{
  ProgressListenerAdapter progressListenerAdapter=new ProgressListenerAdapter(this.progressListeners);
  BuildProgressListenerConfiguration buildProgressListenerConfiguration=new BuildProgressListenerConfiguration(this.testProgressListeners,this.taskProgressListeners,this.buildOperationProgressListeners);
  FailsafeBuildProgressListenerAdapter buildProgressListenerAdapter=new FailsafeBuildProgressListenerAdapter(new BuildProgressListenerAdapter(buildProgressListenerConfiguration));
  return new ConsumerOperationParameters(parameters,stdout,stderr,colorOutput,stdin,javaHome,jvmArguments,arguments,tasks,launchables,progressListenerAdapter,buildProgressListenerAdapter,cancellationToken);
}
