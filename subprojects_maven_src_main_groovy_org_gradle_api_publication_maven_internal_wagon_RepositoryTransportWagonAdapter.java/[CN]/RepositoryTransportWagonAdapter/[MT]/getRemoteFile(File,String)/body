{
  URI uriForResource=getUriForResource(resourceName);
  try {
    ExternalResource resource=transport.getRepository().getResource(uriForResource);
    if (resource == null) {
      return false;
    }
    try {
      resource.writeTo(destination);
    }
  finally {
      resource.close();
    }
    return true;
  }
 catch (  Exception e) {
    LOGGER.debug("Could get and write file:",e);
    return false;
  }
}
