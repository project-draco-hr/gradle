{
  TaskWithOutputFiles task=expectTaskCreated(TaskWithOutputFiles.class,Collections.singletonList(new File(testDir,"subdir/output.txt")));
  GFileUtils.touch(task.outputFiles.get(0).getParentFile());
  assertValidationFails(task,String.format("Cannot write to file '%s' specified for property 'outputFiles', as ancestor '%s' is not a directory.",task.outputFiles.get(0),task.outputFiles.get(0).getParentFile()));
}
