{
  TaskWithOutputFile task=expectTaskCreated(TaskWithOutputFile.class,new File(testDir,"subdir/output.txt"));
  GFileUtils.touch(task.outputFile.getParentFile());
  assertValidationFails(task,String.format("Cannot write to file '%s' specified for property 'outputFile', as ancestor '%s' is not a directory.",task.getOutputFile(),task.outputFile.getParentFile()));
}
