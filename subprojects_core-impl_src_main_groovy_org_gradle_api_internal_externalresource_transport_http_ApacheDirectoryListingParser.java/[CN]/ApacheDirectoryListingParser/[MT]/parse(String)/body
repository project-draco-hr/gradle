{
  List<URL> urlList=new ArrayList<URL>();
  Matcher matcher=PATTERN.matcher(htmlText);
  while (matcher.find()) {
    String href=matcher.group(1);
    String text=matcher.group(2);
    text=text.trim();
    URL child=parseLink(href,text);
    if (child != null) {
      urlList.add(child);
      LOGGER.debug("found URL=[" + child + "].");
    }
  }
  return urlList;
}
