{
  if (href != null && (href.startsWith("http:") || href.startsWith("https:"))) {
    try {
      href=new URL(href).getPath();
      if (!href.startsWith(inputUrl.getPath())) {
        return null;
      }
      return href.substring(inputUrl.getPath().length());
    }
 catch (    Exception ignore) {
      return null;
    }
  }
  return href;
}
