{
  StringBuilder result=new StringBuilder();
  if (description != null) {
    result.append(description);
  }
  if (deprecationWarning != null) {
    if (result.length() > 0) {
      result.append(' ');
    }
    result.append("[deprecated - ");
    result.append(deprecationWarning);
    result.append("]");
  }
  if (experimental) {
    if (result.length() > 0) {
      result.append(' ');
    }
    result.append("[experimental]");
  }
  return result.toString();
}
