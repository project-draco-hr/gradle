{
  List<File> groovyImpl=classPathRegistry.getClassPath(LOCAL_GROOVY.name()).getAsFiles();
  apiClasspath.removeAll(groovyImpl);
  return (FileCollectionInternal)relocatedDepsJar(apiClasspath,"gradleApi()","api").plus(fileResolver.resolveFiles(groovyImpl,classPathRegistry.getClassPath("GRADLE_INSTALLATION_BEACON").getAsFiles()));
}
