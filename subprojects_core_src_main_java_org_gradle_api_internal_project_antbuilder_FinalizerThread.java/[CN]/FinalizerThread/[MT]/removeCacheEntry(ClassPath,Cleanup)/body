{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Removing classloader from cache, classpath = {}",key.getAsURIs());
  }
  lock.lock();
  try {
    cacheEntries.remove(key);
    cleanups.remove(key);
  }
  finally {
    lock.unlock();
  }
  try {
    entry.cleanup();
    entry.clear();
  }
 catch (  Exception ex) {
    LOG.error("Unable to perform cleanup of classloader for classpath: " + key,ex);
  }
}
