{
  int handle=openKey(subkey);
  try {
    int[] info=queryInfoKey(handle);
    int count=info[0];
    int maxLength=info[3];
    List<String> subkeys=new ArrayList<String>(count);
    for (int i=0; i != count; ++i) {
      subkeys.add(fromCString(enumKeyEx(handle,i,maxLength + 1)));
    }
    return subkeys;
  }
  finally {
    closeKey(handle);
  }
}
