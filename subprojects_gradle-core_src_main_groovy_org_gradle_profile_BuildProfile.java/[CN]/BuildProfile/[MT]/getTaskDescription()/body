{
  StringBuilder result=new StringBuilder();
  for (  String name : gradle.getStartParameter().getExcludedTaskNames()) {
    result.append("-x");
    result.append(name);
    result.append(" ");
  }
  for (  String name : gradle.getStartParameter().getTaskNames()) {
    result.append(name);
    result.append(" ");
  }
  return result.toString();
}
