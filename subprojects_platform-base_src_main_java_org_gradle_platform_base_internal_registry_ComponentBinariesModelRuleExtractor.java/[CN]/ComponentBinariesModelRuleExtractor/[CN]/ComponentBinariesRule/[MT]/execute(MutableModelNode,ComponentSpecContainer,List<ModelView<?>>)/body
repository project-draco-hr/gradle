{
  modelNode.applyToAllLinks(ModelActionRole.Mutate,DirectNodeInputUsingModelAction.of(ModelReference.of(ModelType.of(componentType)),getDescriptor(),getInputs(),new TriAction<MutableModelNode,C,List<ModelView<?>>>(){
    @Override public void execute(    MutableModelNode componentModelNode,    C component,    final List<ModelView<?>> componentRuleInputs){
      invoke(componentRuleInputs,component.getBinaries().withType(binaryType),component);
    }
  }
));
}
