{
  return readFromFile(new Callable<Boolean>(){
    public Boolean call() throws Exception {
      try {
        lockFileAccess.seek(STATE_REGION_POS + 1);
        if (!lockFileAccess.readBoolean()) {
          return false;
        }
      }
 catch (      EOFException e) {
        return false;
      }
      return true;
    }
  }
);
}
