{
  String taskName="someTaskName";
  assertThat(new ProjectDependenciesBuildInstruction(null).hashCode(),equalTo(new ProjectDependenciesBuildInstruction(null).hashCode()));
  assertThat(new ProjectDependenciesBuildInstruction(Collections.<String>emptyList()).hashCode(),equalTo(new ProjectDependenciesBuildInstruction(Collections.<String>emptyList()).hashCode()));
  assertThat(new ProjectDependenciesBuildInstruction(WrapUtil.toList(taskName)).hashCode(),equalTo(new ProjectDependenciesBuildInstruction(WrapUtil.toList(taskName)).hashCode()));
}
