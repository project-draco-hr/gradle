{
  StringBuffer sb=new StringBuffer();
  for (  Project project : tasks.keySet()) {
    sb.append(SEPARATOR);
    sb.append("Project: " + project + NL);
    for (    Task task : tasks.get(project)) {
      sb.append("++Task: " + task.getPath() + ": "+ task.getDependsOn()+ NL);
    }
  }
  return sb.toString();
}
