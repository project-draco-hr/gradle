{
  for (  String masterConf : dependency.getDependencyConfigurationMappings().getMappings().keySet()) {
    if (!masterConf.equals("*")) {
      assertEquals(GUtil.addLists(dependency.getDependencyConfigurationMappings().getMappings().get(masterConf),WrapUtil.toList(wildcardConf)),Arrays.asList(dependencyDescriptor.getDependencyConfigurations(masterConf)));
    }
  }
}
