{
  if (configurationContainer == null) {
    final Instantiator instantiator=parent.get(Instantiator.class);
    ConfigurationResolver resolver=createDependencyResolver(getResolveRepositoryHandler());
    configurationContainer=instantiator.newInstance(DefaultConfigurationContainer.class,resolver,instantiator,domainObjectContext,parent.get(ListenerManager.class),dependencyMetaDataProvider);
  }
  return configurationContainer;
}
