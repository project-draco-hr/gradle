{
  PublishModuleDescriptorConverter fileModuleDescriptorConverter=new PublishModuleDescriptorConverter(get(ResolveModuleDescriptorConverter.class),new DefaultArtifactsToModuleDescriptorConverter(DefaultArtifactsToModuleDescriptorConverter.IVY_FILE_STRATEGY));
  return new ErrorHandlingArtifactPublisher(new IvyBackedArtifactPublisher(resolverProvider,get(SettingsConverter.class),get(PublishModuleDescriptorConverter.class),fileModuleDescriptorConverter,get(IvyFactory.class),new DefaultIvyDependencyPublisher(new DefaultPublishOptionsFactory())));
}
