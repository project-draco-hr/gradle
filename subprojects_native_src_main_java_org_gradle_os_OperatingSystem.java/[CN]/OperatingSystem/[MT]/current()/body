{
  String osName=System.getProperty("os.name").toLowerCase();
  if (osName.contains("windows")) {
    return WINDOWS;
  }
 else   if (osName.contains("mac os x") || osName.contains("darwin")) {
    return MAC_OS;
  }
 else   if (osName.contains("sunos")) {
    return SOLARIS;
  }
 else   if (osName.contains("linux")) {
    return LINUX;
  }
 else {
    return UNIX;
  }
}
