{
  Properties wrapperProperties=new Properties();
  wrapperProperties.load(getClass().getResourceAsStream("/org/gradle/wrapper/wrapper.properties"));
  String version=(String)wrapperProperties.get(DISTRIBUTION_VERSION_PROPERTY);
  String gradleHome=install.createDist((String)wrapperProperties.get(URL_ROOT_PROPERTY),(String)wrapperProperties.get(DISTRIBUTION_BASE_PROPERTY),(String)wrapperProperties.get(DISTRIBUTION_PATH_PROPERTY),(String)wrapperProperties.get(DISTRIBUTION_NAME_PROPERTY),version,(String)wrapperProperties.get(ZIP_STORE_BASE_PROPERTY),(String)wrapperProperties.get(ZIP_STORE_PATH_PROPERTY));
  bootstrapMainStarter.start(args,gradleHome,version);
}
