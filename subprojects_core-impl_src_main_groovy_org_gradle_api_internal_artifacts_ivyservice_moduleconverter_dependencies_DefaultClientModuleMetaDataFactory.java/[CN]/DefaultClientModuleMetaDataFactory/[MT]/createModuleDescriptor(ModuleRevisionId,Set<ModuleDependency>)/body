{
  DefaultModuleDescriptor moduleDescriptor=new DefaultModuleDescriptor(moduleRevisionId,"release",null);
  moduleDescriptor.addConfiguration(new Configuration(Dependency.DEFAULT_CONFIGURATION));
  addDependencyDescriptors(moduleDescriptor,dependencies,dependencyDescriptorFactory);
  moduleDescriptor.addArtifact(Dependency.DEFAULT_CONFIGURATION,new DefaultArtifact(moduleRevisionId,null,moduleRevisionId.getName(),"jar","jar"));
  return new DefaultIvyModuleVersionMetaData(moduleDescriptor);
}
