{
  return new AbstractModelAction<T>(reference,descriptor,Arrays.<ModelReference<?>>asList(input1,input2)){
    @Override public void execute(    MutableModelNode modelNode,    List<ModelView<?>> inputs){
      action.execute(modelNode,Cast.<I1>uncheckedCast(inputs.get(0).getInstance()),Cast.<I2>uncheckedCast(inputs.get(1).getInstance()));
