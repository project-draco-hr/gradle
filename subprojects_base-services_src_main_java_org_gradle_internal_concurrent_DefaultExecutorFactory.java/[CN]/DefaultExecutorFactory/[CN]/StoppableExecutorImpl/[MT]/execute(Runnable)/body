{
  executor.execute(new Runnable(){
    public void run(){
      executing.set(command);
      try {
        command.run();
      }
 catch (      Throwable throwable) {
        if (!failure.compareAndSet(null,throwable)) {
          LOGGER.error(String.format("Failed to execute %s.",command),throwable);
        }
      }
 finally {
        executing.set(null);
      }
    }
  }
);
}
