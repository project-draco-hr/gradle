{
  BinaryContainer binaries=extensions.getByType(BinaryContainer.class);
  binaries.registerFactory(type,new NamedDomainObjectFactory<V>(){
    public V create(    String name){
      BinaryNamingScheme binaryNamingScheme=new DefaultBinaryNamingSchemeBuilder().withComponentName(name).build();
      @SuppressWarnings("unchecked") V created=(V)BaseBinarySpec.create(implementation,binaryNamingScheme,instantiator);
      return created;
    }
  }
);
}
