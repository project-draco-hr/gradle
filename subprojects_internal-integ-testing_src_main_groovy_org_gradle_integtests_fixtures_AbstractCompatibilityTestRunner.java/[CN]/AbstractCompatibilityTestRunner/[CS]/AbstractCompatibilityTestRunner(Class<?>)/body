{
  this.target=target;
  previous=new ArrayList<BasicGradleDistribution>();
  for (  String version : Arrays.asList("0.8","0.9-rc-3","0.9","0.9.1","0.9.2","1.0-milestone-1","1.0-milestone-2","1.0-milestone-3","1.0-milestone-4","1.0-milestone-5")) {
    previous.add(current.previousVersion(version));
  }
}
