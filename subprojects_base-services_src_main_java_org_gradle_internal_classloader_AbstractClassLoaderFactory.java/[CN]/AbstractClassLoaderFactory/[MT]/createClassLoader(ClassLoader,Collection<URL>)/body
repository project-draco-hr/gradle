{
  List<File> files=Lists.newArrayListWithCapacity(urls.size());
  for (  URL url : urls) {
    if (!url.getProtocol().equals("file")) {
      return new VisitableURLClassLoader(parent,urls);
    }
    try {
      files.add(new File(url.toURI()));
    }
 catch (    URISyntaxException e) {
      throw new IllegalArgumentException(e);
    }
  }
  return doCreateClassLoader(parent,new DefaultClassPath(files));
}
