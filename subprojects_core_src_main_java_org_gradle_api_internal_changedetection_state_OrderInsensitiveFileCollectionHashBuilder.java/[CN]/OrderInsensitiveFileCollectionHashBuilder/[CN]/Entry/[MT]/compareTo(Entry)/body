{
  int result=key.compareTo(o.key);
  if (result == 0) {
    result=hashCode.length - o.hashCode.length;
    if (result == 0) {
      for (int idx=0; idx < hashCode.length; idx++) {
        result=hashCode[idx] - o.hashCode[idx];
        if (result != 0) {
          break;
        }
      }
    }
  }
  return result;
}
