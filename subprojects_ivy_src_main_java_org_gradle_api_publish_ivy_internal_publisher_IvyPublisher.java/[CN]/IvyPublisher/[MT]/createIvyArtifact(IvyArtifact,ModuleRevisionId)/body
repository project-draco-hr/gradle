{
  Map<String,String> extraAttributes=new HashMap<String,String>();
  if (GUtil.isTrue(ivyArtifact.getClassifier())) {
    extraAttributes.put(Dependency.CLASSIFIER,ivyArtifact.getClassifier());
  }
  return new DefaultArtifact(moduleRevisionId,null,ivyArtifact.getName(),ivyArtifact.getType(),ivyArtifact.getExtension(),extraAttributes);
}
