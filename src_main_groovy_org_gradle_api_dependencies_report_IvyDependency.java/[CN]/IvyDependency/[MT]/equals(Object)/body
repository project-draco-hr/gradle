{
  if (o instanceof IvyDependency) {
    if (((IvyDependency)o).getOrganisation().equals(getOrganisation())) {
      if (((IvyDependency)o).getName().equals(getName())) {
        if (((IvyDependency)o).getRev().equals(getRev())) {
          return true;
        }
      }
    }
  }
  return false;
}
