{
  if (metaData == null) {
    return TYPE_MISSING;
  }
  if (metaData instanceof IvyModuleVersionMetaData) {
    return TYPE_IVY;
  }
  if (metaData instanceof MavenModuleVersionMetaData) {
    return TYPE_MAVEN;
  }
  throw new IllegalArgumentException("Not a valid module version type: " + metaData);
}
