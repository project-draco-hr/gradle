{
  try {
    buildExecuter.hasTask(":a");
    fail();
  }
 catch (  IllegalStateException e) {
    assertThat(e.getMessage(),equalTo("Task information is not available, as this task execution graph has not been populated."));
  }
  try {
    buildExecuter.getAllTasks();
    fail();
  }
 catch (  IllegalStateException e) {
    assertThat(e.getMessage(),equalTo("Task information is not available, as this task execution graph has not been populated."));
  }
}
