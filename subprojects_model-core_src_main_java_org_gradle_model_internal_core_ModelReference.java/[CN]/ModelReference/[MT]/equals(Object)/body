{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ModelReference<?> that=(ModelReference<?>)o;
  if (path == null) {
    if (that.path == null) {
      return type.equals(that.type);
    }
    return false;
  }
  return path.equals(that.path) && type.equals(that.type);
}
