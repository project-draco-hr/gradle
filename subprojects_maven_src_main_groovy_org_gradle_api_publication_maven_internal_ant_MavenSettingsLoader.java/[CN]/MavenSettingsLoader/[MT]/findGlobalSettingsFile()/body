{
  List<String> env=Execute.getProcEnvironment();
  for (  String var : env) {
    if (var.startsWith("M2_HOME=")) {
      String m2Home=var.substring("M2_HOME=".length());
      File candidate=newFile(m2Home,"conf","settings.xml");
      if (candidate.exists()) {
        return candidate;
      }
      break;
    }
  }
  return null;
}
