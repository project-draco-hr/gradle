{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ArchiveEntry that=(ArchiveEntry)o;
  if (crc != that.crc) {
    return false;
  }
  if (directory != that.directory) {
    return false;
  }
  if (size != that.size) {
    return false;
  }
  if (path != null ? !path.equals(that.path) : that.path != null) {
    return false;
  }
  return true;
}
