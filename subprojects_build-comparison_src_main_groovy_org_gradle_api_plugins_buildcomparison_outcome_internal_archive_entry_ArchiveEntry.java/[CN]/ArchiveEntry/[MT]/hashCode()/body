{
  int result=path != null ? path.hashCode() : 0;
  result=31 * result + (directory ? 1 : 0);
  if (subEntries == null) {
    result=31 * result + (int)(size ^ (size >>> 32));
    result=31 * result + (int)(crc ^ (crc >>> 32));
  }
  result=31 * result + (subEntries != null ? subEntries.hashCode() : 0);
  return result;
}
