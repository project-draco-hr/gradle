{
  List<LaunchableGradleTask> out=new LinkedList<LaunchableGradleTask>();
  for (  Task t : tasks) {
    out.add(new LaunchableGradleProjectTask().setProject(owner).setPath(t.getPath()).setName(t.getName()).setDisplayName(t.toString()).setDescription(t.getDescription()).setPublic(PublicTaskSpecification.INSTANCE.isSatisfiedBy(t)));
  }
  return out;
}
