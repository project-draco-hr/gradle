{
  List<DefaultGradleTask> out=new LinkedList<DefaultGradleTask>();
  for (  Task t : tasks) {
    out.add(new DefaultGradleTask().setPath(t.getPath()).setName(t.getName()).setDisplayName(t.getName() + " task (" + t.getPath()+ ")").setDescription(t.getDescription()).setProject(owner));
  }
  return out;
}
