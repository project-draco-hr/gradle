{
  DefaultProjectOutput projectOutput=new DefaultProjectOutput(project.getName(),project.getPath(),project.getDescription(),project.getProjectDir(),project.getGradle().getGradleVersion(),getArchives(project),getTestRuns(project),parent);
  for (  Project child : project.getChildProjects().values()) {
    projectOutput.addChild(buildProjectOutput(child,projectOutput));
  }
  return projectOutput;
}
