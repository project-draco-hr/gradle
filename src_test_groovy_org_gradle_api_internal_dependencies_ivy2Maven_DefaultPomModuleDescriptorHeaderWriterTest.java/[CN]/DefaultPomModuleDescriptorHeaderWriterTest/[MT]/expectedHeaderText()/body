{
  GradleVersion gradleVersion=new GradleVersion();
  return PomModuleDescriptorHeaderWriter.HEADER_XML + NL + PomModuleDescriptorHeaderWriter.GENERATE_TEXT_PRE+ GradleVersion.URL+ NL+ PomModuleDescriptorHeaderWriter.GENERATE_TEXT_VERSION+ gradleVersion.getVersion()+ " "+ gradleVersion.getBuildTime()+ NL+ PomModuleDescriptorHeaderWriter.GENERATE_TEXT_POST+ PomModuleDescriptorHeaderWriter.HEADER_XMLNS+ NL+ PomModuleDescriptorHeaderWriter.HEADER_MAVEN+ NL;
}
