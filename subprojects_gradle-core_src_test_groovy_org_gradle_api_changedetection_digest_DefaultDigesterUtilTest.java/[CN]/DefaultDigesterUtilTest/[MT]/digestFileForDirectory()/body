{
  context.checking(new Expectations(){
{
      one(fileMock).exists();
      will(returnValue(true));
      one(fileMock).isFile();
      will(returnValue(false));
      one(fileMock).getAbsolutePath();
      will(returnValue(fileMockAbsolutePath));
    }
  }
);
  digesterUtil.digestFile(digesterMock,fileMock);
}
