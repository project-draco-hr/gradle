{
  events.add(event);
  allTasksCount++;
  if (event.isUpToDate()) {
    upToDateTaskCount++;
  }
  if (event.isCached()) {
    cachedTasksCount++;
  }
  if (event.isTaskCacheable()) {
    cacheableTasksCount++;
  }
  if (event.isSkipped()) {
    skippedTaskCount++;
  }
  if (event.isExecuted()) {
    executedTaskCount++;
  }
}
