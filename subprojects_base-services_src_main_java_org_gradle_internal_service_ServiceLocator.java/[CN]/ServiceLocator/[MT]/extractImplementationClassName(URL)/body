{
  InputStream inputStream=resource.openStream();
  try {
    BufferedReader reader=new BufferedReader(new InputStreamReader(inputStream));
    String line;
    while ((line=reader.readLine()) != null) {
      line=line.replaceAll("#.*","").trim();
      if (line.length() > 0) {
        return line;
      }
    }
  }
  finally {
    inputStream.close();
  }
  return null;
}
