{
  final Task dependencyProjectUploadTaskStub=context.mock(Task.class,"dependencyProjectUploadTask");
  final Task otherConfTaskMock=context.mock(Task.class,"otherConfTask");
  prepareBuildDependenciesTest(dependencyProjectUploadTaskStub,otherConfTaskMock,new ProjectDependenciesBuildInstruction(Collections.<String>emptyList()),new HashMap<String,Task>());
  assertThat((Set<Task>)configuration.getBuildDependencies().getDependencies(dependencyProjectUploadTaskStub),equalTo(toSet(otherConfTaskMock,dependencyProjectUploadTaskStub)));
}
