{
  this.daemonDir=new DaemonDir(baseFolder);
  cache=new SimpleStateCache<DaemonRegistryContent>(daemonDir.registryFile,new OnDemandFileLock(daemonDir.getRegistry(),"daemon addresses registry",new DefaultFileLockManager(new DefaultProcessMetaDataProvider(NativeEnvironment.current()))),new DefaultSerializer<DaemonRegistryContent>());
}
