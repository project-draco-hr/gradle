{
  lock.lock();
  LOGGER.info("Clearing daemon stop events");
  try {
    cache.update(new PersistentStateCache.UpdateAction<DaemonRegistryContent>(){
      public DaemonRegistryContent update(      DaemonRegistryContent content){
        if (content != null) {
          content.clearStopEvents();
        }
        return content;
      }
    }
);
  }
  finally {
    lock.unlock();
  }
}
