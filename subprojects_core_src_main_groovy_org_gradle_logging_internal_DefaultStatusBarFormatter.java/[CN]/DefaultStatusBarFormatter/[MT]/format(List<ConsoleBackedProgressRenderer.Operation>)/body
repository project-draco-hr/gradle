{
  StringBuilder builder=new StringBuilder();
  for (  ConsoleBackedProgressRenderer.Operation operation : operations) {
    String message=operation.getMessage();
    if (message == null) {
      continue;
    }
    if (builder.length() > 0) {
      builder.append(' ');
    }
    builder.append("> ");
    builder.append(message);
  }
  return builder.toString();
}
