{
  DependencyDescriptor[] dds=md.getDependencies();
  if (dds.length > 0) {
    out.println("\t<dependencies>");
    for (int i=0; i < dds.length; i++) {
      DependencyDescriptor dep=dds[i];
      out.print("\t\t");
      printDependency(md,dep,out);
    }
    printAllExcludes(md,out);
    printAllMediators(md,out);
    out.println("\t</dependencies>");
  }
}
