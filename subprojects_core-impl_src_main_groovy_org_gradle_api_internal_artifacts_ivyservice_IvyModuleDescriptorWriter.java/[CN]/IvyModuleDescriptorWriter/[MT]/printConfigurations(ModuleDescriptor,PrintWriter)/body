{
  Configuration[] confs=md.getConfigurations();
  if (confs.length > 0) {
    out.println("\t<configurations>");
    for (int i=0; i < confs.length; i++) {
      Configuration conf=confs[i];
      out.print("\t\t");
      printConfiguration(conf,out);
    }
    out.println("\t</configurations>");
  }
}
