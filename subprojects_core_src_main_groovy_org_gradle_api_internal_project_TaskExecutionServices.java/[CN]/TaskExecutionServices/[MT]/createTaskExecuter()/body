{
  TaskArtifactStateCacheAccess cacheAccess=get(TaskArtifactStateCacheAccess.class);
  return new ExecuteAtMostOnceTaskExecuter(new SkipOnlyIfTaskExecuter(new SkipTaskWithNoActionsExecuter(new SkipEmptySourceFilesTaskExecuter(new ValidatingTaskExecuter(new CacheLockAcquiringTaskExecuter(new SkipUpToDateTaskExecuter(new CacheLockReleasingTaskExecuter(new PostExecutionAnalysisTaskExecuter(new ExecuteActionsTaskExecuter(get(ListenerManager.class).getBroadcaster(TaskActionListener.class))),cacheAccess),get(TaskArtifactStateRepository.class)),cacheAccess))))));
