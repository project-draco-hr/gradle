{
  if (end == text.length()) {
    start=-1;
    return false;
  }
  if (startsWithEol(text,end)) {
    start=end;
    end=start + EOL.length();
    return true;
  }
  int pos=end;
  while (pos < text.length()) {
    if (startsWithEol(text,pos)) {
      start=end;
      end=pos;
      return true;
    }
    pos++;
  }
  start=end;
  end=text.length();
  return true;
}
