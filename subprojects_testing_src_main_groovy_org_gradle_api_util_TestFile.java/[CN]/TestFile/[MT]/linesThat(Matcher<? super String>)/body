{
  try {
    BufferedReader reader=new BufferedReader(new FileReader(this));
    try {
      List<String> lines=new ArrayList<String>();
      String line;
      while ((line=reader.readLine()) != null) {
        if (matcher.matches(line)) {
          lines.add(line);
        }
      }
      return lines;
    }
  finally {
      reader.close();
    }
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
