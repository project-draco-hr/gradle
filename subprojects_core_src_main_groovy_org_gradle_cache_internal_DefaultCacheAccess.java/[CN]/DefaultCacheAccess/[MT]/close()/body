{
  lock.lock();
  try {
    operations.close();
    lockMode=null;
    owner=null;
    if (fileLock != null) {
      closeFileLock();
    }
  }
  finally {
    if (cacheClosedCount != 1) {
      LOG.info("Cache {} was closed {} times.",cacheDiplayName,cacheClosedCount);
    }
    lock.unlock();
  }
}
