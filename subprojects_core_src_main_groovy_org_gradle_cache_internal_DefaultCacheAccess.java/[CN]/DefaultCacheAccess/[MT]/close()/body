{
  lock.lock();
  try {
    for (    MultiProcessSafePersistentIndexedCache<?,?> cache : caches) {
      cache.close();
    }
    operationStack.clear();
    started=false;
    lockMode=null;
    owner=null;
    if (fileLock != null) {
      try {
        fileLock.close();
      }
  finally {
        fileLock=null;
      }
    }
  }
  finally {
    lock.unlock();
  }
}
