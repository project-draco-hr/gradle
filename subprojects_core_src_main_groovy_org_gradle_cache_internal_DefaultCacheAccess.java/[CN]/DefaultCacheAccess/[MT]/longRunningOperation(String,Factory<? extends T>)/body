{
  if (threadIsInLongRunningOperation()) {
    return action.create();
  }
  checkThreadIsOwner();
  boolean wasEnded=onEndWork();
  List<String> parkedOperationStack=parkOwner();
  try {
    return action.create();
  }
  finally {
    restoreOwner(parkedOperationStack);
    if (wasEnded) {
      onStartWork();
    }
  }
}
