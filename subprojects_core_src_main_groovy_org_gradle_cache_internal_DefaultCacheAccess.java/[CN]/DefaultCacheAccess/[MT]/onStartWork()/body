{
  if (fileLock != null) {
    return false;
  }
  busy=true;
  fileLock=lockManager.lock(lockFile,Exclusive,cacheDiplayName,operations.getDescription(),whenContended());
  for (  MultiProcessSafePersistentIndexedCache<?,?> cache : caches) {
    cache.onStartWork(operations.getDescription());
  }
  return true;
}
