{
  lock.lock();
  try {
    if (!started) {
      throw new IllegalStateException(String.format("The %s has not been locked.",cacheDiplayName));
    }
  }
  finally {
    lock.unlock();
  }
  if (fileLock == null) {
    fileLock=lockManager.lock(lockFile,Exclusive,cacheDiplayName,operationDisplayName);
  }
  return fileLock;
}
