{
  lock.lock();
  try {
    lockCache(operationDisplayName);
    try {
      boolean wasStarted=onStartWork();
      try {
        return action.create();
      }
  finally {
        if (wasStarted) {
          onEndWork();
        }
      }
    }
  finally {
      unlockCache();
    }
  }
  finally {
    lock.unlock();
  }
}
