{
  List<String> illegalOptions=toList("dq","di","qd","qi","iq","id");
  for (  String illegalOption : illegalOptions) {
    try {
      checkConversion("-" + illegalOption,"clean");
    }
 catch (    InvalidUserDataException e) {
      continue;
    }
    fail("Expected InvalidUserDataException");
  }
}
