{
  StoppableExecutor stopMe=null;
  lock.lock();
  try {
    contendedActions.remove(target);
    if (contendedActions.isEmpty()) {
      communicator.stop();
      stopMe=executor;
      stopMe.requestStop();
    }
  }
  finally {
    lock.unlock();
  }
  if (stopMe != null) {
    stopMe.stop();
  }
}
