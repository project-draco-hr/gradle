{
  setUpMocksAndAttributes(testObj,TEST_GROOVY_CLASSPATH);
  context.checking(new Expectations(){
{
      one(antGroovycCompileMock).execute(testObj.getProject().getAnt(),testObj.getSrcDirs(),testObj.getIncludes(),testObj.getExcludes(),testObj.getDestinationDir(),TEST_DEPENDENCY_MANAGER_CLASSPATH,testObj.getSourceCompatibility(),testObj.getTargetCompatibility(),testObj.getGroovyOptions(),testObj.getOptions(),TEST_GROOVY_CLASSPATH);
      one(antGroovycCompileMock).getNumFilesCompiled();
      will(returnValue(numFilesCompiled));
    }
  }
);
  testObj.execute();
}
