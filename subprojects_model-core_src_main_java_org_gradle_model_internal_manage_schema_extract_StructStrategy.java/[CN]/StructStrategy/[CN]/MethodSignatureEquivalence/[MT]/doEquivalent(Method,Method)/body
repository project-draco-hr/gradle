{
  boolean equals=new EqualsBuilder().append(a.getName(),b.getName()).append(a.getGenericParameterTypes(),b.getGenericParameterTypes()).isEquals();
  if (equals) {
    equals=a.getReturnType().equals(b.getReturnType()) || a.getReturnType().isAssignableFrom(b.getReturnType()) || b.getReturnType().isAssignableFrom(a.getReturnType());
  }
  return equals;
}
