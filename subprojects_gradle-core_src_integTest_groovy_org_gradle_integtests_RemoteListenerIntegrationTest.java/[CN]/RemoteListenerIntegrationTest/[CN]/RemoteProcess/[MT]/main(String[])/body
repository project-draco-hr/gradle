{
  try {
    RemoteSender<TestListenerInterface> remoteSender=new RemoteSender<TestListenerInterface>(TestListenerInterface.class,new URI(args[0]));
    TestListenerInterface sender=remoteSender.getSource();
    sender.send(FIRST_MESSAGE_TEXT,FIRST_MESSAGE_INT);
    sender.send(SECOND_MESSAGE_TEXT,SECOND_MESSAGE_INT);
    remoteSender.close();
  }
 catch (  Throwable t) {
    t.printStackTrace();
    System.exit(1);
  }
}
