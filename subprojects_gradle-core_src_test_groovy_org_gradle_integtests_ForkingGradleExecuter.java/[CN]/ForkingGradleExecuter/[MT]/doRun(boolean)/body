{
  String windowsCmd;
  String unixCmd;
  if (command != null) {
    windowsCmd=command;
    unixCmd=String.format("%s/%s",workingDir.getAbsolutePath(),command);
  }
 else {
    windowsCmd="gradle";
    unixCmd=String.format("%s/bin/gradle",distribution.getGradleHomeDir().getAbsolutePath());
  }
  return executeInternal(windowsCmd,unixCmd,expectFailure);
}
