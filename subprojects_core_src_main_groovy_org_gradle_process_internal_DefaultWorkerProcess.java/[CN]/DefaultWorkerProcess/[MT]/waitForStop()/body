{
  ExecResult result=execHandle.waitForFinish();
  ObjectConnection connection;
  lock.lock();
  try {
    connection=this.connection;
  }
  finally {
    this.connection=null;
    this.execHandle=null;
    lock.unlock();
  }
  if (connection != null) {
    connection.stop();
  }
  return result.assertNormalExitValue();
}
