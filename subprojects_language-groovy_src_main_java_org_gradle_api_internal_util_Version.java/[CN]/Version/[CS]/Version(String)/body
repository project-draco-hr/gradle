{
  orig=version;
  String[] strings=version.replaceAll("(\\.0)*$","").replaceAll("[^0-9\\.\\-_]","").split("[\\.\\-_]");
  parts=new ArrayList<Integer>();
  if (strings.length > 0 && !strings[0].isEmpty()) {
    for (    String string : strings) {
      parts.add(Integer.valueOf(string));
    }
  }
}
