{
  return new SourceTransformTaskConfig(){
    @Override public String getTaskPrefix(){
      return "compile";
    }
    @Override public Class<? extends DefaultTask> getTaskType(){
      return ScalaCompile.class;
    }
    @Override public void configureTask(    Task task,    BinarySpec binary,    LanguageSourceSet sourceSet){
      ScalaCompile compile=(ScalaCompile)task;
      ScalaSourceSet scalaSourceSet=(ScalaSourceSet)sourceSet;
    }
  }
;
}
