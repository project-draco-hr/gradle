{
  StringBuilder builder=new StringBuilder();
  for (  Operation operation : currentOperations) {
    if (operation.getStatus().length() == 0) {
      continue;
    }
    if (builder.length() > 0) {
      builder.append(' ');
    }
    builder.append("> ");
    builder.append(operation.getStatus());
  }
  statusBar.setText(builder.toString());
}
