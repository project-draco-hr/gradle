{
  Dependency dependency1=createDependency("group1","name","version");
  Dependency dependency2=createDependency("group2","name","version");
  DefaultConfiguration.ConfigurationFileCollection fileCollection=(DefaultConfiguration.ConfigurationFileCollection)configuration.fileCollection(dependency1);
  assertThat(fileCollection.getDependencySpec().isSatisfiedBy(dependency1),equalTo(true));
  assertThat(fileCollection.getDependencySpec().isSatisfiedBy(dependency2),equalTo(false));
}
