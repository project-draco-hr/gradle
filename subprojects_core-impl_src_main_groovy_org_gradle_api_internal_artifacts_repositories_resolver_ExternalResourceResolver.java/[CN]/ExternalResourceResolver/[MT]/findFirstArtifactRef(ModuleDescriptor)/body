{
  for (  String configuration : md.getConfigurationsNames()) {
    for (    Artifact artifact : md.getArtifacts(configuration)) {
      ResolvedArtifact artifactRef=getArtifactRef(artifact,null,false);
      if (artifactRef != null) {
        return artifactRef;
      }
    }
  }
  return null;
}
