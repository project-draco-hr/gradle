{
  StringBuilder builder=new StringBuilder();
  for (  Operation operation : operations) {
    String message=operation.getMessage();
    if (message == null) {
      continue;
    }
    if (builder.length() > 0) {
      builder.append(' ');
    }
    builder.append("> ");
    builder.append(message);
  }
  if (statusBar == null) {
    statusBar=console.getStatusBar();
  }
  statusBar.setText(builder.toString());
}
