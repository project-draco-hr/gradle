{
  try {
    if (OperatingSystem.current().isUnix()) {
      return new Unix();
    }
 else     if (OperatingSystem.current().isWindows()) {
      return new Windows();
    }
 else {
      return new UnsupportedEnvironment();
    }
  }
 catch (  LinkageError e) {
    return new UnsupportedEnvironment();
  }
}
