{
  SecurityManager current=System.getSecurityManager();
  if (current instanceof GradleSecurityManager) {
    ((GradleSecurityManager)current).active=false;
  }
 else {
    System.out.println("Warning: System.exit() requested but Gradle security manager was not installed or was replaced. " + "Gradle does not know if the request is a legitimate one.");
  }
  System.exit(exitValue);
}
