{
  calcCurrentState();
  if (lastExecution == null) {
    return singletonList(String.format("No history is available for %s.",task));
  }
  if (!task.getClass().getName().equals(lastExecution.taskClass)) {
    return singletonList(String.format("%s has changed type from '%s' to '%s'.",StringUtils.capitalize(task.toString()),lastExecution.taskClass,task.getClass().getName()));
  }
  List<String> messages=new ArrayList<String>();
  checkOutputFiles(messages);
  if (!messages.isEmpty()) {
    return messages;
  }
  checkInputProperties(messages);
  if (!messages.isEmpty()) {
    return messages;
  }
  checkInputs(messages);
  if (!messages.isEmpty()) {
    return messages;
  }
  checkOutputs(messages);
  if (!messages.isEmpty()) {
    return messages;
  }
  upToDate=true;
  return emptyList();
}
