{
  try {
    ObjectInputStream inStr=new ObjectInputStream(new BufferedInputStream(new FileInputStream(getStateFile())));
    try {
      outputs=(Map<File,OutputGenerators>)inStr.readObject();
    }
  finally {
      inStr.close();
    }
  }
 catch (  Exception e) {
    throw new UncheckedIOException(e);
  }
}
