{
switch (result.getResultType()) {
case SUCCESS:
    logger.info("Test {} PASSED",test.getName());
  break;
case SKIPPED:
logger.info("Test {} SKIPPED",test.getName());
break;
case FAILURE:
logger.error("Test {} FAILED",test.getName());
hadFailures=true;
break;
default :
throw new IllegalArgumentException();
}
}
