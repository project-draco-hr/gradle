{
  for (  StackTraceElement element : exceptionInfo.stackTrace) {
    builder.append(element);
    builder.append('\n');
  }
  while ((exceptionInfo=exceptionInfo.cause) != null) {
    builder.append("\nCaused by:\n");
    for (    StackTraceElement element : exceptionInfo.stackTrace) {
      builder.append(element);
      builder.append('\n');
    }
  }
}
