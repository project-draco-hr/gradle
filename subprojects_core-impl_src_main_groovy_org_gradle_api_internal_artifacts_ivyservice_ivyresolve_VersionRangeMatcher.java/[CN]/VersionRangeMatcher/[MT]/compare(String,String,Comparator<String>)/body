{
  Matcher m;
  m=UPPER_INFINITE_RANGE.matcher(requestedVersion);
  if (m.matches()) {
    return 1;
  }
  String upper;
  m=FINITE_RANGE.matcher(requestedVersion);
  if (m.matches()) {
    upper=m.group(2);
  }
 else {
    m=LOWER_INFINITE_RANGE.matcher(requestedVersion);
    if (m.matches()) {
      upper=m.group(1);
    }
 else {
      throw new IllegalArgumentException("impossible to compare: askedMrid is not a dynamic requestedVersion: " + requestedVersion);
    }
  }
  int c=staticComparator.compare(upper,foundVersion);
  return c == 0 ? -1 : c;
}
