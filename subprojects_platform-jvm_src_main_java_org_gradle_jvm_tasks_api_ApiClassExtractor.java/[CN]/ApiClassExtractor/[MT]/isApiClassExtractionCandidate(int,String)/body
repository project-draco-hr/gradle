{
  if (isLocalClass(candidateClassName)) {
    return false;
  }
  if (!ApiMemberSelector.isCandidateApiMember(access,apiIncludesPackagePrivateMembers)) {
    return false;
  }
  if (exportedPackages.isEmpty()) {
    return true;
  }
  String candidatePackageName=packageNameOf(candidateClassName);
  for (  String exportedPackage : exportedPackages) {
    if (candidatePackageName.equals(exportedPackage)) {
      return true;
    }
  }
  return false;
}
