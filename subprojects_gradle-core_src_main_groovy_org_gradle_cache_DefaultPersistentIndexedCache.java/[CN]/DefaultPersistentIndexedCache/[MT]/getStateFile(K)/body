{
  String fileName;
  if (key instanceof File) {
    fileName=getStateFileName((File)key);
  }
 else   if (key instanceof String) {
    fileName=getStateFileName((String)key);
  }
 else   if (key == null) {
    fileName="null";
  }
 else {
    throw new UnsupportedOperationException("Not implemented yet");
  }
  if (fileName.length() < 5) {
    return new File(backingCache.getBaseDir(),String.format("none/%s.bin",fileName));
  }
  return new File(backingCache.getBaseDir(),String.format("%s/%s/%s.bin",fileName.substring(0,2),fileName.subSequence(0,4),fileName));
}
