{
  File stateFile=getStateFile(key);
  try {
    InputStream inStr=new BufferedInputStream(new FileInputStream(stateFile));
    try {
      return serializer.read(inStr);
    }
  finally {
      inStr.close();
    }
  }
 catch (  FileNotFoundException e) {
    return null;
  }
catch (  Exception e) {
    throw new UncheckedIOException(e);
  }
}
