{
  File stateFile=getStateFile(key);
  stateFile.getParentFile().mkdirs();
  try {
    OutputStream outStr=new BufferedOutputStream(new FileOutputStream(stateFile));
    try {
      serializer.write(outStr,value);
    }
  finally {
      outStr.close();
    }
  }
 catch (  Exception e) {
    throw new UncheckedIOException(e);
  }
  backingCache.update();
}
