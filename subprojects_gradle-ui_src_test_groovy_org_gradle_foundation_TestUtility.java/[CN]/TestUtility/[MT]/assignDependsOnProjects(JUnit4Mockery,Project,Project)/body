{
  final Set<Project> set=new LinkedHashSet<Project>();
  if (dependsOnProjects != null && dependsOnProjects.length != 0)   set.addAll(Arrays.asList(dependsOnProjects));
  context.checking(new Expectations(){
{
      allowing(project).getDependsOnProjects();
      will(returnValue(set));
    }
  }
);
}
