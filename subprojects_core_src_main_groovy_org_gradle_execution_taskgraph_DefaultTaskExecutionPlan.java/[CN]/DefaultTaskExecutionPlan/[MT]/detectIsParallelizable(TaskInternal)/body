{
  if (task.getClass().isAnnotationPresent(ParallelizableTask.class)) {
    if (task.isHasCustomActions()) {
      LOGGER.info("Unable to parallelize task " + task.getPath() + " due to presence of custom actions (e.g. doFirst()/doLast())");
    }
 else {
      return true;
    }
  }
  return false;
}
