{
  lock.lock();
  try {
    enforceFinaliserTasks(taskInfo);
    if (taskInfo.isFailed()) {
      handleFailure(taskInfo);
    }
    taskInfo.finishExecution();
    runningProjects.remove(taskInfo.getTask().getProject().getPath());
    condition.signalAll();
  }
  finally {
    lock.unlock();
  }
}
