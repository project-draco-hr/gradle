{
  lock.lock();
  try {
    graph.clear();
    entryTasks.clear();
    executionPlan.clear();
    failures.clear();
    projectsWithRunningTasks.clear();
    projectsWithRunningNonParallelizableTasks.clear();
    canonicalizedOutputCache.clear();
    isParallelSafeCache.clear();
    runningTasks.clear();
  }
  finally {
    lock.unlock();
  }
}
