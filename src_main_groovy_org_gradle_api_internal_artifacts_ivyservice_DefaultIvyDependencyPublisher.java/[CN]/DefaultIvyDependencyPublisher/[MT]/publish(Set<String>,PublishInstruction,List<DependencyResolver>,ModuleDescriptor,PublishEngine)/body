{
  try {
    if (publishInstruction.isUploadDescriptor()) {
      moduleDescriptor.toIvyFile(publishInstruction.getDescriptorDestination());
    }
    for (    DependencyResolver resolver : publishResolvers) {
      logger.info("Publishing to Resolver {}",resolver);
      publishEngine.publish(moduleDescriptor,createArtifactPatterns(moduleDescriptor.getAllArtifacts()),resolver,publishOptionsFactory.createPublishOptions(configurations,publishInstruction));
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
catch (  ParseException e) {
    throw new RuntimeException(e);
  }
}
