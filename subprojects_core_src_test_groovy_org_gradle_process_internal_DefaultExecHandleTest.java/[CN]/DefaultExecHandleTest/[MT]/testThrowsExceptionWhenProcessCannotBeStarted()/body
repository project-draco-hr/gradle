{
  DefaultExecHandle execHandle=new DefaultExecHandle("display-name",tmpDir.getDir(),"no_such_command",Arrays.asList("arg"),System.getenv(),new StreamsForwarder(),Collections.<ExecHandleListener>emptyList(),false);
  try {
    execHandle.start();
    fail();
  }
 catch (  ExecException e) {
    assertEquals("A problem occurred starting display-name.",e.getMessage());
  }
}
