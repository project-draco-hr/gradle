{
  StringBuilder builder=new StringBuilder();
  builder.append(name);
  builder.append('(');
  for (int i=0; i < parameters.size(); i++) {
    ParameterMetaData param=parameters.get(i);
    if (i > 0) {
      builder.append(", ");
    }
    builder.append(param.getSignature());
  }
  builder.append(')');
  return builder.toString();
}
