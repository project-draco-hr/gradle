{
  IdeaModel ideaModel=project.getPlugins().getPlugin(IdeaPlugin.class).getModel();
  IdeaProject projectModel=ideaModel.getProject();
  DefaultIdeaProject newProject=new DefaultIdeaProject().setName(projectModel.getName()).setId(project.getPath()).setJdkName(projectModel.getJdkName()).setLanguageLevel(projectModel.getLanguageLevel().getFormatted());
  return newProject;
}
