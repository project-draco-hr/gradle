{
  if (result.getResultType() == ResultType.FAILURE && result.getException() != null) {
    reportFailure(suite,toString(suite),result);
  }
 else {
    logger.debug("Finished {}",suite);
  }
  if (suite.getParent() == null && result.getResultType() == ResultType.FAILURE) {
    hadFailures=true;
  }
}
