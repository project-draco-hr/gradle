{
  byte[] serialised=entries.get(key);
  if (serialised == null) {
    return null;
  }
  try {
    ByteArrayInputStream instr=new ByteArrayInputStream(serialised);
    return (V)new ObjectInputStream(instr).readObject();
  }
 catch (  Exception e) {
    throw UncheckedException.throwAsUncheckedException(e);
  }
}
