{
  List<String> jvmArguments=new ArrayList<String>(providerParameters.getJvmArguments(Collections.<String>emptyList()));
  Iterator<String> it=jvmArguments.iterator();
  while (it.hasNext()) {
    String arg=it.next();
    if (arg.startsWith("-D")) {
      it.remove();
    }
  }
  return jvmArguments;
}
