{
  try {
    if (service != null) {
      try {
        invoke(service.getClass().getMethod("stop"),service);
      }
 catch (      NoSuchMethodException e) {
      }
      try {
        invoke(service.getClass().getMethod("close"),service);
      }
 catch (      NoSuchMethodException e) {
      }
    }
  }
  finally {
    service=null;
  }
}
