{
  try {
    if (instance != null) {
      try {
        invoke(instance.getClass().getMethod("stop"),instance);
      }
 catch (      NoSuchMethodException e) {
      }
      try {
        invoke(instance.getClass().getMethod("close"),instance);
      }
 catch (      NoSuchMethodException e) {
      }
    }
  }
  finally {
    instance=null;
  }
}
