{
  return createBaseText(org,name) + XmlHelper.enclose(PomModuleDescriptorWriter.DEFAULT_INDENT,PomModuleDescriptorWriter.VERSION,revision) + NL+ XmlHelper.enclose(PomModuleDescriptorWriter.DEFAULT_INDENT,PomModuleDescriptorWriter.PACKAGING,packaging)+ NL+ XmlHelper.enclose(PomModuleDescriptorWriter.DEFAULT_INDENT,PomModuleDescriptorWriter.CLASSIFIER,modifier)+ NL;
}
