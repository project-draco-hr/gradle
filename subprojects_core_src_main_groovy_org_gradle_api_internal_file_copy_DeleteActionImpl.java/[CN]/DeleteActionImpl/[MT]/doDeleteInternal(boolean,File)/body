{
  if (file.isDirectory() && (followSymlinks || !fileSystem.isSymlink(file))) {
    File[] contents=file.listFiles();
    if (contents == null) {
      return;
    }
    for (    File item : contents) {
      doDeleteInternal(followSymlinks,item);
    }
  }
  if (!file.delete() && file.exists()) {
    handleFailedDelete(file);
  }
}
