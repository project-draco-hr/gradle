{
  try {
    InputStream inputStream=sourceUri.toURL().openStream();
    try {
      return IOUtils.toString(inputStream);
    }
  finally {
      inputStream.close();
    }
  }
 catch (  FileNotFoundException e) {
    return "";
  }
catch (  Exception e) {
    throw new GradleException(String.format("Could not read %s.",getDisplayName()),e);
  }
}
