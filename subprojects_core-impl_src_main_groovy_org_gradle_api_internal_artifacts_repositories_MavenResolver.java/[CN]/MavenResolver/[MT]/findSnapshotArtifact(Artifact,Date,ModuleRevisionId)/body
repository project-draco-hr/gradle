{
  String rev=findUniqueSnapshotVersion(mrid);
  if (rev != null) {
    String pattern=getWholePattern().replaceFirst("\\-\\[revision\\]","-" + rev);
    return findResourceUsingPattern(mrid,pattern,artifact,getDefaultRMDParser(artifact.getModuleRevisionId().getModuleId()),date);
  }
  return null;
}
