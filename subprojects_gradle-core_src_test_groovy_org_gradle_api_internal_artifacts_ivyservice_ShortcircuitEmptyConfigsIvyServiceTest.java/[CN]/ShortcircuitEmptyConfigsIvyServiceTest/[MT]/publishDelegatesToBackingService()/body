{
  final Set<Configuration> configurations=toSet(configuration);
  final PublishInstruction instructionDummy=new PublishInstruction();
  final List<DependencyResolver> resolvers=toList(context.mock(DependencyResolver.class));
  context.checking(new Expectations(){
{
      one(delegate).publish(configurations,instructionDummy,resolvers);
    }
  }
);
  ivyService.publish(configurations,instructionDummy,resolvers);
}
