{
  if (!configured) {
    configured=true;
    try {
      actions.execute(extension);
    }
 catch (    ListenerNotificationException e) {
      configureFailure=e.getCause();
    }
    actions=null;
  }
  if (configureFailure != null) {
    throw UncheckedException.throwAsUncheckedException(configureFailure);
  }
}
