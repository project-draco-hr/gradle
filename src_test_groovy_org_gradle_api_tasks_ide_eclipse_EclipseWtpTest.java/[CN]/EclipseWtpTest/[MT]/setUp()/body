{
  super.setUp();
  File projectDir=HelperUtil.makeNewTestDir();
  projectDependencyMock=context.mock(ProjectDependency.class);
  testProject=HelperUtil.createRootProject(new File("dependent"));
  context.checking(new Expectations(){
{
      allowing(projectDependencyMock).getDependencyProject();
      will(returnValue(testProject));
    }
  }
);
  eclipseWtp=new EclipseWtp(getProject(),AbstractTaskTest.TEST_TASK_NAME,getTasksGraph());
}
