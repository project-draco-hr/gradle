{
  ResolvedResource artifactRef=getArtifactRef(artifact,null,false);
  if (artifactRef != null && artifactRef.getResource().exists()) {
    return new ArtifactOriginWithLastModified(artifact,artifactRef.getResource().isLocal(),artifactRef.getResource().getName(),artifactRef.getLastModified());
  }
  return null;
}
