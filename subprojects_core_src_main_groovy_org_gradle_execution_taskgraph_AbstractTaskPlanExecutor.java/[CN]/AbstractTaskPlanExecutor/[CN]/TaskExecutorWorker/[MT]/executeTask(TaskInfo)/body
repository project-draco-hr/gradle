{
  TaskInternal task=taskInfo.getTask();
  TaskStateInternal state=task.getState();
synchronized (lock) {
    taskListener.beforeExecute(task,state);
  }
  try {
    task.executeWithoutThrowingTaskFailure();
  }
  finally {
synchronized (lock) {
      taskListener.afterExecute(task,state);
    }
  }
}
