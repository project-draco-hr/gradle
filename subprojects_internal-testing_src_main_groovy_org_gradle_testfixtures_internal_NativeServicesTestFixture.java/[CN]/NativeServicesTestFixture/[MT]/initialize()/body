{
  if (!initialized) {
    try {
      File nativeDir=File.createTempFile("native","dir");
      nativeDir.delete();
      nativeDir.mkdirs();
      nativeDir.deleteOnExit();
      NativeServices.initialize(nativeDir);
    }
 catch (    IOException e) {
      throw new UncheckedIOException(e);
    }
  }
}
