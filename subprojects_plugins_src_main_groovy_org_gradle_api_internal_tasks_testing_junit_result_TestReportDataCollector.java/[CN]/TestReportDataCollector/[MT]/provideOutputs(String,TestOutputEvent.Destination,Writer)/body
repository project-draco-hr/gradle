{
  File file=outputsFile(className,destination);
  if (!file.exists()) {
    return;
  }
  FileInputStream in=null;
  try {
    in=new FileInputStream(file);
    IOUtils.copy(in,writer);
  }
 catch (  IOException e) {
    throw UncheckedException.throwAsUncheckedException(e);
  }
 finally {
    IOUtils.closeQuietly(in);
  }
}
