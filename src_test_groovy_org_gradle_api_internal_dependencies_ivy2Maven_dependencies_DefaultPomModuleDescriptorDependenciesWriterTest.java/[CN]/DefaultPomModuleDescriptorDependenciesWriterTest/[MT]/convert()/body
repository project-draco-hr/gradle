{
  final MavenDependency testMavenDependency=context.mock(MavenDependency.class);
  context.checking(new Expectations(){
{
      one(testMavenDependency).write(testPrintWriter);
      one(dependenciesConverterMock).convert(moduleDescriptor,true,conf2ScopeMappingContainerMock);
      will(returnValue(WrapUtil.toList(testMavenDependency)));
    }
  }
);
  dependenciesWriter.convert(moduleDescriptor,true,conf2ScopeMappingContainerMock,testPrintWriter);
  assertEquals(XmlHelper.openTag(2,PomModuleDescriptorDependenciesWriter.DEPENDENCIES) + NL + XmlHelper.closeTag(2,PomModuleDescriptorDependenciesWriter.DEPENDENCIES)+ NL,testStringWriter.toString());
}
