{
  GccCompileSpecToArguments<CCompileSpec> specToArguments=new GccCompileSpecToArguments<CCompileSpec>(new CCompileOptionsToArguments(),new GccCompileSourcesToArguments<CCompileSpec>(),useCommandFile);
  this.commandLineTool=new CommandLineTool<CCompileSpec>(executable,execActionFactory).withArguments(specToArguments);
}
