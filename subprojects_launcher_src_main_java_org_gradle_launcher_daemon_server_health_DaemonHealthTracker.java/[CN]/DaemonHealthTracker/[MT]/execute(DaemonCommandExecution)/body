{
  if (execution.isSingleUseDaemon()) {
    execution.proceed();
    return;
  }
  stats.buildStarted();
  logger.logHealth(stats,LOG);
  try {
    execution.proceed();
  }
  finally {
    stats.buildFinished();
  }
  if (status.isDaemonUnhealthy()) {
    execution.getDaemonStateControl().requestStop();
  }
}
