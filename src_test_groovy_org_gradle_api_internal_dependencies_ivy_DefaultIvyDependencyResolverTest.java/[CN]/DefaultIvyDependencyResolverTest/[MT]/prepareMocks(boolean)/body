{
  context.checking(new Expectations(){
{
      allowing(resolveReportMock).hasError();
      will(returnValue(hasError));
      allowing(ivyMock).resolve(expectedModuleDescriptor,expectedResolveOptions);
      will(returnValue(resolveReportMock));
