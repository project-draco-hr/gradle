{
  if (incomingFinished) {
    return null;
  }
  Message result;
  try {
    result=super.receive();
  }
 catch (  Throwable e) {
    LOGGER.error("Could not receive message from connection. Discarding connection.",e);
    result=null;
  }
  if (result instanceof EndOfStreamEvent) {
    incomingFinished=true;
  }
 else   if (result == null) {
    incomingFinished=true;
    result=new EndOfStreamEvent();
  }
  return result;
}
