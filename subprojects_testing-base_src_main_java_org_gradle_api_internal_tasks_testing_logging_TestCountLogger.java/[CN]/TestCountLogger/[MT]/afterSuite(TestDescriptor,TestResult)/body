{
  if (suite.getParent() == null) {
    progressLogger.progress(summary());
    if (failedTests > 0) {
      logger.error(TextUtil.getPlatformLineSeparator() + summary());
    }
    progressLogger.completed();
    if (result.getResultType() == TestResult.ResultType.FAILURE) {
      hadFailures=true;
    }
  }
}
