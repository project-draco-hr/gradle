{
  DeprecationLogger.nagUserOfDiscontinuedMethod("Project.childrenDependOnMe()");
  DeprecationLogger.whileDisabled(new Factory<Void>(){
    public Void create(){
      for (      Project project : childProjects.values()) {
        project.dependsOn(getPath(),false);
      }
      return null;
    }
  }
);
  return this;
}
