{
  addFailuresTab();
  if (!getResults().getPackages().isEmpty()) {
    addTab("Packages",new Action<Element>(){
      public void execute(      Element element){
        renderPackages(element);
      }
    }
);
  }
  addTab("Classes",new Action<Element>(){
    public void execute(    Element element){
      renderClasses(element);
    }
  }
);
}
