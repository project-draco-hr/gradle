{
  Report[] combined=new Report[enabled.size() + disabled.size()];
  int i=0;
  for (  Report report : enabled) {
    combined[i++]=report;
    report.setEnabled(true);
  }
  for (  Report report : disabled) {
    combined[i++]=report;
    report.setEnabled(false);
  }
  return new ReportContainer(combined);
}
