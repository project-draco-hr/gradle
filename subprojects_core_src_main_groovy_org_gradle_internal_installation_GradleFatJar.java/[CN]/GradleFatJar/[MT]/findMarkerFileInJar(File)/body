{
  JarFile jarFile=null;
  try {
    jarFile=new JarFile(jar);
    JarEntry markerFile=jarFile.getJarEntry(MARKER_FILENAME);
    if (markerFile != null) {
      return true;
    }
  }
 catch (  IOException e) {
    UncheckedException.throwAsUncheckedException(e);
  }
 finally {
    if (jarFile != null) {
      try {
        jarFile.close();
      }
 catch (      IOException e) {
        UncheckedException.throwAsUncheckedException(e);
      }
    }
  }
  return false;
}
