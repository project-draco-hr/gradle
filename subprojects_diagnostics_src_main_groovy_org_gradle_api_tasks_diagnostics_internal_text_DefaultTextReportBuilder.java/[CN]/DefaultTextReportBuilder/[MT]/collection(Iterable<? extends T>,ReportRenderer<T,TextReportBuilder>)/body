{
  StyledTextOutput original=textOutput;
  boolean hasItem=false;
  try {
    textOutput=new LinePrefixingStyledTextOutput(original,"    ");
    for (    T t : items) {
      if (!hasItem) {
        textOutput.append("    ");
        hasItem=true;
      }
      try {
        renderer.render(t,this);
      }
 catch (      IOException e) {
        throw new UncheckedIOException(e);
      }
    }
  }
  finally {
    textOutput=original;
  }
}
