{
  if (extra == null) {
    return;
  }
  String delim=prefix;
  for (Iterator iter=extra.entrySet().iterator(); iter.hasNext(); ) {
    Map.Entry entry=(Map.Entry)iter.next();
    writer.write(delim + entry.getKey() + "=\""+ XMLHelper.escape(entry.getValue().toString())+ "\"");
    delim=" ";
  }
}
