{
  DefaultLibraryLocalComponentMetaData metaData=new DefaultLibraryLocalComponentMetaData(localModuleVersionIdentifierFor(componentId),componentId);
  metaData.addConfiguration(usageConfigurationName,String.format("Request metadata: %s",componentId.getDisplayName()),Collections.<String>emptySet(),Collections.singleton(usageConfigurationName),true,true,buildDependencies);
  return metaData;
}
