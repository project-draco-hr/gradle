{
  DefaultLibraryLocalComponentMetaData metaData=new DefaultLibraryLocalComponentMetaData(localModuleVersionIdentifierFor(componentId),componentId);
  metaData.addConfiguration(CONFIGURATION_API,String.format("Request metadata: %s",componentId.getDisplayName()),Collections.<String>emptySet(),Collections.singleton(CONFIGURATION_API),true,true,buildDependencies);
  return metaData;
}
