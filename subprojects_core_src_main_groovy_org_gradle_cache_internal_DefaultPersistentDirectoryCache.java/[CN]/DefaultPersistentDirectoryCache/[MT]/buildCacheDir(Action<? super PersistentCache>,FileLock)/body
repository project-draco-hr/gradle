{
  for (  File file : getBaseDir().listFiles()) {
    if (fileLock.isLockFile(file) || file.equals(propertiesFile)) {
      continue;
    }
    GFileUtils.forceDelete(file);
  }
  if (initAction != null) {
    initAction.execute(this);
  }
  GUtil.saveProperties(properties,propertiesFile);
  didRebuild=true;
}
