{
  lifecycleLock.lock();
  try {
    if (!stopped) {
      try {
        disconnectableInput.close();
      }
 catch (      IOException e) {
        throw UncheckedException.throwAsUncheckedException(e);
      }
      forwardingExecuter.stop();
      stopped=true;
      cancellationToken.removeCallback(cancellationCallback);
    }
  }
  finally {
    lifecycleLock.unlock();
  }
}
