{
  try {
    InputStream inputStream=getInputStream(sourceUri);
    try {
      return true;
    }
  finally {
      inputStream.close();
    }
  }
 catch (  FileNotFoundException e) {
    return false;
  }
catch (  Exception e) {
    throw new ResourceException(String.format("Could not determine if %s exists.",getDisplayName()),e);
  }
}
