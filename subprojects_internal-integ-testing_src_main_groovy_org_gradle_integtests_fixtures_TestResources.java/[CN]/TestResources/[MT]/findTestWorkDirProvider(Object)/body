{
  if (target instanceof TestWorkDirProvider) {
    return (TestWorkDirProvider)target;
  }
  TestWorkDirProvider dist=RuleHelper.findField(target,TestWorkDirProvider.class);
  if (dist != null) {
    return dist;
  }
  throw new RuntimeException(String.format("Test class %s does not implement %s and has no field of this type",target.getClass().getName(),TestWorkDirProvider.class.getName()));
}
