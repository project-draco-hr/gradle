{
  processor.completed(testId,event);
  if (testId.equals(suite)) {
    try {
      PrintStream capturingOut=System.out;
      PrintStream capturingErr=System.err;
      System.setOut(out);
      System.setErr(err);
      capturingOut.close();
      capturingErr.close();
    }
  finally {
      suite=null;
      out=null;
      err=null;
    }
  }
}
