{
  lock.lock();
  try {
    Thread current=Thread.currentThread();
    for (    Thread thread : active) {
      if (thread == current) {
        continue;
      }
switch (thread.getState()) {
case NEW:
case RUNNABLE:
case TERMINATED:
        return false;
    }
  }
  return true;
}
  finally {
  lock.unlock();
}
}
