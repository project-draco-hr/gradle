{
  lock.lock();
  try {
    active.remove(thread);
    if (failure != null) {
      LOGGER.debug(String.format("Failure in %s",thread),failure);
      failures.add(failure);
    }
 else {
      LOGGER.debug("Finished {}",thread);
    }
    activeChanged.signalAll();
  }
  finally {
    lock.unlock();
  }
}
