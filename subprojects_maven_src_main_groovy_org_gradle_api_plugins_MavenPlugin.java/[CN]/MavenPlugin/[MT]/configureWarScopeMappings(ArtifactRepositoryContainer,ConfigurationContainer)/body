{
  repositoryContainer.getMavenScopeMappings().addMapping(PROVIDED_COMPILE_PRIORITY,configurations.getByName(WarPlugin.PROVIDED_COMPILE_CONFIGURATION_NAME),Conf2ScopeMappingContainer.PROVIDED);
  repositoryContainer.getMavenScopeMappings().addMapping(PROVIDED_RUNTIME_PRIORITY,configurations.getByName(WarPlugin.PROVIDED_RUNTIME_CONFIGURATION_NAME),Conf2ScopeMappingContainer.PROVIDED);
}
