  htmlWriter.startElement("div").attribute("class","breadcrumbs").startElement("a").attribute("href",getResults().getUrlTo(getResults().getParent().getParent())).characters("all").endElement().characters(" > ").startElement("a").attribute("href",getResults().getUrlTo(getResults().getPackageResults())).characters(getResults().getPackageResults().getName()).endElement().characters(" > " + getResults().getSimpleName()).endElement();
}
