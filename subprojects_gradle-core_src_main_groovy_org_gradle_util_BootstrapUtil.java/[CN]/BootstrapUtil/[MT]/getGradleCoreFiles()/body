{
  List<File> files=gradleLibClasspath("gradle-core");
  if (!files.isEmpty()) {
    return files;
  }
  files=new ArrayList<File>();
  for (  File file : getGradleClasspath()) {
    if (file.isDirectory()) {
      if (new File(file,BootstrapUtil.class.getName().replace('.','/') + ".class").isFile()) {
        files.add(file);
      }
    }
  }
  return files;
}
