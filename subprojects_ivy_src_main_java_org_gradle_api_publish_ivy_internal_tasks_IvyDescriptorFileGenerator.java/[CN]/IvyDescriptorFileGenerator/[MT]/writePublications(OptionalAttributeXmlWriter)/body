{
  if (artifacts.isEmpty()) {
    return;
  }
  xmlWriter.startElement("publications");
  for (  IvyArtifact artifact : artifacts) {
    xmlWriter.startElement("artifact").attribute("name",artifact.getName()).attribute("type",artifact.getType()).attribute("ext",artifact.getExtension()).attribute("conf",artifact.getConf()).endElement();
  }
  xmlWriter.endElement();
}
