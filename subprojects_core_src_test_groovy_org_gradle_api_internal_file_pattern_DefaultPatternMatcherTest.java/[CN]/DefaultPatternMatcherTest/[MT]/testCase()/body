{
  matcher=new DefaultPatternMatcher(true,true,"a","b");
  assertTrue(matcher.isSatisfiedBy(file("a","b")));
  assertFalse(matcher.isSatisfiedBy(file("A","B")));
  matcher=new DefaultPatternMatcher(true,false,"a","b");
  assertTrue(matcher.isSatisfiedBy(file("a","b")));
  assertTrue(matcher.isSatisfiedBy(file("A","B")));
}
