{
  try {
    file.getParentFile().mkdirs();
    Writer writer=new OutputStreamWriter(new BufferedOutputStream(new FileOutputStream(file)),"utf-8");
    try {
      writeTo(model,writer);
    }
  finally {
      writer.close();
    }
  }
 catch (  Exception e) {
    throw UncheckedException.throwAsUncheckedException(e);
  }
}
