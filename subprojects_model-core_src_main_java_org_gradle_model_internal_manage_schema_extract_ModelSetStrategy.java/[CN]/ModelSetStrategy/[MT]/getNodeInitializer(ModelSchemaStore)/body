{
  return new Function<ModelCollectionSchema<T,E>,NodeInitializer>(){
    @Override public NodeInitializer apply(    ModelCollectionSchema<T,E> schema){
      return new ProjectionOnlyNodeInitializer(TypedModelProjection.of(ModelTypes.modelSet(schema.getElementType()),new ModelSetModelViewFactory<E>(schema.getElementType(),store)));
    }
  }
;
}
