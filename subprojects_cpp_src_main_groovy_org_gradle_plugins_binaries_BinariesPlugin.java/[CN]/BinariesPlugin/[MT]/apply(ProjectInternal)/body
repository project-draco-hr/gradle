{
  project.getPlugins().apply(BasePlugin.class);
  ClassGenerator classGenerator=project.getServices().get(ClassGenerator.class);
  project.getExtensions().add("binaries",classGenerator.newInstance(FactoryNamedDomainObjectContainer.class,DefaultBinary.class,classGenerator,new NamedDomainObjectFactory<Binary>(){
    public Binary create(    String name){
      return new DefaultBinary(name,new GppCompileSpec(name,project));
    }
  }
));
}
