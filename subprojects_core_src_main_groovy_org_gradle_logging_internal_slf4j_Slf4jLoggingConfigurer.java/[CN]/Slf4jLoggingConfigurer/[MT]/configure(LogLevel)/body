{
  if (currentLevel == logLevel) {
    return;
  }
  try {
    doConfigure(logLevel);
    LOGGER.debug("Finished configuring slf4j, log level: {}, current level: {}",currentLevel,logLevel);
  }
 catch (  Throwable e) {
    doFailsafeConfiguration();
    throw UncheckedException.asUncheckedException(e);
  }
}
