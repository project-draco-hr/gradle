{
  File artifactResolutionCacheFile=new File(cacheMetadata.getCacheDir(),"artifact-resolution.bin");
  FileLock artifactResolutionCacheLock=cacheLockingManager.getCacheMetadataFileLock(artifactResolutionCacheFile);
  return new BTreePersistentIndexedCache<RevisionKey,ArtifactResolutionCacheEntry>(artifactResolutionCacheFile,artifactResolutionCacheLock,new DefaultSerializer<ArtifactResolutionCacheEntry>(ArtifactResolutionCacheEntry.class.getClassLoader()));
}
