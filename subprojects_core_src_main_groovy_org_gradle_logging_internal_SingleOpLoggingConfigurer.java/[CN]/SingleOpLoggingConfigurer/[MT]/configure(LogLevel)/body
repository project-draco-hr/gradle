{
  if (configured) {
    return;
  }
  LOGGER.debug("Single-op logging configuration; configurer id: {}, log level: {}",System.identityHashCode(this),logLevel);
  sync.synchronize(new Operation(){
    public void execute(){
      if (configured) {
        return;
      }
      configured=true;
      delegate.configure(logLevel);
    }
  }
);
}
