{
  for (  ModuleVersionSpec thisSpec : getSpecs()) {
    boolean found=false;
    for (    ModuleVersionSpec otherSpec : spec.getSpecs()) {
      if (thisSpec.acceptsSameModulesAs(otherSpec)) {
        found=true;
        break;
      }
    }
    if (!found) {
      return false;
    }
  }
  return true;
}
