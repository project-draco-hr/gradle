{
  boolean stdout=checkIsConsole(FileDescriptor.out);
  boolean stderr=checkIsConsole(FileDescriptor.err);
  if (!stdout && !stderr) {
    return null;
  }
  String term=System.getenv("TERM");
  if (term != null && term.equals("dumb")) {
    return null;
  }
  return new UnixConsoleMetaData(stdout,stderr);
}
