{
  if (root.getAllModuleFiles().isEmpty()) {
    return;
  }
  for (  ResolvedDependency dep : root.getChildren()) {
    String edge="\"" + root.toString() + "\" -> \""+ dep.toString().replace('-','_')+ "\";";
    edges.add(edge);
  }
  for (  ResolvedDependency dep : root.getChildren()) {
    buildDotDependencyTree(dep,edges);
  }
}
