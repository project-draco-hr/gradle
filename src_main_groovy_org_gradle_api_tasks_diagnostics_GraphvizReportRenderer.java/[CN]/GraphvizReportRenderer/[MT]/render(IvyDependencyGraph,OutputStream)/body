{
  OutputStreamWriter writer=new OutputStreamWriter(output);
  writer.write("digraph " + graph.getConf() + " {\n");
  Set<String> edges=new HashSet<String>();
  buildDotDependencyTree(graph.getRoot(),edges);
  for (  String edge : edges) {
    writer.write(edge + "\n");
  }
  writer.write("}\n");
}
