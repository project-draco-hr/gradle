{
  if (!pathEntries.isEmpty()) {
    String exeName=operatingSystem.getExecutableName(name);
    for (    File pathEntry : pathEntries) {
      File candidate=new File(pathEntry,exeName);
      if (candidate.isFile()) {
        return candidate;
      }
    }
    return null;
  }
 else {
    return operatingSystem.findInPath(name);
  }
}
