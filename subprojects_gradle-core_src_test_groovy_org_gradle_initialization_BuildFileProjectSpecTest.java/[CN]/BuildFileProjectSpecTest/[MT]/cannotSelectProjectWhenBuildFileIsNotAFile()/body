{
  file.delete();
  try {
    spec.containsProject(registry());
    fail();
  }
 catch (  InvalidUserDataException e) {
    assertThat(e.getMessage(),equalTo("Build file '" + file + "' does not exist."));
  }
  file.mkdirs();
  try {
    spec.containsProject(registry());
    fail();
  }
 catch (  InvalidUserDataException e) {
    assertThat(e.getMessage(),equalTo("Build file '" + file + "' is not a file."));
  }
}
