{
  for (  String libPath : EclipseUtil.getSortedStringList(getWarLibs())) {
    wbModule.addElement("dependent-module").addAttribute("deploy-path","/WEB-INF/lib").addAttribute("handle","module:/classpath/" + FilenameUtils.separatorsToUnix(libPath)).addElement("dependency-type").setText("uses");
  }
}
