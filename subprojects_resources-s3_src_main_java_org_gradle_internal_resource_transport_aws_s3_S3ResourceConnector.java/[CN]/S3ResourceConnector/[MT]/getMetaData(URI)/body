{
  LOGGER.debug("Attempting to get resource metadata: {}",location);
  try {
    S3Object s3Object=s3Client.getMetaData(location);
    if (s3Object == null) {
      return null;
    }
    ObjectMetadata objectMetadata=s3Object.getObjectMetadata();
    return new DefaultExternalResourceMetaData(location,objectMetadata.getLastModified().getTime(),objectMetadata.getContentLength(),objectMetadata.getETag(),null);
  }
 catch (  S3Exception s3x) {
    throw new UncheckedIOException(s3x.getMessage(),s3x);
  }
}
