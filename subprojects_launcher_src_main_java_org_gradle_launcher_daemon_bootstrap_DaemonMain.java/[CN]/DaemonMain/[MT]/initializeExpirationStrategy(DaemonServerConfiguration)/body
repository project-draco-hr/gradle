{
  DaemonIdleTimeoutExpirationStrategy timeoutStrategy=new DaemonIdleTimeoutExpirationStrategy(params.getIdleTimeout(),TimeUnit.MILLISECONDS);
  DaemonRegistryUnavailableExpirationStrategy registryUnavailableStrategy=new DaemonRegistryUnavailableExpirationStrategy();
  DaemonExpirationStrategy lowMemoryLruStrategy=new AllDaemonExpirationStrategy(ImmutableList.of(LowMemoryDaemonExpirationStrategy.belowFreePercentage(0.2),new LruDaemonExpirationStrategy()));
  return new AnyDaemonExpirationStrategy(ImmutableList.of(timeoutStrategy,registryUnavailableStrategy,lowMemoryLruStrategy));
}
