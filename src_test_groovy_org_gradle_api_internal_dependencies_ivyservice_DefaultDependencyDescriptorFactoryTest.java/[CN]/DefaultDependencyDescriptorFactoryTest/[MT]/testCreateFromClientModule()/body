{
  ClientModule moduleDependency=(ClientModule)new ClientModule(TEST_CONF_MAPPING,"org.gradle:gradle-core:1.0",null).setForce(true).addArtifact(artifact).addArtifact(artifactWithClassifierAndConfs);
  moduleDependency.setExcludeRules(excludeRuleContainerMock);
  moduleDependency.setDependencyConfigurationMappings(dependencyConfigurationMappingContainerMock);
  DefaultDependencyDescriptor dependencyDescriptor=(DefaultDependencyDescriptor)dependencyDescriptorFactory.createFromClientModule(TEST_PARENT,moduleDependency);
  assertSame(TEST_PARENT.getModuleRevisionId(),dependencyDescriptor.getParentRevisionId());
  checkModuleDependency(dependencyDescriptor,moduleDependency);
}
