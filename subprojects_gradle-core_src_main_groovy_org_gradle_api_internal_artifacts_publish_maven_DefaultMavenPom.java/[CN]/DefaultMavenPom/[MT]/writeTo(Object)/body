{
  try {
    File file=fileResolver.resolve(path);
    if (file.getParentFile() != null) {
      file.getParentFile().mkdirs();
    }
    FileWriter writer=new FileWriter(file);
    try {
      return writeTo(writer);
    }
  finally {
      writer.close();
    }
  }
 catch (  IOException e) {
    throw new UncheckedIOException(e);
  }
}
