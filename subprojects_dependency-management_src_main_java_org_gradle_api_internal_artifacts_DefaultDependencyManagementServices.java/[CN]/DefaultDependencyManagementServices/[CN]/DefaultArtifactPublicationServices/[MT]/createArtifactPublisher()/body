{
  return new IvyBackedArtifactPublisher(services.get(ConfigurationsToModuleDescriptorConverter.class),services.get(DependenciesToModuleDescriptorConverter.class),services.get(ConfigurationsToArtifactsConverter.class),services.get(IvyContextManager.class),new DefaultIvyDependencyPublisher(),new IvyXmlModuleDescriptorWriter());
}
