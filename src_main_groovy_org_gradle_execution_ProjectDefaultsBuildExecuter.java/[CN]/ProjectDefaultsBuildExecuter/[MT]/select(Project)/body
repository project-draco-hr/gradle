{
  if (getDelegate() == null) {
    List<String> defaultTasks=project.getDefaultTasks();
    if (defaultTasks.size() == 0) {
      throw new InvalidUserDataException(String.format("No tasks have been specified and project %s has not defined any default tasks.",project));
    }
    setDelegate(new TaskNameResolvingBuildExecuter(defaultTasks));
  }
  super.select(project);
}
