{
  if (value != null && !value.isEmpty()) {
    for (    final String group : value.keySet()) {
      final List<String> groupPackages=value.get(group);
      writerContext.writeOptionHeader(option).write(new StringBuffer().append("\"").append(group).append("\"").toString()).write(" ").write(new StringBuffer().append("\"").append(GUtil.join(groupPackages,":")).append("\"").toString()).newLine();
    }
  }
}
