{
  for (int i=0; i < args.length; i++) {
    Object arg=args[i];
    if (i > 0) {
      writer.print(' ');
    }
    String str=arg.toString();
    if (backslashEscape) {
      str=str.replace("\\","\\\\").replace("\"","\\\"");
    }
    if (WHITESPACE.matcher(str).find() || (str.isEmpty() && quoteEmpty)) {
      writer.print('\"');
      writer.print(str);
      writer.print('\"');
    }
 else {
      writer.print(str);
    }
  }
  writer.println();
  return this;
}
