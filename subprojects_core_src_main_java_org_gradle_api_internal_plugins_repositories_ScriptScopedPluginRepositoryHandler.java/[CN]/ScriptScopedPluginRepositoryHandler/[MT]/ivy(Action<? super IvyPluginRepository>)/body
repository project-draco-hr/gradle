{
  final Action<? super IvyPluginRepository> localAction=action;
  return delegate.ivy(new Action<IvyPluginRepository>(){
    @Override public void execute(    IvyPluginRepository ivyPluginRepository){
      IvyPluginRepositoryWrapper wrapper=instantiator.newInstance(IvyPluginRepositoryWrapper.class,ivyPluginRepository,fileResolver);
      localAction.execute(wrapper);
    }
  }
);
}
