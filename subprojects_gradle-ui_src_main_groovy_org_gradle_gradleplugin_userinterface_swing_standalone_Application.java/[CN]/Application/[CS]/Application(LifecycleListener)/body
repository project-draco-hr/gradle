{
  this.lifecycleListener=lifecycleListener;
  try {
    UIManager.setLookAndFeel(UIManager.getSystemLookAndFeelClassName());
  }
 catch (  Exception e) {
  }
  this.doesSupportEditingFiles=determineIfSupportsEditingFiles();
  rootSettingsNode=DOM4JSerializer.readSettingsFile(new SettingsImportInteraction(),createFileFilter());
  if (rootSettingsNode == null)   rootSettingsNode=DOM4JSerializer.createBlankSettings();
  singlePaneUIInstance=new SinglePaneUIInstance(rootSettingsNode,this);
  setupUI();
  restoreSettings();
  frame.setVisible(true);
  lifecycleListener.hasStarted();
}
