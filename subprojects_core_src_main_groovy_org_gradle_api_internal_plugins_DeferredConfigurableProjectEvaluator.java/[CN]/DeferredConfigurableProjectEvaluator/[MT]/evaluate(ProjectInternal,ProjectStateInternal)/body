{
  delegate.evaluate(project,state);
  if (state.hasFailure()) {
    return;
  }
  try {
    project.getExtensions().getAsMap();
  }
 catch (  Exception e) {
    GradleScriptException failure=new GradleScriptException(String.format("A problem occurred evaluating %s.",project),e);
    state.executed(failure);
  }
}
