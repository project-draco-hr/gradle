{
  target.getPluginManager().apply(LifecycleBasePlugin.class);
  target.getExtensions().create("sources",DefaultProjectSourceSet.class,instantiator);
  DefaultBinaryContainer binaries=target.getExtensions().create("binaries",DefaultBinaryContainer.class,instantiator);
  modelRegistry.create(ModelCreators.of(ModelReference.of("binaries",BinaryContainer.class),binaries).simpleDescriptor("Project.<init>.binaries()").withProjection(new PolymorphicDomainObjectContainerModelProjection<DefaultBinaryContainer,BinarySpec>(binaries,BinarySpec.class)).build());
}
