{
  if (isSameOrNewer("2.10")) {
    return javaVersion.compareTo(JavaVersion.VERSION_1_6) >= 0;
  }
  if (javaVersion.compareTo(JavaVersion.VERSION_1_9) >= 0) {
    return false;
  }
  if (isSameOrNewer("2.0")) {
    return javaVersion.compareTo(JavaVersion.VERSION_1_6) >= 0;
  }
  return javaVersion.compareTo(JavaVersion.VERSION_1_5) >= 0;
}
