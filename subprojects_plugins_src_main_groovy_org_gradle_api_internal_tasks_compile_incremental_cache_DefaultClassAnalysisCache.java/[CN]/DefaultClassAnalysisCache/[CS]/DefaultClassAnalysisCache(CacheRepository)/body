{
  cacheAccess=cacheRepository.cache("classAnalysisCache").withDisplayName("class analysis cache").withLockOptions(mode(FileLockManager.LockMode.None)).open();
  PersistentIndexedCacheParameters<byte[],ClassAnalysis> params=new PersistentIndexedCacheParameters<byte[],ClassAnalysis>("classAnalysisCache",byte[].class,ClassAnalysis.class);
  cache=cacheAccess.createCache(params);
}
