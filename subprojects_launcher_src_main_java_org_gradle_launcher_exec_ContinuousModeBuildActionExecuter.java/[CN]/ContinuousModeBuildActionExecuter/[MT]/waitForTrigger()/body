{
  try {
synchronized (lock) {
      while (lastSeenTrigger == null) {
        lock.wait();
      }
      logger.lifecycle("Rebuild triggered due to " + lastSeenTrigger.getReason());
      lastSeenTrigger=null;
    }
  }
 catch (  InterruptedException e) {
    UncheckedException.throwAsUncheckedException(e);
  }
}
