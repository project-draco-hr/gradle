{
  String projectPathValue=descriptor.getAttribute(DependencyDescriptorFactory.PROJECT_PATH_KEY);
  if (projectPathValue == null) {
    return null;
  }
  Project project=gradle.getRootProject().project(projectPathValue);
  DefaultModule module=new DefaultModule(project.getGroup().toString(),project.getPath(),project.getVersion().toString(),project.getStatus().toString());
  return moduleDescriptorConverter.convert(project.getConfigurations().getAll(),module,IvyContext.getContext().getIvy().getSettings());
}
