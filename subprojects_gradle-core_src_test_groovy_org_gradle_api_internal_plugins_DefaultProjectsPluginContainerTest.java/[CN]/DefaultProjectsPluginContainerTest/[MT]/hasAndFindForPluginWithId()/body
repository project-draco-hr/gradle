{
  projectsPluginHandler.usePlugin(pluginId);
  assertThat(projectsPluginHandler.hasPlugin(pluginId),equalTo(true));
  assertThat(projectsPluginHandler.hasPlugin(pluginWithIdMock.getClass()),equalTo(true));
  assertThat((TestPlugin1)projectsPluginHandler.findPlugin(pluginId),sameInstance((Plugin)pluginWithIdMock));
  assertThat((TestPlugin1)projectsPluginHandler.findPlugin(pluginWithIdMock.getClass()),sameInstance((Plugin)pluginWithIdMock));
}
