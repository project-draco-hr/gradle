{
  T binary=BaseBinarySpec.create(type,name,instantiator,taskFactory);
  Action<NativeBinarySpec> initializer=DefaultNativeBinariesFactory.binaryInitializer(Actions.doNothing(),component,namingScheme,resolver,toolChain,toolProvider,platform,buildType,flavor);
  initializer.execute(binary);
  ComponentSpecInternal componentInternal=(ComponentSpecInternal)component;
  binary.setBinarySources(componentInternal.getSources().copy(name));
  return binary;
}
