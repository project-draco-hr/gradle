{
  return NotationParserBuilder.toType(new TypeInfo<Spec<DependencyResult>>(Spec.class)).invalidNotationMessage("Please check the input for the DependencyInsight.dependency element.").fromType(Closure.class,new ClosureToSpecNotationConverter<DependencyResult>(DependencyResult.class)).fromCharSequence(new DependencyResultSpecNotationConverter()).toComposite();
}
