{
  InputStream instr=new DisconnectableInputStream(input);
  standardOutputRunner=new ExecOutputHandleRunner("read process standard output",process.getInputStream(),standardOutput);
  errorOutputRunner=new ExecOutputHandleRunner("read process error output",process.getErrorStream(),errorOutput);
  standardInputRunner=new ExecOutputHandleRunner("write process standard input",instr,process.getOutputStream());
}
