{
  String supportedModifiersStr=Joiner.on(", ").join(supportedModifiers.keySet());
  if (supportedModifiers.get(modifier) == null) {
    throw new GradleException("The requested init modifier '" + modifier + "' is not supported."+ " Supported modifiers for this build setup type are: "+ supportedModifiersStr);
  }
  initModifiers.put(modifier,true);
  return this;
}
