{
  ImmutableList<JdkSpec> jdkSpecs=index.get(path);
  if (jdkSpecs.size() > 1) {
    errors.add(String.format("   - %s are both pointing to the same JDK installation path: %s",Joiner.on(", ").join(Iterables.transform(jdkSpecs,new Function<JdkSpec,String>(){
      @Override public String apply(      JdkSpec input){
        return input.getName();
      }
    }
)),path));
  }
}
