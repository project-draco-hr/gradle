{
  super(project,name);
  setDagNeutral(true);
  doFirst(new TaskAction(){
    public void execute(    Task task){
      try {
        ProjectReportRenderer renderer=getRenderer();
        if (outputFile != null) {
          outputFile.getParentFile().mkdirs();
          renderer.setOutputFile(outputFile);
        }
        generate();
        renderer.complete();
      }
 catch (      IOException e) {
        throw new GradleException(e);
      }
    }
  }
);
}
