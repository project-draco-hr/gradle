{
  final String configurationName=configuration.getName();
  context.checking(new Expectations(){
{
      allowing(dependencyDescriptorFactoryStub).createDependencyDescriptor(with(equal(configurationName)),with(equal(parent)),with(sameInstance(dependency)),with(equal(CLIENT_MODULE_REGISTRY_DUMMY)));
