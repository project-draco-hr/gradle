{
  ModelPromise promise=node.getPromise();
  if (isTypeCompatible(promise)) {
    boundTo=node;
    bindAction.execute(node);
    return true;
  }
 else {
    throw new InvalidModelRuleException(referrer,new ModelRuleBindingException(IncompatibleTypeReferenceReporter.of(node,promise,reference,writable).asString()));
  }
}
