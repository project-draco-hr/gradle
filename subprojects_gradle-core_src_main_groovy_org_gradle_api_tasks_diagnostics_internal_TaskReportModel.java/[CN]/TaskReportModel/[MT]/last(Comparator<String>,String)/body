{
  return new Comparator<String>(){
    public int compare(    String o1,    String o2){
      boolean o1Other=o1.equalsIgnoreCase(lastValue);
      boolean o2Other=o2.equalsIgnoreCase(lastValue);
      if (o1Other && o2Other) {
        return 0;
      }
      if (o1Other && !o2Other) {
        return 1;
      }
      if (!o1Other && o2Other) {
        return -1;
      }
      return comparator.compare(o1,o2);
    }
  }
;
}
