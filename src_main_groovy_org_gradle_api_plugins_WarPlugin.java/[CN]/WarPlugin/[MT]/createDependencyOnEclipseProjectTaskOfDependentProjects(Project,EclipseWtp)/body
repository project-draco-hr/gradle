{
  Set<Dependency> projectDependencies=Specs.filterIterable(project.getConfigurations().get(JavaPlugin.RUNTIME).getDependencies(),DependencySpecs.type(Type.PROJECT));
  for (  Dependency dependentProject : projectDependencies) {
    eclipseWtp.dependsOn(((ProjectDependency)dependentProject).getDependencyProject().getPath() + ":eclipseProject");
  }
}
