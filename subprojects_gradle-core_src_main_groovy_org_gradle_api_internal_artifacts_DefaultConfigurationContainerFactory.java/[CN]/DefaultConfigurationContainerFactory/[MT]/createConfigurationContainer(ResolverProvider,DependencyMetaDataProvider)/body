{
  IvyService ivyService=new ErrorHandlingIvyService(new ShortcircuitEmptyConfigsIvyService(new DefaultIvyService(dependencyMetaDataProvider,resolverProvider,new DefaultModuleDescriptorConverter(new DefaultDependenciesToModuleDescriptorConverter(new DefaultDependencyDescriptorFactory(clientModuleRegistry))),clientModuleRegistry)));
  return new DefaultConfigurationHandler(ivyService);
}
