{
  IvyService ivyService=new ErrorHandlingIvyService(new ShortcircuitEmptyConfigsIvyService(new DefaultIvyService(dependencyMetaDataProvider,resolverProvider,settingsConverter,resolveModuleDescriptorConverter,publishModuleDescriptorConverter,ivyFileConverter,ivyFactory,dependencyResolver,dependencyPublisher,clientModuleRegistry)));
