{
  try {
    TerminalDetector detector=new TerminalDetector();
    System.setOut(AnsiConsole.out());
    System.setErr(AnsiConsole.err());
    runClient(new BuildArgs(currentDir,args,detector.isSatisfiedBy(FileDescriptor.out),detector.isSatisfiedBy(FileDescriptor.err)));
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}
