{
  DefaultExecHandle execHandle=new DefaultExecHandle(tmpDir.getDir(),"no_such_command",Arrays.asList("arg"),System.getenv(),System.out,System.err,new ByteArrayInputStream(new byte[0]),Collections.<ExecHandleListener>emptyList());
  try {
    execHandle.start();
    fail();
  }
 catch (  ExecException e) {
    assertEquals("A problem occurred starting command 'no_such_command'.",e.getMessage());
  }
}
