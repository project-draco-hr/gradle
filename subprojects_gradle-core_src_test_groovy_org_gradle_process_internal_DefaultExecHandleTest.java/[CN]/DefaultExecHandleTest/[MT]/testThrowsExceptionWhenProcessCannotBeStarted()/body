{
  DefaultExecHandle execHandle=new DefaultExecHandle(tmpDir.getDir(),"no_such_command",Arrays.asList(),System.getenv(),System.out,System.err,new ByteArrayInputStream(new byte[0]),null);
  try {
    execHandle.start();
    fail();
  }
 catch (  ExecException e) {
    assertEquals("A problem occurred starting 'no_such_command'.",e.getMessage());
  }
}
