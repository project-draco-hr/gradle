{
  DefaultExecHandle execHandle=new DefaultExecHandle("display-name",tmpDir.getDir(),"no_such_command",Arrays.asList("arg"),System.getenv(),System.out,System.err,new ByteArrayInputStream(new byte[0]),Collections.<ExecHandleListener>emptyList());
  try {
    execHandle.start();
    fail();
  }
 catch (  ExecException e) {
    assertEquals("A problem occurred starting display-name.",e.getMessage());
  }
}
