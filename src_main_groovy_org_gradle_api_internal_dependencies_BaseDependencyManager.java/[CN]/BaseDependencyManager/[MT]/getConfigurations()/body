{
  Map<String,org.apache.ivy.core.module.descriptor.Configuration> configurations=new HashMap<String,org.apache.ivy.core.module.descriptor.Configuration>();
  for (  Map.Entry<String,DefaultConfiguration> entry : this.configurations.entrySet()) {
    configurations.put(entry.getKey(),entry.getValue().getIvyConfiguration());
  }
  return configurations;
}
