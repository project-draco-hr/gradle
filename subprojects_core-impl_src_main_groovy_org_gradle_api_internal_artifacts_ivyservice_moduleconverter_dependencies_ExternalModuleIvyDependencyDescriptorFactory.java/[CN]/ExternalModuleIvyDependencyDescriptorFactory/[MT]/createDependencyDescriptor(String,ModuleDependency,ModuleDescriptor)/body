{
  ModuleRevisionId moduleRevisionId=createModuleRevisionId(dependency);
  EnhancedDependencyDescriptor dependencyDescriptor=new EnhancedDependencyDescriptor(dependency,parent,moduleRevisionId,getExternalModuleDependency(dependency).isForce(),getExternalModuleDependency(dependency).isChanging(),getExternalModuleDependency(dependency).isTransitive());
  addExcludesArtifactsAndDependencies(configuration,getExternalModuleDependency(dependency),dependencyDescriptor);
  return dependencyDescriptor;
}
