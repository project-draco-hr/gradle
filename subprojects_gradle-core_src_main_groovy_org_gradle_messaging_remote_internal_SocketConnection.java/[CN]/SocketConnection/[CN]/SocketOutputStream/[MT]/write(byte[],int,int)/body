{
  int remaining=max;
  int currentPos=offset;
  while (remaining > 0) {
    int count=Math.min(remaining,buffer.remaining());
    if (count > 0) {
      buffer.put(src,currentPos,count);
      remaining-=count;
      currentPos+=count;
    }
    if (buffer.remaining() == 0) {
      flush();
    }
  }
}
