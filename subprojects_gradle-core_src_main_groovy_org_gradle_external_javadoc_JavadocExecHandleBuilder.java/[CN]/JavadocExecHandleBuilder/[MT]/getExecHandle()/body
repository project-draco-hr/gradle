{
  try {
    options.write(optionsFile);
  }
 catch (  IOException e) {
    throw new GradleException("Faild to store javadoc options.",e);
  }
  final ExecHandleBuilder execHandleBuilder=new ExecHandleBuilder(true).execDirectory(execDirectory).execCommand(JavaEnvUtils.getJdkExecutable("javadoc")).arguments("@" + optionsFile.getAbsolutePath());
  options.contributeCommandLineOptions(execHandleBuilder);
  return execHandleBuilder.getExecHandle();
}
