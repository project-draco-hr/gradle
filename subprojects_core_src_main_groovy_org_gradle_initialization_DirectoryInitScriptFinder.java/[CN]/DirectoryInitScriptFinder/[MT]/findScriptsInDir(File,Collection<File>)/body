{
  if (!initScriptsDir.isDirectory()) {
    return;
  }
  List<File> files=new ArrayList<File>();
  for (  File file : initScriptsDir.listFiles()) {
    if (file.isFile() && file.getName().endsWith(".gradle")) {
      files.add(file);
    }
  }
  Collections.sort(files);
  for (  File file : files) {
    scripts.add(file);
  }
}
