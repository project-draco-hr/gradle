{
  modelRegistry.registerListener(new ModelListener(){
    @Override public ModelPath getAncestor(){
      return ModelElementNode.this.getPath();
    }
    @Override public boolean matches(    MutableModelNode node){
      return node.canBeViewedAs(type);
    }
    @Override public boolean onDiscovered(    ModelNodeInternal node){
      node.applyToSelf(rules);
      return false;
    }
  }
);
}
