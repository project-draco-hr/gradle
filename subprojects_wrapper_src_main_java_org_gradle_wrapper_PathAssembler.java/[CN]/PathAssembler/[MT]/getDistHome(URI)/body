{
  String name=getDistName(distUrl);
  Matcher matcher=Pattern.compile("(\\p{Alpha}+(-\\p{Alpha}+)*-\\d+\\.\\d+.*?)(-\\p{Alpha}+)?\\.zip").matcher(name);
  if (!matcher.matches()) {
    throw new RuntimeException(String.format("Cannot determine Gradle version from distribution URL '%s'.",distUrl));
  }
  return matcher.group(1);
}
