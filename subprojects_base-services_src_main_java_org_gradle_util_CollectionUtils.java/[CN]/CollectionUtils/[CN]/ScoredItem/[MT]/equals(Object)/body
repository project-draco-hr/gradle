{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ScoredItem that=(ScoredItem)o;
  if (!item.equals(that.item)) {
    return false;
  }
  if (score != null ? !score.equals(that.score) : that.score != null) {
    return false;
  }
  return true;
}
