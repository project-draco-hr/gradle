{
  dependsOn(new Callable<TaskDependency>(){
    public TaskDependency call() throws Exception {
      return getLeft().getBuildDependencies();
    }
  }
);
  dependsOn(new Callable<TaskDependency>(){
    public TaskDependency call() throws Exception {
      return getRight().getBuildDependencies();
    }
  }
);
}
