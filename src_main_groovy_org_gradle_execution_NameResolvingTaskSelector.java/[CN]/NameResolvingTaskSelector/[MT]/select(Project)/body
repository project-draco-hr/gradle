{
  if (current == null) {
    current=names.iterator();
    checkForUnknownTasks(project);
  }
  if (!current.hasNext()) {
    tasks=Collections.emptySet();
    description="";
    return;
  }
  String taskName=current.next();
  description=String.format("primary task '%s'",taskName);
  tasks=findTasks(project,taskName);
}
