{
  BuildActionParameters parameters=new DefaultBuildActionParameters(new GradleLauncherMetaData(),actionParameters.getStartTime(),this.parameters.getEffectiveSystemProperties(),System.getenv(),SystemProperties.getCurrentDir(),actionParameters.getBuildLogLevel());
  try {
    return executer.execute(action,parameters);
  }
 catch (  ReportedException e) {
    throw new BuildExceptionVersion1(e.getCause());
  }
}
