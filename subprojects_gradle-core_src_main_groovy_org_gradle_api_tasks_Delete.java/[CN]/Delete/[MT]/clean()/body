{
  setDidWork(false);
  for (  File file : getTargetFiles()) {
    if (!file.exists()) {
      continue;
    }
    logger.debug("Deleting {}",file);
    setDidWork(true);
    if (file.isFile()) {
      GFileUtils.deleteQuietly(file);
    }
 else {
      GFileUtils.deleteDirectory(file);
    }
  }
}
