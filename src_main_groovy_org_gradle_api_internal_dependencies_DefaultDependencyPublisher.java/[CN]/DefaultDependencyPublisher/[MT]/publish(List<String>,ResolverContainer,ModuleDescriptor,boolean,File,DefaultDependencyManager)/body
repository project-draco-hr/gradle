{
  PublishOptions publishOptions=new PublishOptions();
  if (uploadModuleDescriptor) {
    try {
      moduleDescriptor.toIvyFile(ivyFile);
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
    publishOptions.setSrcIvyPattern(ivyFile.getAbsolutePath());
  }
  publish(configurations,resolvers,moduleDescriptor,absoluteArtifactPatterns(dependencyManager.getAbsoluteArtifactPatterns(),dependencyManager.getDefaultArtifactPattern(),dependencyManager.getArtifactParentDirs()),publishOptions);
}
