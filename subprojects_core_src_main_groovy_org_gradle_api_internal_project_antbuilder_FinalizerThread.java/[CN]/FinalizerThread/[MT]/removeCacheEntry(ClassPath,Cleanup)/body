{
  lock.writeLock().lock();
  try {
    if (LOG.isDebugEnabled()) {
      LOG.debug(String.format("Removing classloader from cache, classpath = %s",key.getAsURIs()));
    }
    cacheEntries.remove(key);
    cleanups.remove(key);
    entry.cleanup();
    entry.clear();
  }
  finally {
    lock.writeLock().unlock();
  }
}
