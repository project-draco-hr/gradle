{
  if (LOG.isDebugEnabled()) {
    LOG.debug(String.format("Removing classloader from cache, classpath = %s",key.getAsURIs()));
  }
  lock.lock();
  try {
    cacheEntries.remove(key);
    cleanups.remove(key);
  }
  finally {
    lock.unlock();
  }
  try {
    entry.clear();
    entry.cleanup();
  }
 catch (  Exception ex) {
    LOG.error("Unable to perform cleanup of classloader for classpath: " + key,ex);
  }
}
