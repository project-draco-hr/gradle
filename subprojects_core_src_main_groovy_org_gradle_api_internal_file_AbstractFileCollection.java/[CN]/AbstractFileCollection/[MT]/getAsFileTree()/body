{
  return new CompositeFileTree(){
    @Override public TaskDependency getBuildDependencies(){
      return AbstractFileCollection.this.getBuildDependencies();
    }
    @Override public void resolve(    FileCollectionResolveContext context){
      DefaultFileCollectionResolveContext nested=new DefaultFileCollectionResolveContext();
      nested.add(AbstractFileCollection.this.getFiles());
      context.add(nested.resolveAsFileTrees());
    }
    @Override public String getDisplayName(){
      return AbstractFileCollection.this.getDisplayName();
    }
  }
;
}
