{
  try {
    while (System.in.available() > 0) {
      long available=System.in.available();
      for (int i=0; i < available; i++) {
        if (System.in.read() == -1) {
          break;
        }
      }
    }
  }
 catch (  IOException e) {
    logger.warn("Error discarding console input buffer",e);
  }
}
