{
  boolean didWork=false;
  for (  File sourceFile : spec.getSourceFiles()) {
    WorkResult result=commandLineTool.inWorkDirectory(spec.getObjectFileDir()).withArguments(new SingleSourceCompileArgTransformer<T>(sourceFile,argsTransFormer)).execute(spec);
    didWork=didWork || result.getDidWork();
  }
  return new SimpleWorkResult(didWork);
}
