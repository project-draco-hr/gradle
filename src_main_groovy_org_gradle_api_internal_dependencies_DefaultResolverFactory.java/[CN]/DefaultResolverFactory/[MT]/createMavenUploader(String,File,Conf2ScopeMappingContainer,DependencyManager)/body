{
  return new DefaultGroovyMavenUploader(name,new DefaultArtifactPomContainer(pomDir),new DefaultMavenPomFactory(conf2ScopeMapping,new DefaultPomFileWriter(new DefaultPomWriter(new DefaultPomHeaderWriter(),new DefaultPomModuleIdWriter(),new DefaultPomDependenciesWriter(new DefaultPomDependenciesConverter(new DefaultExcludeRuleConverter()))))),dependencyManager);
}
