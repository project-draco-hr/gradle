{
  return new DefaultGroovyMavenUploader(name,new DefaultArtifactPomContainer(pomDir),new DefaultMavenPomFactory(conf2ScopeMapping,dependencyManager,new DefaultPomModuleDescriptorFileWriter(new DefaultPomModuleDescriptorWriter(new DefaultPomModuleDescriptorHeaderWriter(),new DefaultPomModuleDescriptorModuleIdWriter(),new DefaultPomModuleDescriptorDependenciesWriter(new DefaultPomModuleDescriptorDependenciesConverter(new DefaultExcludeRuleConverter()))))));
}
