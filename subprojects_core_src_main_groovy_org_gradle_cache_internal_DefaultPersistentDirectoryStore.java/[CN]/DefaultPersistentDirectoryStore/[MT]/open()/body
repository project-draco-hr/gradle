{
  GFileUtils.mkdirs(dir);
  cacheAccess=createCacheAccess();
  try {
    cacheAccess.open(lockMode);
    try {
      init();
    }
 catch (    Throwable throwable) {
      if (cacheAccess != null) {
        cacheAccess.close();
      }
      throw throwable;
    }
  }
 catch (  Throwable e) {
    throw new CacheOpenException(String.format("Could not open %s.",this),e);
  }
  return this;
}
