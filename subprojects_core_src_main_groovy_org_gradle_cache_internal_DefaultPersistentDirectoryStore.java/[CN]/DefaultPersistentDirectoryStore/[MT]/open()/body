{
  dir.mkdirs();
  cacheAccess=new DefaultCacheAccess(displayName,getLockTarget(),lockManager);
  try {
    cacheAccess.open(lockMode);
    try {
      init();
    }
 catch (    Throwable throwable) {
      cacheAccess.close();
      throw throwable;
    }
  }
 catch (  Throwable e) {
    throw new CacheOpenException(String.format("Could not open %s.",this),e);
  }
  return this;
}
