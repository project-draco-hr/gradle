{
  if (!a.equals(b)) {
    return false;
  }
  if (!b.equals(a)) {
    return false;
  }
  if (!a.equals(a)) {
    return false;
  }
  if (b.equals(null)) {
    return false;
  }
  if (b.equals(new Object())) {
    return false;
  }
  if (a.hashCode() != b.hashCode()) {
    return false;
  }
  return true;
}
