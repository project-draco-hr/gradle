{
  String txt;
  if (Strings.isNullOrEmpty(libraryName)) {
    txt="project '" + projectPath + "'";
  }
 else   if (Strings.isNullOrEmpty(variant)) {
    txt="project '" + projectPath + "' library '"+ libraryName+ "'";
  }
 else {
    txt="project '" + projectPath + "' library '"+ libraryName+ "' binary '"+ variant+ "'";
  }
  return txt;
}
