{
  lockingManager.useCache(String.format("resolve %s",artifact),new Runnable(){
    public void run(){
      ivyContextManager.withIvy(new Action<Ivy>(){
        public void execute(        Ivy ivy){
          delegate.resolveArtifact(moduleMetaData,artifact,result);
        }
      }
);
    }
  }
);
}
