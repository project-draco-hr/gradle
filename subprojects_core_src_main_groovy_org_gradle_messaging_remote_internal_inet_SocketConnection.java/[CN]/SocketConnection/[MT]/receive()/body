{
  try {
    return serializer.read(instr,localAddress,remoteAddress);
  }
 catch (  Exception e) {
    if (isEndOfStream(e)) {
      return null;
    }
    MessageIOException exception=new MessageIOException(String.format("Could not read message from '%s'.",remoteAddress),e);
    exception.printStackTrace();
    throw exception;
  }
}
