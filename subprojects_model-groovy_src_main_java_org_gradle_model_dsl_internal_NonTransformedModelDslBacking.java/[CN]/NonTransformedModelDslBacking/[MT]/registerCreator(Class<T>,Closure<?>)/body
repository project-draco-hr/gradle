{
  ModelRuleDescriptor descriptor=new SimpleModelRuleDescriptor("model." + modelPath);
  NodeInitializer nodeInitializer=nodeInitializerRegistry.getNodeInitializer(ModelType.of(type));
  modelRegistry.create(ModelCreators.of(modelPath,nodeInitializer).descriptor(descriptor).action(ModelActionRole.Initialize,NoInputsModelAction.of(ModelReference.of(modelPath,type),descriptor,new ClosureBackedAction<T>(closure))).build());
}
