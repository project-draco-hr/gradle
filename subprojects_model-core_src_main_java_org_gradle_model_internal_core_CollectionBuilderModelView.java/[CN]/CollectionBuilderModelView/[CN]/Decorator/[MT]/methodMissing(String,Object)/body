{
  Object[] args=(Object[])argsObj;
  if (args.length == 1 && args[0] instanceof Class<?>) {
    @SuppressWarnings("unchecked") Class<? extends T> itemType=(Class<? extends T>)args[0];
    create(name,itemType);
  }
 else   if (args.length == 2 && args[0] instanceof Class<?> && args[1] instanceof Closure<?>) {
    @SuppressWarnings("unchecked") Class<? extends T> itemType=(Class<? extends T>)args[0];
    @SuppressWarnings("unchecked") Closure<? super T> closure=(Closure<T>)args[1];
    create(name,itemType,closure);
  }
 else   if (args.length == 1 && args[0] instanceof Closure<?>) {
    @SuppressWarnings("unchecked") Closure<? super T> closure=(Closure<? super T>)args[0];
    named(name,closure);
  }
 else {
    throw new MissingMethodException(name,getClass(),args);
  }
  return null;
}
