{
  return new DefaultBuildExecuter(asList(new OnlyWhenConfigureOnDemand(new ProjectEvaluatingAction(new TaskPathProjectEvaluator())),new DefaultTasksBuildExecutionAction(),new ExcludedTaskFilteringBuildConfigurationAction(),new TaskNameResolvingBuildConfigurationAction()),asList(new DryRunBuildExecutionAction(),new TaskCacheLockHandlingBuildExecuter(get(TaskArtifactStateCacheAccess.class)),new SelectedTaskExecutionAction()));
