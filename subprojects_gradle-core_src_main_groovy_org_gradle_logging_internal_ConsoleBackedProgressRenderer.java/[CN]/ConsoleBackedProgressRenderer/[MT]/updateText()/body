{
  StringBuilder builder=new StringBuilder();
  for (  String operation : operations) {
    if (operation.length() == 0) {
      continue;
    }
    if (builder.length() > 0) {
      builder.append(' ');
    }
    builder.append("> ");
    builder.append(operation);
  }
  if (statusBar == null) {
    statusBar=console.getStatusBar();
  }
  statusBar.setText(builder.toString());
}
