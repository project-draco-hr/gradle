{
  String projectName=makeName(getFlavorComponent(nativeBinary),getVariantComponent((ProjectNativeBinaryInternal)nativeBinary),baseName((ProjectNativeBinaryInternal)nativeBinary),projectSuffix(nativeBinary));
  String configurationName=makeName(getPlatformComponent(nativeBinary),nativeBinary.getBuildType().getName());
  String platformName=getArchitectureName(nativeBinary.getTargetPlatform());
  return new ProjectConfigurationNames(projectName,configurationName,platformName);
}
