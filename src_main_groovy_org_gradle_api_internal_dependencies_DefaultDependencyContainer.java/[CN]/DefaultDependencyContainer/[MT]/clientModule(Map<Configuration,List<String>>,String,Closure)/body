{
  DefaultConfigurationContainer defaultConfigurationContainer=new DefaultConfigurationContainer();
  defaultConfigurationContainer.add(Dependency.DEFAULT_CONFIGURATION);
  ClientModule clientModule=new ClientModule(getStandardConfigurationMapping(configurationMappings),moduleDescriptor,new DefaultDependencyContainer(project,defaultConfigurationContainer,dependencyFactory,new DefaultExcludeRuleContainer(),clientModuleRegistry));
  dependencies.add(clientModule);
  ConfigureUtil.configure(configureClosure,clientModule);
  return clientModule;
}
