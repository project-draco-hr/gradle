{
  if (getDelegate() == null) {
    ProjectInternal project=gradle.getDefaultProject();
    defaultTasks=project.getDefaultTasks();
    if (defaultTasks.size() == 0) {
      throw new TaskSelectionException(String.format("No tasks have been specified and %s has not defined any default tasks.",project));
    }
    setDelegate(new TaskNameResolvingBuildExecuter(defaultTasks));
  }
  super.select(gradle);
}
