{
  List<Node> compilers=Lists.newArrayList();
  for (  Node toolChain : (List<Node>)getRootToolChains()) {
    List<Node> tools=getChildren(toolChain,"tool");
    for (    Node tool : tools) {
      if (isGnuCompilerTool(tool)) {
        compilers.add(tool);
      }
    }
  }
  return new NodeList(compilers);
}
