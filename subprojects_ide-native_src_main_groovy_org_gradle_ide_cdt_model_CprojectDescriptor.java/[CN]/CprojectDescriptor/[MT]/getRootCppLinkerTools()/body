{
  List<Node> linkers=Lists.newArrayList();
  for (  Node toolChain : (List<Node>)getRootToolChains()) {
    List<Node> tools=getChildren(toolChain,"tool");
    for (    Node tool : tools) {
      if (isGnuLinkerTool(tool)) {
        linkers.add(tool);
      }
    }
  }
  return new NodeList(linkers);
}
