{
  if (toolChains == null) {
    List<ToolChainCandidate> compilers=new ArrayList<ToolChainCandidate>();
    if (OperatingSystem.current().isWindows()) {
      compilers.addAll(findVisualCpps());
    }
 else {
      compilers.add(findGcc());
      compilers.add(findClang());
    }
    toolChains=compilers;
  }
  return toolChains;
}
