{
  DefaultNativePlatform targetPlatform=new DefaultNativePlatform("default",ArchitectureNotationParser.parser(),OperatingSystemNotationParser.parser());
  installDir=install.getVisualStudioDir();
  version=install.getVersion();
  pathEntries.addAll(install.getVisualCpp().getPath(targetPlatform));
  return this;
}
