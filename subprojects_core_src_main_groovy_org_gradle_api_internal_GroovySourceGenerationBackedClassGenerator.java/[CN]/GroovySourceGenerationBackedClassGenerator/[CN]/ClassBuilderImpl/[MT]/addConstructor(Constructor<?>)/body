{
  src.format("public %s(",className);
  for (int i=0; i < constructor.getParameterTypes().length; i++) {
    Class<?> paramType=constructor.getParameterTypes()[i];
    if (i > 0) {
      src.format(",");
    }
    src.format("%s p%d",paramType.getCanonicalName(),i);
  }
  src.format(") { super(");
  for (int i=0; i < constructor.getParameterTypes().length; i++) {
    if (i > 0) {
      src.format(",");
    }
    src.format("p%d",i);
  }
  src.format("); }%n");
}
