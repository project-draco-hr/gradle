{
  MetaMethod getter=property.getGetter();
  String returnTypeName=getter.getReturnType().getCanonicalName();
  src.format("private boolean %sSet;%n",property.getName());
  src.format("public %s %s() { getConventionMapping().getConventionValue(super.%s(), '%s', %sSet); }%n",returnTypeName,getter.getName(),getter.getName(),property.getName(),property.getName());
}
