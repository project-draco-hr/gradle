{
  ClassLoaderCache classLoaderCache=new ClassLoaderCache();
  return new ProviderConnection(serviceRegistry,loggingServices,daemonClientFactory,buildActionExecuter,new PayloadSerializer(new ClientSidePayloadClassLoaderRegistry(new DefaultPayloadClassLoaderRegistry(classLoaderCache,new ClientSidePayloadClassLoaderFactory(new ModelClassLoaderFactory())),new ClasspathInferer(),classLoaderCache)),jvmVersionDetector);
}
