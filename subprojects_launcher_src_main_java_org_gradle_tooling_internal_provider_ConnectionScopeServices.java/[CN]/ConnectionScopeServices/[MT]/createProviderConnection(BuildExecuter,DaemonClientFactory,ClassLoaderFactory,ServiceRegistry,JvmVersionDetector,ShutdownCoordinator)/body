{
  return new ProviderConnection(serviceRegistry,loggingServices,daemonClientFactory,buildActionExecuter,new PayloadSerializer(new ClientSidePayloadClassLoaderRegistry(new DefaultPayloadClassLoaderRegistry(new ClassLoaderCache(),new ClientSidePayloadClassLoaderFactory(new ModelClassLoaderFactory(classLoaderFactory))),new ClasspathInferer())),jvmVersionDetector);
}
