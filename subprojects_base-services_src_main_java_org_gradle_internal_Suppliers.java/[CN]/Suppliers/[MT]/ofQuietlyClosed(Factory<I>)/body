{
  return new Supplier<I>(){
    public <R>R supplyTo(    Transformer<R,? super I> transformer){
      I thing=factory.create();
      try {
        return transformer.transform(thing);
      }
  finally {
        try {
          thing.close();
        }
 catch (        Exception ignore) {
        }
      }
    }
  }
;
}
