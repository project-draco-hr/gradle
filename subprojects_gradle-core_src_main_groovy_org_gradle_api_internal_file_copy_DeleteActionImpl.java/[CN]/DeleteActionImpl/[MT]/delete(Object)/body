{
  boolean didWork=false;
  for (  File file : fileResolver.resolveFiles(deletes)) {
    if (!file.exists()) {
      continue;
    }
    logger.debug("Deleting {}",file);
    didWork=true;
    if (file.isFile()) {
      GFileUtils.deleteQuietly(file);
    }
 else {
      GFileUtils.deleteDirectory(file);
    }
  }
  return didWork;
}
