{
  try {
    Socket socket=new Socket(InetAddress.getByName(null),DAEMON_PORT);
    try {
      return new SocketConnection(socket);
    }
 catch (    IOException e) {
      socket.close();
      throw e;
    }
  }
 catch (  ConnectException e) {
    return null;
  }
catch (  IOException e) {
    throw UncheckedException.asUncheckedException(e);
  }
}
