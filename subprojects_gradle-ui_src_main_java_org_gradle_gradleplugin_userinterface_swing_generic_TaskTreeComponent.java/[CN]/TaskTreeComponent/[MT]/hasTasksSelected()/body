{
  TreePath[] treePaths=tree.getSelectionPaths();
  if (treePaths == null) {
    return false;
  }
  for (int index=0; index < treePaths.length; index++) {
    TreePath treePath=treePaths[index];
    Object o=treePath.getLastPathComponent();
    if (o instanceof TaskTreeNode) {
      return true;
    }
  }
  return false;
}
