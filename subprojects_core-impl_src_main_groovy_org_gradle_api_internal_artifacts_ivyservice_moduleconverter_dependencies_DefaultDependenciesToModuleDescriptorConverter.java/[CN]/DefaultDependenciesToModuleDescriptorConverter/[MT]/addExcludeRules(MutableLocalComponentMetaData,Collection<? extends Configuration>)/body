{
  for (  Configuration configuration : configurations) {
    for (    ExcludeRule excludeRule : configuration.getExcludeRules()) {
      org.apache.ivy.core.module.descriptor.ExcludeRule rule=excludeRuleConverter.createExcludeRule(configuration.getName(),excludeRule);
      metaData.addExcludeRule(rule);
    }
  }
}
