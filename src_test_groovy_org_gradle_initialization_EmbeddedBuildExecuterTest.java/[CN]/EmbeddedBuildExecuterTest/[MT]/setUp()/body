  expectedStartParameter.setProjectProperties(WrapUtil.toMap("projectProp1","projectProp1"));
  embeddedBuildExecuter=new EmbeddedBuildExecuter(buildFactoryMock);
  expectedBuildResolverDir=new File("buildResolverDir");
